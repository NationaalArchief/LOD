  "http://creativecommons.org/ns#license" : [ {
    "@language" : "nl",
    "@value" : "???"
  } ],
  "http://purl.org/dc/elements/1.1/abstract" : [ {
    "@language" : "nl",
    "@value" : "Ontologie van het Nationaal Archief. Deze ontologie beschrijft de termen die nodig zijn om de Nadere Toegangen en de Fotocollectie om te zetten naar Linked Open Data. De termen in deze ontologie worden (standaard) gebruikt voor al de Nadere Toegangen en Fotocollecties en zijn generiek toepasbaar voor datasets in het archiefdomein."
  } ],
  "http://purl.org/dc/elements/1.1/contributor" : [ {
    "@language" : "nl",
    "@value" : "Fairoesh Nanhekhan - Informatieanalist/Projectleider \nVincent Sleebe - Senior Medewerker Verwerven en Beheren Digitale Archieven\nEd de Heer - Adviseur/Ptojectleider Linked Open Data\nGijsbert Kruithof - Enterprise Architect\nElvin Dechesne - Consultant Ordina"
  } ],
  "http://purl.org/dc/elements/1.1/creator" : [ {
    "@language" : "nl",
    "@value" : "Nationaal Archief"
  } ],
  "http://purl.org/dc/elements/1.1/description" : [ {
    "@language" : "nl",
    "@value" : "Ontologie van het Nationaal Archief. Deze ontologie definieert de termen die nodig zijn voor het omzetten van datasets in het archiefdomein. De termen in deze ontologie worden (standaard) gebruikt voor de Nadere Toegangen en Fotocollecties en zijn generiek toepasbaar voor datasets in het archiefdomein. de termen zijn gebaseerd op de ore en edm ontologie.\n\nDeze ontologie bestaat naast het Nationaal Archief Vocabulaire. Het NA Vocabulaire betreft termen die specifiek zijn voor de fotocollectie en de Nadere Toegangen."
  } ],
  "http://purl.org/dc/elements/1.1/rights" : [ {
    "@language" : "nl",
    "@value" : "Copyright 2021 - Nationaal Archief"
  } ],
  "http://purl.org/dc/terms/publisher" : [ {
    "@language" : "nl",
    "@value" : "Nationaal Archief\nWillem Alexanderhof 20\n2595BE Den Haag"
  } ],
  "http://purl.org/dc/terms/title" : [ {
    "@language" : "nl",
    "@value" : "Nationaal Archief Ontologie"
  } ],
  "http://www.w3.org/2000/01/rdf-schema#comment" : [ {
    "@language" : "nl",
    "@value" : "Ontologie van het NA. HIerin staan de NA-termen die voor de Nadere Toegangen en Fotocollectie worden gebruikt. Deze termen zijn generiek toepasbaar voor een dataset in het archiefdomein."
  } ],
  "http://www.w3.org/2000/01/rdf-schema#label" : [ {
    "@language" : "nl",
    "@value" : "Nationaal Archief Ontologie"
  } ],
  "http://www.w3.org/2002/07/owl#versionInfo" : [ {
    "@language" : "nl",
    "@value" : "Versie 0.1 - 3 maart 2020"
  } ]
