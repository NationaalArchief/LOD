#################################################################
#    Object Properties
#################################################################

###  http://archief.nl/def/hasName
naa:hasName rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf dcterms:identifier ;
            rdfs:range bgn:Name ;
            rdfs:label "heeft naam"@nl ;
            skos:label "heeft naam"@nl .


###  http://archief.nl/def/hasProfession
naa:hasProfession rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf naa:hasRole ;
                  rdfs:domain edm:Agent ;
                  rdfs:range naa:Profession ;
                  rdfs:label "heeft beroep"@nl ;
                  skos:definition "denotes that an actor has a profession"@en ,
                                  "verbindt een actor met een beroep"@nl ;
                  skos:label "heeft beroep"@nl .


###  http://archief.nl/def/hasReligion
naa:hasReligion rdf:type owl:ObjectProperty ;
                owl:equivalentProperty gvp:aat2335_associated_with ;
                rdfs:domain naa:PersonEntity ;
                rdfs:range skos:Concept ;
                rdfs:label "has religion"@en ,
                           "heeft (geloofs-)overtuiging"@nl ;
                skos:prefLabel "heeft (geloofs-)overtuiging"@en .


###  http://archief.nl/def/hasResidency
naa:hasResidency rdf:type owl:ObjectProperty ;
                 owl:equivalentProperty cidoc-crm:P74_has_current_or_former_residence ;
                 owl:inverseOf naa:isResidencyOf ;
                 rdfs:domain naa:Agent ;
                 rdfs:range hg:PlaceInTime ;
                 rdfs:label "has residency (domicile)"@en ,
                            "heeft verblijfplaats"@nl ;
                 skos:prefLabel "heeft verblijfplaats"@nl .


###  http://archief.nl/def/hasRole
naa:hasRole rdf:type owl:ObjectProperty ;
            owl:equivalentProperty ebucore:hasRole ;
            rdfs:range naa:Role ;
            rdfs:label "heeft rol"@nl ;
            skos:definition "verbindt een entiteit met een rol"@nl ;
            skos:label "heeft rol"@nl ;
            skos:prefLabel "denotes that something has a role"@en ,
                           "heeft rol"@nl .


###  http://archief.nl/def/isAggregationType
naa:isAggregationType rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf dcterms:type ;
                      rdfs:domain ore:Aggregation ;
                      rdfs:range naa:AggregationType .


###  http://archief.nl/def/isEntityType
naa:isEntityType rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf dcterms:type ;
                 rdfs:domain naa:ArchivalEntity ;
                 rdfs:range naa:ArchivalEntityType ;
                 skos:label "is van het type"@nl .


###  http://archief.nl/def/isMaterialType
naa:isMaterialType rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf naa:isEntityType ;
                   rdfs:domain naa:ArchivalEntity ;
                   rdfs:range naa:ArchivalEntityType ;
                   skos:label "is van het type materiaal"@nl .


###  http://archief.nl/def/isResidencyOf
naa:isResidencyOf rdf:type owl:ObjectProperty ;
                  owl:equivalentProperty cidoc-crm:P74i_is_current_or_former_residence_of ;
                  rdfs:domain hg:PlaceInTime ;
                  rdfs:range bf:Agent ;
                  rdfs:label "is residency of"@en ,
                             "is verblijfplaats van"@nl ;
                  skos:prefLabel "is verblijfplaats van"@nl .


###  http://archief.nl/def/owned
naa:owned rdf:type owl:ObjectProperty ;
          rdfs:domain fibo-fnd-pty-pty:IndependentParty ;
          rdfs:label "bezat"@nl ;
          skos:definition "To have owned something."@en ;
          skos:note "Deze term is gemunt om overdracht van eigenaarschap te kunnen duiden die in het verleden heeft plaats gevonden."@nl .


###  http://archief.nl/def/sex
naa:sex rdf:type owl:ObjectProperty ;
        owl:equivalentProperty dbo:sex ,
                               schema:gender ;
        rdfs:domain naa:PersonEntity ;
        rdfs:range naa:GenderType ;
        rdfs:label "geslacht"@nl ;
        skos:prefLabel "geslacht"@nl .


###  http://archief.nl/def/wasOwnedBy
naa:wasOwnedBy rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf fibo-fnd-oac-own:isOwnedBy ;
               rdfs:label "is het bezit geweest van"@nl ,
                          "was owned by"@en ;
               skos:prefLabel "is het bezit geweest van"@nl .


###  http://dbpedia.org/ontology/sex
dbo:sex rdf:type owl:ObjectProperty .


###  http://purl.org/dc/terms/accessRights
dcterms:accessRights rdf:type owl:ObjectProperty ;
                     skos:note "MAIS openb.beperking"@nl .


###  http://purl.org/dc/terms/coverage
dcterms:coverage rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf dc:coverage .


###  http://purl.org/dc/terms/identifier
dcterms:identifier rdf:type owl:ObjectProperty .


###  http://purl.org/dc/terms/isPartOf
dcterms:isPartOf rdf:type owl:ObjectProperty .


###  http://purl.org/dc/terms/license
dcterms:license rdf:type owl:ObjectProperty ;
                skos:note "MAIS Licentie methode"@nl .


###  http://purl.org/dc/terms/publisher
dcterms:publisher rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf dc:publisher .


###  http://purl.org/dc/terms/relation
dcterms:relation rdf:type owl:ObjectProperty .


###  http://purl.org/dc/terms/rights
dcterms:rights rdf:type owl:ObjectProperty ;
               rdfs:range dcterms:RightsStatement ;
               skos:note "TODO: een gecontrolleerde vocabulaire (naa) zodat we naar geclassificeerde begrippen kunnen verwijzen met eigen URI's."@nl ;
               skos:scopeNote "MAIS Rechten"@nl .


###  http://purl.org/dc/terms/subject
dcterms:subject rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf dc:subject ;
                skos:note """MAIS Relaties (thesaurusterm). Bijvoorbeeld een verwijzing naar de GTAA thesaurus.
Gebruiken bij de ore:Aggregation indien het een externe verwijzing (URI) betreft. Een naa:ArchivalEntity mag geen ongecontrolleerde verwijzingen bevatten naar objecten buiten het domein van de RijksCollectie."""@nl .


###  http://purl.org/dc/terms/temporal
dcterms:temporal rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf dcterms:coverage .


###  http://purl.org/dc/terms/type
dcterms:type rdf:type owl:ObjectProperty .


###  http://rdfs.org/sioc/services#has_service
svcs:has_service rdf:type owl:ObjectProperty .


###  http://schema.org/areaServed
schema:areaServed rdf:type owl:ObjectProperty ;
                  rdfs:domain schema:Offer ;
                  rdfs:comment "The geographic area where a service or offered item is provided."@en ;
                  rdfs:label "area served"@en ,
                             "leverings gebied"@nl ;
                  skos:prefLabel "leverings gebied"@nl .


###  http://schema.org/availableAtOrFrom
schema:availableAtOrFrom rdf:type owl:ObjectProperty ;
                         rdfs:domain [ rdf:type owl:Class ;
                                       owl:unionOf ( naa:MultipleOffers
                                                     schema:Demand
                                                     schema:Offer
                                                   )
                                     ] ;
                         rdfs:comment "The place(s) from which the offer can be obtained (e.g. store locations)."@en ;
                         rdfs:label "available at or from"@en ,
                                    "beschikbaar op lokatie"@nl ;
                         skos:prefLabel "beschikbaar op lokatie"@nl .


###  http://schema.org/category
schema:category rdf:type owl:ObjectProperty ;
                rdfs:domain [ rdf:type owl:Class ;
                              owl:unionOf ( naa:MultipleOffers
                                            schema:Demand
                                            schema:Offer
                                            schema:Product
                                            ore:Aggregation
                                          )
                            ] ;
                rdfs:comment "Categorisatie is het cognitieve proces waarmee mensen een verdeling aanbrengen in objecten in de wereld of hun eigen cognitieve kennis. Deze verdeling leidt tot een categorie op basis van een concept. Een willekeurige verdeling van objecten uit de wereld wordt meestal een klasse genoemd."@nl ;
                rdfs:label "categorie"@nl ,
                           "category"@en ;
                rdfs:seeAlso <https://nl.wikipedia.org/wiki/Categorisatie> ;
                skos:prefLabel "categorie"@nl .


###  http://schema.org/gender
schema:gender rdf:type owl:ObjectProperty .


###  http://schema.org/inventoryLevel
schema:inventoryLevel rdf:type owl:ObjectProperty ;
                      rdfs:domain [ rdf:type owl:Class ;
                                    owl:unionOf ( naa:MultipleOffers
                                                  schema:SomeProducts
                                                )
                                  ] ;
                      rdfs:range schema:QuantitativeValue ;
                      rdfs:comment "The current approximate inventory level for the item or items."@en ;
                      rdfs:label "hoeveelheid"@nl ,
                                 "inventory level"@en .


###  http://schema.org/itemOffered
schema:itemOffered rdf:type owl:ObjectProperty ;
                   rdfs:domain schema:Offer ;
                   rdfs:comment "The item being offered."@en ;
                   rdfs:label "aangeboden object"@nl ,
                              "item offered"@en ;
                   skos:prefLabel "aangeboden object"@nl .


###  http://schema.org/makesOffer
schema:makesOffer rdf:type owl:ObjectProperty ;
                  rdfs:domain schema:Person ;
                  rdfs:range schema:Offer ;
                  rdfs:comment "A pointer to products or services offered by the organization or person."@en ;
                  rdfs:label "doet aanbod"@nl ,
                             "makes offer"@en ;
                  skos:prefLabel "doet aanbod"@nl .


###  http://schema.org/offeredBy
schema:offeredBy rdf:type owl:ObjectProperty ;
                 rdfs:domain schema:Offer ;
                 rdfs:range naa:Agent ;
                 rdfs:comment "A pointer to the organization or person making the offer."@en ;
                 rdfs:label "aangeboden door"@nl ,
                            "offered by"@en .


###  http://vocab.getty.edu/ontology#aat2335_associated_with
gvp:aat2335_associated_with rdf:type owl:ObjectProperty .


###  http://www.cidoc-crm.org/cidoc-crm/P74_has_current_or_former_residence
cidoc-crm:P74_has_current_or_former_residence rdf:type owl:ObjectProperty ;
                                              rdfs:comment "This property describes the current or former E53 Place of residence of an E39 Actor.  The residence may be either the Place where the Actor resides, or a legally registered address of any kind."@en ;
                                              rdfs:label "has current or former residence"@en .


###  http://www.cidoc-crm.org/cidoc-crm/P74i_is_current_or_former_residence_of
cidoc-crm:P74i_is_current_or_former_residence_of rdf:type owl:ObjectProperty ;
                                                 rdfs:label "is current or former residence of"@en .


###  http://www.cidoc-crm.org/rdfs/cidoc-crm#P129_is_about
<http://www.cidoc-crm.org/rdfs/cidoc-crm#P129_is_about> rdf:type owl:ObjectProperty .


###  http://www.ebu.ch/metadata/ontologies/ebucore/ebucore#hasRole
ebucore:hasRole rdf:type owl:ObjectProperty .


###  http://www.europeana.eu/schemas/edm/hasView
edm:hasView rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf ore:aggregates ;
            rdfs:domain ore:Aggregation ;
            rdfs:range edm:WebResource ;
            rdfs:label "Has View"@en ;
            skos:definition """This property relates a ORE aggregation about a CHO with a web resource
providing a view of that CHO. Examples of view are: a thumbnail, a textual
abstract and a table of contents. The ORE aggregation may be a Europeana
Aggregation, in which case the view is an object owned by Europeana (i.e., an instance of edm:EuropeanaObject) or an aggregation contributed by a content provider. In order to capture both these cases, the domain of edm:hasView is ore:Aggregation and its range is edm:WebResource"""@en .


###  http://www.europeana.eu/schemas/edm/isShownBy
edm:isShownBy rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf edm:hasView ;
              rdfs:range edm:WebResource .


###  http://www.openarchives.org/ore/terms/aggregates
ore:aggregates rdf:type owl:ObjectProperty ;
               owl:inverseOf ore:isAggregatedBy ;
               rdfs:domain ore:Aggregation ;
               rdfs:range ore:AggregatedResource .


###  http://www.openarchives.org/ore/terms/isAggregatedBy
ore:isAggregatedBy rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf dcterms:isPartOf ;
                   rdfs:range ore:Aggregation .


###  http://www.w3.org/ns/prov#actedOnBehalfOf
prov:actedOnBehalfOf rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf prov:wasInfluencedBy .


###  http://www.w3.org/ns/prov#hadMember
prov:hadMember rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf prov:wasInfluencedBy .


###  http://www.w3.org/ns/prov#used
prov:used rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf prov:wasInfluencedBy .


###  http://www.w3.org/ns/prov#wasAssociatedWith
prov:wasAssociatedWith rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf prov:wasInfluencedBy .


###  http://www.w3.org/ns/prov#wasInfluencedBy
prov:wasInfluencedBy rdf:type owl:ObjectProperty .


###  http://xmlns.com/foaf/0.1/isPrimaryTopicOf
foaf:isPrimaryTopicOf rdf:type owl:ObjectProperty .


###  http://xmlns.com/foaf/0.1/knows
foaf:knows rdf:type owl:ObjectProperty .


###  http://xmlns.com/foaf/0.1/made
foaf:made rdf:type owl:ObjectProperty .


###  http://xmlns.com/foaf/0.1/maker
foaf:maker rdf:type owl:ObjectProperty .


###  http://xmlns.com/foaf/0.1/primaryTopic
foaf:primaryTopic rdf:type owl:ObjectProperty .


###  https://spec.edmcouncil.org/fibo/ontology/FND/OwnershipAndControl/Ownership/isOwnedBy
fibo-fnd-oac-own:isOwnedBy rdf:type owl:ObjectProperty ;
                           rdfs:domain fibo-fnd-oac-own:Asset ;
                           rdfs:range fibo-fnd-oac-own:Owner ;
                           rdfs:isDefinedBy fibo-fnd-oac-own: ;
                           rdfs:label "is an asset of" ;
                           skos:definition "identifies the party that owns the asset" .


###  https://spec.edmcouncil.org/fibo/ontology/FND/OwnershipAndControl/Ownership/owns
fibo-fnd-oac-own:owns rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf naa:owned ;
                      rdfs:domain fibo-fnd-pty-pty:IndependentParty ;
                      rdfs:isDefinedBy fibo-fnd-oac-own: ;
                      rdfs:label "owns" ;
                      skos:definition "to have (something) as one's own, possess" .


#################################################################
#    Data properties
#################################################################

###  http://archief.nl/def/additionalDescription
naa:additionalDescription rdf:type owl:DatatypeProperty ;
                          rdfs:subPropertyOf dc:description ;
                          rdfs:label "extra toelichting"@nl ;
                          skos:note "MAIS Annotatie"@nl ;
                          skos:prefLabel "extra toelichting"@nl .


###  http://archief.nl/def/age
naa:age rdf:type owl:DatatypeProperty ;
        owl:equivalentProperty dbo:age ,
                               foaf:age ;
        rdfs:domain naa:Agent ;
        rdfs:range xsd:int ;
        rdfs:label "age in years"@en ,
                   "leeftijd in jaren"@nl ;
        rdfs:seeAlso dbo:age ,
                     foaf:age ;
        skos:prefLabel "leeftijd in jaren"@nl .


###  http://archief.nl/def/alias
naa:alias rdf:type owl:DatatypeProperty ;
          rdfs:domain [ rdf:type owl:Class ;
                        owl:unionOf ( naa:PersonEntity
                                      bgn:Name
                                    )
                      ] ;
          rdfs:range xsd:string ;
          rdfs:label "alias"@nl .


###  http://archief.nl/def/alternativeFirstName
naa:alternativeFirstName rdf:type owl:DatatypeProperty ;
                         rdfs:subPropertyOf naa:alternativeNamePosition ,
                                            naa:prepositionName ;
                         rdfs:domain [ rdf:type owl:Class ;
                                       owl:unionOf ( naa:PersonEntity
                                                     bgn:Name
                                                   )
                                     ] ;
                         rdfs:range xsd:string ;
                         rdfs:label "alternative first name"@en ,
                                    "ook geheten"@nl ;
                         skos:prefLabel "ook geheten"@nl .


###  http://archief.nl/def/alternativeFullname
naa:alternativeFullname rdf:type owl:DatatypeProperty ;
                        rdfs:subPropertyOf naa:alternativeNamePosition ,
                                           naa:fullname ;
                        rdfs:domain [ rdf:type owl:Class ;
                                      owl:unionOf ( naa:PersonEntity
                                                    bgn:Name
                                                  )
                                    ] ;
                        rdfs:range xsd:string ;
                        rdfs:label "alternative full name"@en ,
                                   "ook vermeld als"@nl ;
                        skos:prefLabel "ook vermeld als"@nl .


###  http://archief.nl/def/alternativeIntraposition
naa:alternativeIntraposition rdf:type owl:DatatypeProperty ;
                             rdfs:subPropertyOf naa:alternativeNamePosition ,
                                                naa:intraposition ;
                             rdfs:domain [ rdf:type owl:Class ;
                                           owl:unionOf ( naa:PersonEntity
                                                         bgn:Name
                                                       )
                                         ] ;
                             rdfs:range xsd:string ;
                             rdfs:comment "Komt voor in gevallen als wanneer iemand met het Franse equivalent van 'van' wordt aangeduid"@nl ;
                             rdfs:label "alternative intraposition"@en ,
                                        "ook vermeld als"@nl ;
                             skos:prefLabel "ook vermeld als"@nl .


###  http://archief.nl/def/alternativeIntrapositionName
naa:alternativeIntrapositionName rdf:type owl:DatatypeProperty ;
                                 rdfs:subPropertyOf naa:alternativeIntraposition ;
                                 rdfs:domain [ rdf:type owl:Class ;
                                               owl:unionOf ( naa:PersonEntity
                                                             bgn:Name
                                                           )
                                             ] ;
                                 rdfs:range xsd:string ;
                                 rdfs:label "alternative name in intraposition"@en ,
