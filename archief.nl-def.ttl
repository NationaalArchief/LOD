#    Object Properties
#################################################################

###  http://archief.nl/def/beginVessel
:beginVessel rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf :passengerOnVessel ;
             rdfs:domain [ rdf:type owl:Class ;
                           owl:unionOf ( :ArchivalReference
                                         pnv:Person
                                       )
                         ] ;
             rdfs:range schema:Vehicle ;
             rdfs:comment "Deze term is gemunt voor het duiden van informatie uit NT00444 VOC Opvarenden waarbij er enkel informatie is over welk schip het begin van een reis gemaakt heeft."@nl ;
             rdfs:label "schip bij vertrek"@nl ,
                        "ship at beginning of voyage"@en ;
             skos:prefLabel "schip bij vertrek"@nl .


###  http://archief.nl/def/beginVoyage
:beginVoyage rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf :passengerOnVoyage ;
             rdfs:domain pnv:Person ;
             rdfs:range :Voyage ;
             rdfs:label "beginning of voyage"@en ,
                        "beginreis"@nl ;
             skos:prefLabel "beginreis"@nl .


###  http://archief.nl/def/beginVoyageFromCape
:beginVoyageFromCape rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf :beginVoyage ;
                     rdfs:domain pnv:Person ;
                     rdfs:range :Voyage ;
                     rdfs:label "beginning of voyage starting from Cape"@en ,
                                "beginreis vanaf de Kaap"@nl ;
                     skos:prefLabel "beginreis vanaf de Kaap"@nl .


###  http://archief.nl/def/benefactor
:benefactor rdf:type owl:ObjectProperty ;
            owl:inverseOf :beneficiary ;
            rdfs:range pnv:Person ;
            rdfs:label "benefactor"@en ,
                       "weldoener"@nl ;
            skos:prefLabel "weldoener"@nl .


###  http://archief.nl/def/beneficiary
:beneficiary rdf:type owl:ObjectProperty ;
             rdfs:range pnv:Person ;
             rdfs:label "begunstigde"@nl ,
                        "beneficiary"@en ;
             skos:prefLabel "begunstigde"@nl .


###  http://archief.nl/def/coverageEstimated
:coverageEstimated rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf dct:coverage ;
                   rdfs:label "datering geschat"@nl ,
                              "estimated coverage"@en ;
                   skos:prefLabel "datering geschat"@nl .


###  http://archief.nl/def/hasConfig
:hasConfig rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf ore:aggregates .


###  http://archief.nl/def/hasDestination
:hasDestination rdf:type owl:ObjectProperty ;
                rdfs:domain :Voyage ;
                rdfs:range hg:PlaceInTime ;
                rdfs:label "has destination"@en ,
                           "heeft bestemming"@nl ;
                skos:prefLabel "heeft bestemming"@nl .


###  http://archief.nl/def/hasEditorial
:hasEditorial rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf ore:aggregates .


###  http://archief.nl/def/hasProfession
:hasProfession rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf :hasRole ;
               rdfs:domain edm:Agent ;
               rdfs:range :Profession ;
               rdfs:label "heeft beroep"@nl ;
               skos:definition "denotes that an actor has a profession"@en ,
                               "verbindt een actor met een beroep"@nl ;
               skos:label "heeft beroep"@nl .


###  http://archief.nl/def/hasReligion
:hasReligion rdf:type owl:ObjectProperty ;
             owl:equivalentProperty gvp:aat2335_associated_with ;
             rdfs:domain pnv:Person ;
             rdfs:range skos:Concept ;
             rdfs:label "has religion"@en ,
                        "heeft (geloofs-)overtuiging"@nl ;
             skos:prefLabel "heeft (geloofs-)overtuiging"@en .


###  http://archief.nl/def/hasResidency
:hasResidency rdf:type owl:ObjectProperty ;
              owl:equivalentProperty dbo:residence ,
                                     cidoc-crm:P74_has_current_or_former_residence ;
              owl:inverseOf :isResidencyOf ;
              rdfs:domain :Agent ;
              rdfs:range hg:PlaceInTime ;
              rdfs:label "has residency (domicile)"@en ,
                         "heeft verblijfplaats"@nl ;
              skos:prefLabel "heeft verblijfplaats"@nl .


###  http://archief.nl/def/hasRole
:hasRole rdf:type owl:ObjectProperty ;
         owl:equivalentProperty ebucore:hasRole ;
         rdfs:range :Role ;
         rdfs:label "heeft rol"@nl ;
         skos:definition "verbindt een entiteit met een rol"@nl ;
         skos:label "heeft rol"@nl ;
         skos:prefLabel "denotes that something has a role"@en ,
                        "heeft rol"@nl .


###  http://archief.nl/def/isAggregationType
:isAggregationType rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf dct:type ;
                   rdfs:domain ore:Aggregation ;
                   rdfs:range :AggregationType .


###  http://archief.nl/def/isEntityType
:isEntityType rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf dct:type ;
              rdfs:domain :ArchivalEntity ;
              rdfs:range :ArchivalEntityType ;
              skos:label "is van het type"@nl .


###  http://archief.nl/def/isMaterialType
:isMaterialType rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf :isEntityType ;
                rdfs:domain :ArchivalEntity ;
                rdfs:range :ArchivalEntityType ;
                skos:label "is van het type materiaal"@nl .


###  http://archief.nl/def/isResidencyOf
:isResidencyOf rdf:type owl:ObjectProperty ;
               owl:equivalentProperty cidoc-crm:P74i_is_current_or_former_residence_of ;
               rdfs:domain hg:PlaceInTime ;
               rdfs:range bf:Agent ;
               rdfs:label "is residency of"@en ,
                          "is verblijfplaats van"@nl ;
               skos:prefLabel "is verblijfplaats van"@nl .


###  http://archief.nl/def/itenerary
:itenerary rdf:type owl:ObjectProperty ;
           owl:equivalentProperty schema:itinerary ;
           rdfs:domain :Voyage ;
           rdfs:range [ rdf:type owl:Class ;
                        owl:unionOf ( hg:PlaceInTime
                                      edm:TimeSpan
                                    )
                      ] ;
           rdfs:comment "Destination(s) ( PlaceInTime ) that make up a trip. For a trip where destination order is important use edm:TimeSpan to specify that order using edm:Event."@en ;
           rdfs:label "reisschema"@nl ;
           skos:prefLabel "reisschema"@nl .


###  http://archief.nl/def/meansOfTransport
:meansOfTransport rdf:type owl:ObjectProperty ;
                  rdfs:domain :Voyage ;
                  rdfs:range [ rdf:type owl:Class ;
                               owl:unionOf ( schema:Vehicle
                                             skos:Concept
                                           )
                             ] ;
                  rdfs:label "means of transport"@en ,
                             "transport middel"@nl ;
                  skos:prefLabel "transport middel"@nl .


###  http://archief.nl/def/occupationHistory
:occupationHistory rdf:type owl:ObjectProperty ;
                   rdfs:range edm:TimeSpan ;
                   rdfs:comment "Deze term mag gebruikt worden bij een persoon entiteit of als onderdeel van een beschrijving. De beschrijving moet dan wel over die persoon gaan."@nl ;
                   rdfs:label "a period describing a person's occupation"@en ,
                              "periode waarin iemand een beroep heeft uitgeoefend"@nl ;
                   skos:prefLabel "periode waarin iemand een beroep heeft uitgeoefend"@nl .


###  http://archief.nl/def/owned
:owned rdf:type owl:ObjectProperty ;
       rdfs:domain fibo-fnd-pty-pty:IndependentParty ;
       rdfs:label "bezat"@nl ;
       skos:definition "To have owned something."@en ;
       skos:note "Deze term is gemunt om overdracht van eigenaarschap te kunnen duiden die in het verleden heeft plaats gevonden."@nl .


###  http://archief.nl/def/passengerOnVessel
:passengerOnVessel rdf:type owl:ObjectProperty ;
                   rdfs:domain [ rdf:type owl:Class ;
                                 owl:unionOf ( :ArchivalReference
                                               pnv:Person
                                             )
                               ] ;
                   rdfs:range schema:Vehicle ;
                   rdfs:label "passagier op schip"@nl ,
                              "passenger on ship"@nl ;
                   skos:prefLabel "passagier op schip"@nl .


###  http://archief.nl/def/passengerOnVoyage
:passengerOnVoyage rdf:type owl:ObjectProperty ;
                   owl:inverseOf :voyageHasPassenger ;
                   rdfs:domain pnv:Person ;
                   rdfs:range :Voyage ;
                   rdfs:label "passagier op/van (reis)"@nl ,
                              "passenger on/of (voyage)"@en ;
                   skos:prefLabel "passagier op/van (reis)"@nl .


###  http://archief.nl/def/returnVoyage
:returnVoyage rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf :passengerOnVoyage ;
              rdfs:domain pnv:Person ;
              rdfs:range :Voyage ;
              rdfs:label "return voyage"@en ,
                         "terugreis"@nl ;
              skos:prefLabel "terugreis"@nl .


###  http://archief.nl/def/returnVoyageFromCape
:returnVoyageFromCape rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf :returnVoyage ;
                      rdfs:domain pnv:Person ;
                      rdfs:range :Voyage ;
                      rdfs:label "return voyage from Cape"@en ,
                                 "terugreis vanaf de Kaap"@nl ;
                      skos:prefLabel "terugreis vanaf de Kaap"@nl .


###  http://archief.nl/def/sex
:sex rdf:type owl:ObjectProperty ;
     owl:equivalentProperty dbo:sex ,
                            schema:gender ;
     rdfs:domain pnv:Person ;
     rdfs:range :GenderType ;
     rdfs:label "geslacht"@nl ;
     skos:prefLabel "geslacht"@nl .


###  http://archief.nl/def/vesselOfVoyage
:vesselOfVoyage rdf:type owl:ObjectProperty ;
                owl:inverseOf :voyageHasVessel ;
                rdfs:domain schema:Vehicle ;
                rdfs:range :Voyage ;
                rdfs:label "schip van reis"@nl ,
                           "ship of voyage"@en ;
                skos:prefLabel "schip van reis"@nl .


###  http://archief.nl/def/voyageHasPassenger
:voyageHasPassenger rdf:type owl:ObjectProperty ;
                    rdfs:domain :Voyage ;
                    rdfs:range pnv:Person ;
                    rdfs:label "reis heeft passagier"@nl ,
                               "voyage has passenger"@en ;
                    skos:prefLabel "reis heeft passagier"@nl .


###  http://archief.nl/def/voyageHasVessel
:voyageHasVessel rdf:type owl:ObjectProperty ;
                 rdfs:domain :Voyage ;
                 rdfs:range schema:Vehicle ;
                 rdfs:label "reis heeft schip"@nl ,
                            "voyage has vessel"@en ;
                 skos:prefLabel "reis heeft schip"@nl .


###  http://archief.nl/def/wasOwnedBy
:wasOwnedBy rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf fibo-fnd-oac-own:isOwnedBy ;
            rdfs:label "is het bezit geweest van"@nl ,
                       "was owned by"@en ;
            skos:prefLabel "is het bezit geweest van"@nl .


