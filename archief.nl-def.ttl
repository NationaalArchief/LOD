#################################################################
#    Object Properties
#################################################################

###  http://archief.nl/def/beginVessel
naa:beginVessel rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf naa:passengerOnVessel ;
                rdfs:domain [ rdf:type owl:Class ;
                              owl:unionOf ( naa:ArchivalReference
                                            pnv:Person
                                          )
                            ] ;
                rdfs:range schema:Vehicle ;
                rdfs:comment "Deze term is gemunt voor het duiden van informatie uit NT00444 VOC Opvarenden waarbij er enkel informatie is over welk schip het begin van een reis gemaakt heeft."@nl ;
                rdfs:label "schip bij vertrek"@nl ,
                           "ship at beginning of voyage"@en ;
                skos:prefLabel "schip bij vertrek"@nl .


###  http://archief.nl/def/beginVoyage
naa:beginVoyage rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf naa:passengerOnVoyage ;
                rdfs:domain pnv:Person ;
                rdfs:range naa:Voyage ;
                rdfs:label "beginning of voyage"@en ,
                           "beginreis"@nl ;
                skos:prefLabel "beginreis"@nl .


###  http://archief.nl/def/beginVoyageFromCape
naa:beginVoyageFromCape rdf:type owl:ObjectProperty ;
                        rdfs:subPropertyOf naa:beginVoyage ;
                        rdfs:domain pnv:Person ;
                        rdfs:range naa:Voyage ;
                        rdfs:label "beginning of voyage starting from Cape"@en ,
                                   "beginreis vanaf de Kaap"@nl ;
                        skos:prefLabel "beginreis vanaf de Kaap"@nl .


###  http://archief.nl/def/benefactor
naa:benefactor rdf:type owl:ObjectProperty ;
               owl:inverseOf naa:beneficiary ;
               rdfs:range pnv:Person ;
               rdfs:label "benefactor"@en ,
                          "weldoener"@nl ;
               skos:prefLabel "weldoener"@nl .


###  http://archief.nl/def/beneficiary
naa:beneficiary rdf:type owl:ObjectProperty ;
                rdfs:range pnv:Person ;
                rdfs:label "begunstigde"@nl ,
                           "beneficiary"@en ;
                skos:prefLabel "begunstigde"@nl .


###  http://archief.nl/def/convictedOf
naa:convictedOf rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf dc:relation ;
                rdfs:domain pnv:Person ;
                rdfs:range naa:Felony ;
                rdfs:label "convited of"@en ,
                           "veroordeeld voor"@nl ;
                skos:prefLabel "veroordeeld voor"@nl .


###  http://archief.nl/def/countryOfOrigin
naa:countryOfOrigin rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf dc:relation ;
                    rdfs:domain pnv:Person ;
                    rdfs:range hg:PlaceInTime ;
                    rdfs:label "country of origin"@en ,
                               "land van herkomst"@nl ;
                    skos:prefLabel "land van herkomst"@nl .


###  http://archief.nl/def/coverageEstimated
naa:coverageEstimated rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf dct:coverage ;
                      rdfs:label "datering geschat"@nl ,
                                 "estimated coverage"@en ;
                      skos:prefLabel "datering geschat"@nl .


###  http://archief.nl/def/hasCargo
naa:hasCargo rdf:type owl:ObjectProperty ;
             rdfs:domain naa:Voyage ;
             rdfs:range skos:Concept ;
             rdfs:label "has cargo"@en ,
                        "heeft lading"@nl ;
             skos:prefLabel "lading"@nl .


###  http://archief.nl/def/hasConfig
naa:hasConfig rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf ore:aggregates .


###  http://archief.nl/def/hasDestination
naa:hasDestination rdf:type owl:ObjectProperty ;
                   owl:equivalentProperty dbo:destination ;
                   rdfs:domain naa:Voyage ;
                   rdfs:range hg:PlaceInTime ;
                   rdfs:label "has destination"@en ,
                              "heeft bestemming"@nl ;
                   skos:prefLabel "heeft bestemming"@nl .


###  http://archief.nl/def/hasEditorial
naa:hasEditorial rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf ore:aggregates .


###  http://archief.nl/def/hasProfession
naa:hasProfession rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf naa:hasRole ;
                  rdfs:domain edm:Agent ;
                  rdfs:range naa:Profession ;
                  rdfs:label "heeft beroep"@nl ;
                  skos:definition "denotes that an actor has a profession"@en ,
                                  "verbindt een actor met een beroep"@nl ;
                  skos:label "heeft beroep"@nl .


###  http://archief.nl/def/hasReligion
naa:hasReligion rdf:type owl:ObjectProperty ;
                owl:equivalentProperty gvp:aat2335_associated_with ;
                rdfs:domain pnv:Person ;
                rdfs:range skos:Concept ;
                rdfs:label "has religion"@en ,
                           "heeft (geloofs-)overtuiging"@nl ;
                skos:prefLabel "heeft (geloofs-)overtuiging"@en .


###  http://archief.nl/def/hasResidency
naa:hasResidency rdf:type owl:ObjectProperty ;
                 owl:equivalentProperty dbo:residence ,
                                        cidoc-crm:P74_has_current_or_former_residence ;
                 owl:inverseOf naa:isResidencyOf ;
                 rdfs:domain naa:Agent ;
                 rdfs:range hg:PlaceInTime ;
                 rdfs:label "has residency (domicile)"@en ,
                            "heeft verblijfplaats"@nl ;
                 skos:prefLabel "heeft verblijfplaats"@nl .


###  http://archief.nl/def/hasRole
naa:hasRole rdf:type owl:ObjectProperty ;
            owl:equivalentProperty ebucore:hasRole ;
            rdfs:range naa:Role ;
            rdfs:label "heeft rol"@nl ;
            skos:definition "verbindt een entiteit met een rol"@nl ;
            skos:label "heeft rol"@nl ;
            skos:prefLabel "denotes that something has a role"@en ,
                           "heeft rol"@nl .


###  http://archief.nl/def/isAggregationType
naa:isAggregationType rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf dct:type ;
                      rdfs:domain ore:Aggregation ;
                      rdfs:range naa:AggregationType .


###  http://archief.nl/def/isEntityType
naa:isEntityType rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf dct:type ;
                 rdfs:domain naa:ArchivalEntity ;
                 rdfs:range naa:ArchivalEntityType ;
                 skos:label "is van het type"@nl .


###  http://archief.nl/def/isMaterialType
naa:isMaterialType rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf naa:isEntityType ;
                   rdfs:domain naa:ArchivalEntity ;
                   rdfs:range naa:ArchivalEntityType ;
                   skos:label "is van het type materiaal"@nl .


###  http://archief.nl/def/isResidencyOf
naa:isResidencyOf rdf:type owl:ObjectProperty ;
                  owl:equivalentProperty cidoc-crm:P74i_is_current_or_former_residence_of ;
                  rdfs:domain hg:PlaceInTime ;
                  rdfs:range bf:Agent ;
                  rdfs:label "is residency of"@en ,
                             "is verblijfplaats van"@nl ;
                  skos:prefLabel "is verblijfplaats van"@nl .


###  http://archief.nl/def/itenerary
naa:itenerary rdf:type owl:ObjectProperty ;
              owl:equivalentProperty schema:itinerary ;
              rdfs:domain naa:Voyage ;
              rdfs:range [ rdf:type owl:Class ;
                           owl:unionOf ( hg:PlaceInTime
                                         edm:TimeSpan
                                       )
                         ] ;
              rdfs:comment "Destination(s) ( PlaceInTime ) that make up a trip. For a trip where destination order is important use edm:TimeSpan to specify that order using edm:Event."@en ;
              rdfs:label "reisschema"@nl ;
              skos:prefLabel "reisschema"@nl .


###  http://archief.nl/def/meansOfTransport
naa:meansOfTransport rdf:type owl:ObjectProperty ;
                     rdfs:domain naa:Voyage ;
                     rdfs:range [ rdf:type owl:Class ;
                                  owl:unionOf ( schema:Vehicle
                                                skos:Concept
                                              )
                                ] ;
                     rdfs:label "means of transport"@en ,
                                "transport middel"@nl ;
                     skos:prefLabel "transport middel"@nl .


###  http://archief.nl/def/occupationHistory
naa:occupationHistory rdf:type owl:ObjectProperty ;
                      rdfs:range edm:TimeSpan ;
                      rdfs:comment "Deze term mag gebruikt worden bij een persoon entiteit of als onderdeel van een beschrijving. De beschrijving moet dan wel over die persoon gaan."@nl ;
                      rdfs:label "a period describing a person's occupation"@en ,
                                 "periode waarin iemand een beroep heeft uitgeoefend"@nl ;
                      skos:prefLabel "periode waarin iemand een beroep heeft uitgeoefend"@nl .


###  http://archief.nl/def/owned
naa:owned rdf:type owl:ObjectProperty ;
          rdfs:domain fibo-fnd-pty-pty:IndependentParty ;
          rdfs:label "bezat"@nl ;
          skos:definition "To have owned something."@en ;
          skos:note "Deze term is gemunt om overdracht van eigenaarschap te kunnen duiden die in het verleden heeft plaats gevonden."@nl .


###  http://archief.nl/def/passengerOnVessel
naa:passengerOnVessel rdf:type owl:ObjectProperty ;
                      rdfs:domain [ rdf:type owl:Class ;
                                    owl:unionOf ( naa:ArchivalReference
                                                  pnv:Person
                                                )
                                  ] ;
                      rdfs:range schema:Vehicle ;
                      rdfs:label "passagier op schip"@nl ,
                                 "passenger on ship"@nl ;
                      skos:prefLabel "passagier op schip"@nl .


###  http://archief.nl/def/passengerOnVoyage
naa:passengerOnVoyage rdf:type owl:ObjectProperty ;
                      owl:inverseOf naa:voyageHasPassenger ;
                      rdfs:domain pnv:Person ;
                      rdfs:range naa:Voyage ;
                      rdfs:label "passagier op/van (reis)"@nl ,
                                 "passenger on/of (voyage)"@en ;
                      skos:prefLabel "passagier op/van (reis)"@nl .


###  http://archief.nl/def/placeOfArrival
naa:placeOfArrival rdf:type owl:ObjectProperty ,
                            owl:FunctionalProperty ;
                   rdfs:domain naa:Voyage ;
                   rdfs:range hg:PlaceInTime ;
                   rdfs:label "plaats van aankomst"@nl ,
                              "place of arrival"@nl ;
                   skos:prefLabel "plaats van aankomst"@nl .


###  http://archief.nl/def/placeOfDeparture
naa:placeOfDeparture rdf:type owl:ObjectProperty ,
                              owl:FunctionalProperty ;
                     rdfs:domain naa:Voyage ;
                     rdfs:range hg:PlaceInTime ;
                     rdfs:label "place of departure"@en ,
                                "vertrekplaats"@nl ;
                     skos:prefLabel "vertrekplaats"@nl .


###  http://archief.nl/def/returnVoyage
naa:returnVoyage rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf naa:passengerOnVoyage ;
                 rdfs:domain pnv:Person ;
                 rdfs:range naa:Voyage ;
                 rdfs:label "return voyage"@en ,
                            "terugreis"@nl ;
                 skos:prefLabel "terugreis"@nl .


###  http://archief.nl/def/returnVoyageFromCape
naa:returnVoyageFromCape rdf:type owl:ObjectProperty ;
                         rdfs:subPropertyOf naa:returnVoyage ;
                         rdfs:domain pnv:Person ;
                         rdfs:range naa:Voyage ;
                         rdfs:label "return voyage from Cape"@en ,
                                    "terugreis vanaf de Kaap"@nl ;
                         skos:prefLabel "terugreis vanaf de Kaap"@nl .


###  http://archief.nl/def/sentenceReceived
naa:sentenceReceived rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf dc:relation ;
                     rdfs:domain pnv:Person ;
                     rdfs:range naa:Sentence ;
                     rdfs:label "sentence received"@en ,
                                "vonnis ontvangen"@nl ;
                     skos:prefLabel "vonnis ontvangen"@nl .


###  http://archief.nl/def/sex
naa:sex rdf:type owl:ObjectProperty ;
        owl:equivalentProperty dbo:sex ,
                               schema:gender ;
        rdfs:domain pnv:Person ;
        rdfs:range naa:GenderType ;
        rdfs:label "geslacht"@nl ;
        skos:prefLabel "geslacht"@nl .


###  http://archief.nl/def/transferTo
naa:transferTo rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf edm:hasMet ;
               rdfs:label "overschrijving aan"@nl ,
                          "transfer to"@en ;
               skos:prefLabel "overschrijving aan"@nl .


###  http://archief.nl/def/vesselOfVoyage
naa:vesselOfVoyage rdf:type owl:ObjectProperty ;
                   owl:inverseOf naa:voyageHasVessel ;
                   rdfs:domain schema:Vehicle ;
                   rdfs:range naa:Voyage ;
                   rdfs:label "schip van reis"@nl ,
                              "ship of voyage"@en ;
                   skos:prefLabel "schip van reis"@nl .


###  http://archief.nl/def/voyageHasPassenger
naa:voyageHasPassenger rdf:type owl:ObjectProperty ;
                       rdfs:domain naa:Voyage ;
                       rdfs:range pnv:Person ;
                       rdfs:label "reis heeft passagier"@nl ,
                                  "voyage has passenger"@en ;
                       skos:prefLabel "reis heeft passagier"@nl .


###  http://archief.nl/def/voyageHasVessel
naa:voyageHasVessel rdf:type owl:ObjectProperty ;
                    rdfs:domain naa:Voyage ;
                    rdfs:range schema:Vehicle ;
                    rdfs:label "reis heeft schip"@nl ,
                               "voyage has vessel"@en ;
                    skos:prefLabel "reis heeft schip"@nl .


###  http://archief.nl/def/wasOwnedBy
naa:wasOwnedBy rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf fibo-fnd-oac-own:isOwnedBy ;
               rdfs:label "is het bezit geweest van"@nl ,
                          "was owned by"@en ;
               skos:prefLabel "is het bezit geweest van"@nl .


###  http://dbpedia.org/ontology/destination
dbo:destination rdf:type owl:ObjectProperty .


###  http://dbpedia.org/ontology/residence
dbo:residence rdf:type owl:ObjectProperty .


###  http://dbpedia.org/ontology/sex
dbo:sex rdf:type owl:ObjectProperty .


###  http://purl.org/dc/elements/1.1/relation
dc:relation rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf edm:isRelatedTo .


###  http://purl.org/dc/elements/1.1/subject
dc:subject rdf:type owl:ObjectProperty ;
           rdfs:subPropertyOf edm:isRelatedTo .


###  http://purl.org/dc/terms/accessRights
dct:accessRights rdf:type owl:ObjectProperty ;
                 skos:note "MAIS openb.beperking"@nl .


###  http://purl.org/dc/terms/coverage
dct:coverage rdf:type owl:ObjectProperty .


###  http://purl.org/dc/terms/identifier
dct:identifier rdf:type owl:ObjectProperty .


###  http://purl.org/dc/terms/isPartOf
dct:isPartOf rdf:type owl:ObjectProperty .


###  http://purl.org/dc/terms/license
dct:license rdf:type owl:ObjectProperty ;
            skos:note "MAIS Licentie methode"@nl .


###  http://purl.org/dc/terms/relation
dct:relation rdf:type owl:ObjectProperty .


###  http://purl.org/dc/terms/rights
dct:rights rdf:type owl:ObjectProperty ;
           rdfs:range dct:RightsStatement ;
           skos:note "TODO: een gecontrolleerde vocabulaire (naa) zodat we naar geclassificeerde begrippen kunnen verwijzen met eigen URI's."@nl ;
           skos:scopeNote "MAIS Rechten"@nl .


###  http://purl.org/dc/terms/type
dct:type rdf:type owl:ObjectProperty .


###  http://rdf.histograph.io/contains
hg:contains rdf:type owl:ObjectProperty .


###  http://rdfs.org/sioc/services#has_service
svcs:has_service rdf:type owl:ObjectProperty .


###  http://schema.org/acquiredFrom
schema:acquiredFrom rdf:type owl:ObjectProperty ;
                    rdfs:domain schema:OwnershipInfo ;
                    rdfs:range [ rdf:type owl:Class ;
                                 owl:unionOf ( schema:Organization
                                               schema:Person
                                             )
                               ] ;
                    rdfs:label "acquired from"@en ,
                               "afkomstig van"@nl ;
                    rdfs:seeAlso <https://schema.org/acquiredFrom> ;
                    skos:prefLabel "afkomstig van"@nl .


###  http://schema.org/areaServed
schema:areaServed rdf:type owl:ObjectProperty ;
                  rdfs:domain schema:Offer ;
                  rdfs:comment "The geographic area where a service or offered item is provided."@en ;
                  rdfs:label "area served"@en ,
                             "leverings gebied"@nl ;
                  skos:prefLabel "leverings gebied"@nl .


###  http://schema.org/availableAtOrFrom
schema:availableAtOrFrom rdf:type owl:ObjectProperty ;
                         rdfs:domain [ rdf:type owl:Class ;
                                       owl:unionOf ( naa:MultipleOffers
                                                     schema:Demand
                                                     schema:Offer
                                                   )
                                     ] ;
                         rdfs:comment "The place(s) from which the offer can be obtained (e.g. store locations)."@en ;
                         rdfs:label "available at or from"@en ,
                                    "beschikbaar op lokatie"@nl ;
                         skos:prefLabel "beschikbaar op lokatie"@nl .


###  http://schema.org/birthPlace
schema:birthPlace rdf:type owl:ObjectProperty ;
                  rdfs:domain premis:Person ;
                  rdfs:range schema:Place ;
                  rdfs:label "birth place"@en ,
                             "geboorteplaats"@nl ;
                  rdfs:seeAlso <https://schema.org/birthPlace> ;
                  skos:prefLabel "geboorteplaats"@nl .


###  http://schema.org/category
