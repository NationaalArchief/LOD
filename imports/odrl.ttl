@prefix : <http://www.w3.org/ns/odrl/2/> .
@prefix cc: <http://creativecommons.org/ns#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix vcard: <http://www.w3.org/2006/vcard/ns#> .
@prefix schema: <http://schema.org/> .
@base <http://www.w3.org/ns/odrl/2/> .

<http://www.w3.org/ns/odrl/2/> rdf:type owl:Ontology ;
                                dct:creator "Renato Iannella" ;
                                dct:contributor "W3C Permissions & Obligations Expression Working Group" ;
                                rdfs:label "ODRL Version 2.2"@en ;
                                owl:versionInfo "2.2" ;
                                dct:creator "Stuart Myles" ;
                                dct:description "The ODRL Vocabulary and Expression defines a set of concepts and terms (the vocabulary) and encoding mechanism (the expression) for permissions and obligations statements describing digital content usage based on the ODRL Information Model."@en ;
                                dct:license <https://www.w3.org/Consortium/Legal/2002/ipr-notice-20021231#Copyright/> ;
                                rdfs:comment "This is the RDF ontology for ODRL Version 2.2."@en ;
                                dct:creator "Víctor Rodríguez-Doncel" ,
                                            "Michael Steidl" .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/terms/contributor
dct:contributor rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/creator
dct:creator rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/description
dct:description rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/format
dct:format rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/isVersionOf
dct:isVersionOf rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/issued
dct:issued rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/license
dct:license rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/subject
dct:subject rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#broader
skos:broader rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#broaderTransitive
skos:broaderTransitive rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#definition
skos:definition rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#exactMatch
skos:exactMatch rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#hasTopConcept
skos:hasTopConcept rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#member
skos:member rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#note
skos:note rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#prefLabel
skos:prefLabel rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#scopeNote
skos:scopeNote rdf:type owl:AnnotationProperty .


###  http://www.w3.org/ns/odrl/2/profile
:profile rdf:type owl:AnnotationProperty ;
         rdfs:domain :Policy .


###  http://www.w3.org/ns/odrl/2/rightOperandReference
:rightOperandReference rdf:type owl:AnnotationProperty ;
                       rdfs:domain :Constraint .


###  http://www.w3.org/ns/odrl/2/source
:source rdf:type owl:AnnotationProperty ;
        rdfs:domain _:genid56 .


###  http://www.w3.org/ns/odrl/2/status
:status rdf:type owl:AnnotationProperty ;
        rdfs:domain :Constraint .


###  http://www.w3.org/ns/odrl/2/uid
:uid rdf:type owl:AnnotationProperty ;
     rdfs:domain _:genid62 .


###  http://www.w3.org/ns/odrl/2/unit
:unit rdfs:isDefinedBy <http://www.w3.org/ns/odrl/2/> ;
      rdfs:label "Unit"@en ;
      skos:definition "The unit of measurement of the value of the rightOperand or rightOperandReference of a Constraint."@en ;
      rdf:type owl:AnnotationProperty ;
      rdfs:domain :Constraint .


#################################################################
#    Object Properties
#################################################################

###  http://www.w3.org/ns/odrl/2/action
:action rdf:type owl:ObjectProperty ;
        rdfs:domain [ rdf:type owl:Class ;
                      owl:unionOf ( :Policy
                                    :Rule
                                  )
                    ] ;
        rdfs:range :Action .


###  http://www.w3.org/ns/odrl/2/and
:and rdf:type owl:ObjectProperty ;
     rdfs:subPropertyOf :operand .


###  http://www.w3.org/ns/odrl/2/andSequence
:andSequence rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf :operand .


###  http://www.w3.org/ns/odrl/2/assignee
:assignee rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf :function ;
          rdfs:domain [ rdf:type owl:Class ;
                        owl:unionOf ( :Policy
                                      :Rule
                                    )
                      ] ;
          rdfs:range :Party .


###  http://www.w3.org/ns/odrl/2/assigneeOf
:assigneeOf rdf:type owl:ObjectProperty ;
            rdfs:domain :Party ;
            rdfs:range :Policy .


###  http://www.w3.org/ns/odrl/2/assigner
:assigner rdf:type owl:ObjectProperty ;
          rdfs:subPropertyOf :function ;
          rdfs:domain [ rdf:type owl:Class ;
                        owl:unionOf ( :Policy
                                      :Rule
                                    )
                      ] ;
          rdfs:range :Party .


###  http://www.w3.org/ns/odrl/2/assignerOf
:assignerOf rdf:type owl:ObjectProperty ;
            rdfs:domain :Party ;
            rdfs:range :Policy .


###  http://www.w3.org/ns/odrl/2/attributedParty
:attributedParty rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf :function .


###  http://www.w3.org/ns/odrl/2/attributingParty
:attributingParty rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf :function .


###  http://www.w3.org/ns/odrl/2/compensatedParty
:compensatedParty rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf :function .


###  http://www.w3.org/ns/odrl/2/compensatingParty
:compensatingParty rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf :function .


###  http://www.w3.org/ns/odrl/2/conflict
:conflict rdf:type owl:ObjectProperty ;
          rdfs:domain :Policy ;
          rdfs:range :ConflictTerm .


###  http://www.w3.org/ns/odrl/2/consentedParty
:consentedParty rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf :function .


###  http://www.w3.org/ns/odrl/2/consentingParty
:consentingParty rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf :function .


###  http://www.w3.org/ns/odrl/2/consequence
:consequence rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf :failure ;
             rdfs:domain :Duty ;
             rdfs:range :Duty .


###  http://www.w3.org/ns/odrl/2/constraint
:constraint rdf:type owl:ObjectProperty ;
            rdfs:domain [ rdf:type owl:Class ;
                          owl:unionOf ( :Policy
                                        :Rule
                                      )
                        ] ;
            rdfs:range [ rdf:type owl:Class ;
                         owl:unionOf ( :Constraint
                                       :LogicalConstraint
                                     )
                       ] .


###  http://www.w3.org/ns/odrl/2/contractedParty
:contractedParty rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf :function .


###  http://www.w3.org/ns/odrl/2/contractingParty
:contractingParty rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf :function .


###  http://www.w3.org/ns/odrl/2/dataType
:dataType rdf:type owl:ObjectProperty ;
          rdfs:domain :Constraint ;
          rdfs:range rdfs:Datatype .


###  http://www.w3.org/ns/odrl/2/duty
:duty rdf:type owl:ObjectProperty ;
      rdfs:domain :Permission ;
      rdfs:range :Duty .


###  http://www.w3.org/ns/odrl/2/failure
:failure rdf:type owl:ObjectProperty ;
         rdfs:domain :Rule ;
         rdfs:range :Rule .


###  http://www.w3.org/ns/odrl/2/function
:function rdf:type owl:ObjectProperty ;
          rdfs:domain [ rdf:type owl:Class ;
                        owl:unionOf ( :Policy
                                      :Rule
                                    )
                      ] ;
          rdfs:range :Party .


###  http://www.w3.org/ns/odrl/2/hasPolicy
:hasPolicy rdf:type owl:ObjectProperty ;
           rdfs:domain :Asset ;
           rdfs:range :Policy .


###  http://www.w3.org/ns/odrl/2/implies
:implies rdf:type owl:ObjectProperty ;
         rdfs:domain :Action ;
         rdfs:range :Action .


###  http://www.w3.org/ns/odrl/2/includedIn
:includedIn rdf:type owl:ObjectProperty ,
                     owl:TransitiveProperty ;
            rdfs:domain :Action ;
            rdfs:range :Action .


###  http://www.w3.org/ns/odrl/2/informedParty
:informedParty rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf :function .


###  http://www.w3.org/ns/odrl/2/informingParty
:informingParty rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf :function .


###  http://www.w3.org/ns/odrl/2/inheritFrom
:inheritFrom rdf:type owl:ObjectProperty ;
             rdfs:domain :Policy ;
             rdfs:range :Policy .


###  http://www.w3.org/ns/odrl/2/inheritRelation
:inheritRelation rdf:type owl:ObjectProperty .


###  http://www.w3.org/ns/odrl/2/leftOperand
:leftOperand rdf:type owl:ObjectProperty ;
             rdfs:domain :Constraint ;
             rdfs:range :LeftOperand .


###  http://www.w3.org/ns/odrl/2/obligation
:obligation rdf:type owl:ObjectProperty ;
            rdfs:domain :Policy ;
            rdfs:range :Duty .


###  http://www.w3.org/ns/odrl/2/operand
:operand rdf:type owl:ObjectProperty ;
         rdfs:domain :LogicalConstraint .


###  http://www.w3.org/ns/odrl/2/operator
:operator rdf:type owl:ObjectProperty ;
          rdfs:domain :Constraint ;
          rdfs:range :Operator .


###  http://www.w3.org/ns/odrl/2/or
:or rdf:type owl:ObjectProperty ;
    rdfs:subPropertyOf :operand .


###  http://www.w3.org/ns/odrl/2/output
:output rdf:type owl:ObjectProperty ;
        rdfs:subPropertyOf :relation ;
        rdfs:domain :Rule ;
        rdfs:range :Asset .


###  http://www.w3.org/ns/odrl/2/partOf
:partOf rdf:type owl:ObjectProperty ;
        rdfs:domain [ rdf:type owl:Class ;
                      owl:unionOf ( :Asset
                                    :Party
                                  )
                    ] ;
        rdfs:range [ rdf:type owl:Class ;
                     owl:unionOf ( :AssetCollection
                                   :PartyCollection
                                 )
                   ] .


###  http://www.w3.org/ns/odrl/2/payeeParty
:payeeParty rdf:type owl:ObjectProperty .


###  http://www.w3.org/ns/odrl/2/permission
:permission rdf:type owl:ObjectProperty ;
            rdfs:domain :Policy ;
            rdfs:range :Permission .


###  http://www.w3.org/ns/odrl/2/prohibition
:prohibition rdf:type owl:ObjectProperty ;
             rdfs:domain :Policy ;
             rdfs:range :Prohibition .


###  http://www.w3.org/ns/odrl/2/refinement
:refinement rdf:type owl:ObjectProperty ;
            rdfs:domain [ rdf:type owl:Class ;
                          owl:unionOf ( :Action
                                        :AssetCollection
                                        :PartyCollection
                                      )
                        ] ;
            rdfs:range [ rdf:type owl:Class ;
                         owl:unionOf ( :Constraint
                                       :LogicalConstraint
                                     )
                       ] .


###  http://www.w3.org/ns/odrl/2/relation
:relation rdf:type owl:ObjectProperty ;
          rdfs:domain [ rdf:type owl:Class ;
                        owl:unionOf ( :Policy
                                      :Rule
                                    )
                      ] ;
          rdfs:range :Asset .


###  http://www.w3.org/ns/odrl/2/remedy
:remedy rdf:type owl:ObjectProperty ;
        rdfs:subPropertyOf :failure ;
        rdfs:domain :Prohibition ;
        rdfs:range :Duty .


###  http://www.w3.org/ns/odrl/2/rightOperand
:rightOperand rdf:type owl:ObjectProperty ;
              rdfs:domain :Constraint ;
              rdfs:range [ rdf:type owl:Class ;
                           owl:unionOf ( rdfs:Literal
                                         xsd:anyURI
                                         :RightOperand
                                       )
                         ] .


###  http://www.w3.org/ns/odrl/2/scope
:scope rdf:type owl:ObjectProperty .


###  http://www.w3.org/ns/odrl/2/target
:target rdf:type owl:ObjectProperty ;
        rdfs:subPropertyOf :relation ;
        rdfs:domain [ rdf:type owl:Class ;
                      owl:unionOf ( :Policy
                                    :Rule
                                  )
                    ] ;
        rdfs:range :Asset .


###  http://www.w3.org/ns/odrl/2/trackedParty
:trackedParty rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf :function .


###  http://www.w3.org/ns/odrl/2/trackingParty
:trackingParty rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf :function .


###  http://www.w3.org/ns/odrl/2/undefined
:undefined rdf:type owl:ObjectProperty ;
           rdfs:range :UndefinedTerm .


###  http://www.w3.org/ns/odrl/2/xone
:xone rdf:type owl:ObjectProperty ;
      rdfs:subPropertyOf :operand .


#################################################################
#    Data properties
#################################################################

###  http://www.w3.org/ns/odrl/2/inheritAllowed
:inheritAllowed rdf:type owl:DatatypeProperty .


###  http://www.w3.org/ns/odrl/2/proximity
:proximity rdf:type owl:DatatypeProperty .


###  http://www.w3.org/ns/odrl/2/timedCount
:timedCount rdf:type owl:DatatypeProperty ;
            rdfs:range rdfs:Literal .


#################################################################
#    Classes
#################################################################

###  http://schema.org/Action
schema:Action rdf:type owl:Class .


###  http://schema.org/Organization
schema:Organization rdf:type owl:Class .


###  http://schema.org/Person
schema:Person rdf:type owl:Class .


###  http://www.w3.org/2000/01/rdf-schema#Datatype
rdfs:Datatype rdf:type owl:Class .


###  http://www.w3.org/2001/XMLSchema#anyURI
xsd:anyURI rdf:type owl:Class .


###  http://www.w3.org/2004/02/skos/core#Collection
skos:Collection rdf:type owl:Class .


###  http://www.w3.org/2004/02/skos/core#Concept
skos:Concept rdf:type owl:Class .


###  http://www.w3.org/2004/02/skos/core#ConceptScheme
skos:ConceptScheme rdf:type owl:Class .


###  http://www.w3.org/2006/vcard/ns#Agent
vcard:Agent rdf:type owl:Class .


###  http://www.w3.org/2006/vcard/ns#Individual
vcard:Individual rdf:type owl:Class .


###  http://www.w3.org/2006/vcard/ns#Organization
vcard:Organization rdf:type owl:Class .


###  http://www.w3.org/ns/odrl/2/Action
:Action rdf:type owl:Class ;
        rdfs:subClassOf schema:Action .


###  http://www.w3.org/ns/odrl/2/Agreement
:Agreement rdf:type owl:Class ;
           rdfs:subClassOf :Policy ;
           owl:disjointWith :Assertion ,
                            :Offer ,
                            :Privacy ,
                            :Request ,
                            :Set ,
                            :Ticket .


###  http://www.w3.org/ns/odrl/2/Assertion
:Assertion rdf:type owl:Class ;
           rdfs:subClassOf :Policy ;
           owl:disjointWith :Offer ,
                            :Privacy ,
                            :Request ,
                            :Set ,
                            :Ticket .


###  http://www.w3.org/ns/odrl/2/Asset
:Asset rdf:type owl:Class .


###  http://www.w3.org/ns/odrl/2/AssetCollection
:AssetCollection rdf:type owl:Class ;
                 rdfs:subClassOf :Asset .


###  http://www.w3.org/ns/odrl/2/AssetScope
:AssetScope rdf:type owl:Class .


###  http://www.w3.org/ns/odrl/2/ConflictTerm
:ConflictTerm rdf:type owl:Class .


###  http://www.w3.org/ns/odrl/2/Constraint
:Constraint rdf:type owl:Class .


###  http://www.w3.org/ns/odrl/2/Duty
:Duty rdf:type owl:Class ;
      rdfs:subClassOf :Rule ;
      owl:disjointWith :Permission ,
                       :Prohibition .


###  http://www.w3.org/ns/odrl/2/LeftOperand
:LeftOperand rdf:type owl:Class .


###  http://www.w3.org/ns/odrl/2/LogicalConstraint
:LogicalConstraint rdf:type owl:Class .


###  http://www.w3.org/ns/odrl/2/Offer
:Offer rdf:type owl:Class ;
       rdfs:subClassOf :Policy ;
       owl:disjointWith :Privacy ,
                        :Request ,
                        :Set ,
                        :Ticket .


###  http://www.w3.org/ns/odrl/2/Operator
:Operator rdf:type owl:Class .


###  http://www.w3.org/ns/odrl/2/Party
:Party rdf:type owl:Class ;
       rdfs:subClassOf [ rdf:type owl:Class ;
                         owl:unionOf ( schema:Organization
                                       schema:Person
                                       vcard:Agent
                                       vcard:Individual
                                       vcard:Organization
                                       foaf:Agent
                                       foaf:Organization
                                       foaf:Person
                                     )
                       ] .


###  http://www.w3.org/ns/odrl/2/PartyCollection
:PartyCollection rdf:type owl:Class ;
                 rdfs:subClassOf :Party .


###  http://www.w3.org/ns/odrl/2/PartyScope
:PartyScope rdf:type owl:Class .


###  http://www.w3.org/ns/odrl/2/Permission
:Permission rdf:type owl:Class ;
            rdfs:subClassOf :Rule ;
            owl:disjointWith :Prohibition .


###  http://www.w3.org/ns/odrl/2/Policy
:Policy rdf:type owl:Class .


###  http://www.w3.org/ns/odrl/2/Privacy
:Privacy rdf:type owl:Class ;
         rdfs:subClassOf :Policy ;
         owl:disjointWith :Request ,
                          :Set ,
                          :Ticket .


###  http://www.w3.org/ns/odrl/2/Prohibition
:Prohibition rdf:type owl:Class ;
             rdfs:subClassOf :Rule .


###  http://www.w3.org/ns/odrl/2/Request
:Request rdf:type owl:Class ;
         rdfs:subClassOf :Policy ;
         owl:disjointWith :Set ,
                          :Ticket .


###  http://www.w3.org/ns/odrl/2/RightOperand
:RightOperand rdf:type owl:Class .


###  http://www.w3.org/ns/odrl/2/Rule
:Rule rdf:type owl:Class .


###  http://www.w3.org/ns/odrl/2/Set
:Set rdf:type owl:Class ;
     rdfs:subClassOf :Policy ;
     owl:disjointWith :Ticket .


###  http://www.w3.org/ns/odrl/2/Ticket
:Ticket rdf:type owl:Class ;
        rdfs:subClassOf :Policy .


###  http://www.w3.org/ns/odrl/2/UndefinedTerm
:UndefinedTerm rdf:type owl:Class .


###  http://xmlns.com/foaf/0.1/Agent
foaf:Agent rdf:type owl:Class .


###  http://xmlns.com/foaf/0.1/Organization
foaf:Organization rdf:type owl:Class .


###  http://xmlns.com/foaf/0.1/Person
foaf:Person rdf:type owl:Class .


#################################################################
#    Individuals
#################################################################

###  http://creativecommons.org/ns#Attribution
cc:Attribution rdf:type owl:NamedIndividual ,
                        skos:Concept ,
                        :Action ;
               :includedIn :use ;
               rdfs:isDefinedBy <http://www.w3.org/ns/odrl/2/> ;
               rdfs:label "Attribution"@en ;
               skos:definition "Credit be given to copyright holder and/or author."@en ;
               skos:note "This term is defined by Creative Commons."@en ;
               skos:scopeNote "Non-Normative"@en .


###  http://creativecommons.org/ns#CommericalUse
cc:CommericalUse rdf:type owl:NamedIndividual ,
                          skos:Concept ,
                          :Action ;
                 :includedIn :use ;
                 rdfs:isDefinedBy <http://www.w3.org/ns/odrl/2/> ;
                 rdfs:label "Commercial Use"@en ;
                 skos:definition "Exercising rights for commercial purposes."@en ;
                 skos:note "This term is defined by Creative Commons."@en ;
                 skos:scopeNote "Non-Normative"@en .


###  http://creativecommons.org/ns#DerivativeWorks
cc:DerivativeWorks rdf:type owl:NamedIndividual ,
                            skos:Concept ,
                            :Action ;
                   :includedIn :use ;
                   rdfs:isDefinedBy <http://www.w3.org/ns/odrl/2/> ;
                   rdfs:label "Derivative Works"@en ;
                   skos:definition "Distribution of derivative works."@en ;
                   skos:note "This term is defined by Creative Commons."@en ;
                   skos:scopeNote "Non-Normative"@en .


###  http://creativecommons.org/ns#Distribution
cc:Distribution rdf:type owl:NamedIndividual ,
                         skos:Concept ,
                         :Action ;
                :includedIn :use ;
                rdfs:isDefinedBy <http://www.w3.org/ns/odrl/2/> ;
                rdfs:label "Distribution"@en ;
                skos:definition "Distribution, public display, and publicly performance."@en ;
                skos:note "This term is defined by Creative Commons."@en ;
                skos:scopeNote "Non-Normative"@en .


###  http://creativecommons.org/ns#Notice
cc:Notice rdf:type owl:NamedIndividual ,
                   skos:Concept ,
                   :Action ;
          :includedIn :use ;
          rdfs:isDefinedBy <http://www.w3.org/ns/odrl/2/> ;
          rdfs:label "Notice"@en ;
          skos:definition "Copyright and license notices be kept intact."@en ;
          skos:note "This term is defined by Creative Commons."@en ;
          skos:scopeNote "Non-Normative"@en .


###  http://creativecommons.org/ns#Reproduction
cc:Reproduction rdf:type owl:NamedIndividual ,
                         skos:Concept ,
                         :Action ;
                :includedIn :use ;
                rdfs:isDefinedBy <http://www.w3.org/ns/odrl/2/> ;
                rdfs:label "Reproduction"@en ;
                skos:definition "Making multiple copies."@en ;
                skos:note "This term is defined by Creative Commons."@en ;
                skos:scopeNote "Non-Normative"@en .


###  http://creativecommons.org/ns#ShareAlike
cc:ShareAlike rdf:type owl:NamedIndividual ,
                       skos:Concept ,
                       :Action ;
              :includedIn :use ;
              rdfs:isDefinedBy <http://www.w3.org/ns/odrl/2/> ;
              rdfs:label "Share Alike"@en ;
              skos:definition "Derivative works be licensed under the same terms or compatible terms as the original work."@en ;
              skos:note "This term is defined by Creative Commons."@en ;
              skos:scopeNote "Non-Normative"@en .


###  http://creativecommons.org/ns#Sharing
cc:Sharing rdf:type owl:NamedIndividual ,
                    skos:Concept ,
                    :Action ;
           :includedIn :use ;
           rdfs:isDefinedBy <http://www.w3.org/ns/odrl/2/> ;
           rdfs:label "Sharing"@en ;
           skos:definition "Permits commercial derivatives, but only non-commercial distribution."@en ;
           skos:note "This term is defined by Creative Commons."@en ;
           skos:scopeNote "Non-Normative"@en .


###  http://creativecommons.org/ns#SourceCode
cc:SourceCode rdf:type owl:NamedIndividual ,
                       skos:Concept ,
                       :Action ;
              :includedIn :use ;
              rdfs:isDefinedBy <http://www.w3.org/ns/odrl/2/> ;
              rdfs:label "Source Code"@en ;
