@base <http://data.europa.eu/euodp/ontologies/ec-odp> .

<http://data.europa.eu/euodp/ontologies/ec-odp> rdf:type owl:Ontology ;
                                                 owl:imports <http://www.w3.org/ns/dcat> ;
                                                 rdfs:comment "Ontology describing the metadata vocabulary for the Open Data Portal of the European Union" ;
                                                 rdfs:label "ODP Metadata Vocabulary" .

#################################################################
#    Object Properties
#################################################################

###  http://data.europa.eu/euodp/ontologies/ec-odp#contactPoint
:contactPoint rdf:type owl:ObjectProperty ;
              rdfs:domain dcat:Dataset ;
              rdfs:range foaf:Agent ;
              rdfs:comment "main contact, for enquiries about this particular dataset"@en ;
              rdfs:isDefinedBy <http://data.europa.eu/euodp/ontologies/ec-odp> ;
              rdfs:label "Contact Point"@en .


###  http://data.europa.eu/euodp/ontologies/ec-odp#datasetStatus
:datasetStatus rdf:type owl:ObjectProperty ;
               rdfs:domain dcat:Dataset ;
               rdfs:range skos:Concept ;
               rdfs:comment "Dataset status"@en ;
               rdfs:isDefinedBy <http://data.europa.eu/euodp/ontologies/ec-odp> ;
               rdfs:label "Dataset Status"@en .


###  http://data.europa.eu/euodp/ontologies/ec-odp#datasetType
:datasetType rdf:type owl:ObjectProperty ;
             rdfs:domain dcat:Dataset ;
             rdfs:range skos:Concept ;
             rdfs:comment "Based on multiple controlled vocabularies, including http://purl.org/adms/assettype" ,
                          "Type of the dataset" ;
             rdfs:isDefinedBy <http://data.europa.eu/euodp/ontologies/ec-odp> .


###  http://data.europa.eu/euodp/ontologies/ec-odp#documentationFormat
:documentationFormat rdf:type owl:ObjectProperty ;
                     rdfs:domain :DatasetDocumentation ;
                     rdfs:range skos:PlainLiteral ;
                     rdfs:comment "Format of the documentation" ;
                     rdfs:isDefinedBy <http://data.europa.eu/euodp/ontologies/ec-odp> ;
                     rdfs:label "Documentation Format"@en .


###  http://data.europa.eu/euodp/ontologies/ec-odp#documentationType
:documentationType rdf:type owl:ObjectProperty ;
                   rdfs:domain :DatasetDocumentation ;
                   rdfs:range skos:Concept ;
                   rdfs:comment "Type of the documentation" ;
                   rdfs:isDefinedBy <http://data.europa.eu/euodp/ontologies/ec-odp> ;
                   rdfs:label "Documentation Type"@en .


###  http://data.europa.eu/euodp/ontologies/ec-odp#interoperabilityLevel
:interoperabilityLevel rdf:type owl:ObjectProperty ;
                       rdfs:domain dcat:Dataset ;
                       rdfs:range skos:Concept ;
                       rdfs:comment "Based on ADMS controlled vocabulary"@en ,
                                    "Interoperability level for which the dataset is relevant"@en ;
                       rdfs:isDefinedBy <http://data.europa.eu/euodp/ontologies/ec-odp> ;
                       rdfs:label "Interoperability Level"@en .


###  http://data.europa.eu/euodp/ontologies/ec-odp#isDocumentedBy
:isDocumentedBy rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf dct:isReferencedBy ;
                rdfs:domain dcat:Dataset ;
                rdfs:range :DatasetDocumentation ;
                rdfs:comment "Documentation or specification of the dataset" ;
                rdfs:isDefinedBy <http://data.europa.eu/euodp/ontologies/ec-odp> .


###  http://data.europa.eu/euodp/ontologies/ec-odp#record
:record rdf:type owl:ObjectProperty ;
        rdfs:comment "can not use dcat:record to link the dataset to its records as the domain of dcat:record is resctricted to dcat:Catalog"@en ;
        rdfs:isDefinedBy <http://data.europa.eu/euodp/ontologies/ec-odp> ;
        rdfs:label "Catalog record"@en .


###  http://purl.org/dc/terms/isReferencedBy
dct:isReferencedBy rdf:type owl:ObjectProperty .


#################################################################
#    Data properties
#################################################################

###  http://data.europa.eu/euodp/ontologies/ec-odp#accessURL
:accessURL rdf:type owl:DatatypeProperty ;
           rdfs:range xsd:anyURI ;
           rdfs:comment "Can not use dcat:accessURL as it does restrict the domain to Dataset"@en ;
           rdfs:isDefinedBy <http://data.europa.eu/euodp/ontologies/ec-odp> ;
           rdfs:label "Access URL"@en .


###  http://data.europa.eu/euodp/ontologies/ec-odp#accrualPeriodicity
:accrualPeriodicity rdf:type owl:DatatypeProperty ;
                    rdfs:comment "The frequency with which the dataset is published."@en ;
                    rdfs:isDefinedBy <http://data.europa.eu/euodp/ontologies/ec-odp> ;
                    rdfs:label "Accrual Periodicity"@en .


###  http://data.europa.eu/euodp/ontologies/ec-odp#contactAddress
:contactAddress rdf:type owl:DatatypeProperty ;
                rdfs:domain foaf:Agent ;
                rdfs:range rdf:PlainLiteral ;
                rdfs:comment "the full address of the contact point"@en ;
                rdfs:isDefinedBy <http://data.europa.eu/euodp/ontologies/ec-odp> ;
                rdfs:label "Contact Address"@en .


###  http://data.europa.eu/euodp/ontologies/ec-odp#distributionFormat
:distributionFormat rdf:type owl:DatatypeProperty ;
                    rdfs:domain dcat:Distribution ;
                    rdfs:range rdf:PlainLiteral ;
                    rdfs:comment "This should give the format in which the distribution is supplied."@en ;
                    rdfs:isDefinedBy <http://data.europa.eu/euodp/ontologies/ec-odp> ;
                    rdfs:label "Distribution Format"@en .


###  http://data.europa.eu/euodp/ontologies/ec-odp#keyword
:keyword rdf:type owl:DatatypeProperty ;
         rdfs:range rdf:PlainLiteral ;
         rdfs:comment "can not use dcat:keyword with text values as its a subproperty of dcat:subject that recommends use of a controlled vocabulary"^^rdfs:Literal ;
         rdfs:isDefinedBy <http://data.europa.eu/euodp/ontologies/ec-odp> ;
         rdfs:label "Keyword"@en .


###  http://data.europa.eu/euodp/ontologies/ec-odp#periodEnd
:periodEnd rdf:type owl:DatatypeProperty ;
           rdfs:domain dct:PeriodOfTime ;
           rdfs:range rdfs:Literal ;
           rdfs:comment "Time period end"@en ;
           rdfs:isDefinedBy <http://data.europa.eu/euodp/ontologies/ec-odp> ;
           rdfs:label "Time Period End"@en .


###  http://data.europa.eu/euodp/ontologies/ec-odp#periodStart
:periodStart rdf:type owl:DatatypeProperty ;
             rdfs:domain dct:PeriodOfTime ;
             rdfs:range rdfs:Literal ;
             rdfs:comment "Time period start"@en ;
             rdfs:isDefinedBy <http://data.europa.eu/euodp/ontologies/ec-odp> ;
             rdfs:label "Time Period Start"@en .


###  http://data.europa.eu/euodp/ontologies/ec-odp#ckan-name%20
: rdf:type owl:DatatypeProperty ;
  rdfs:domain dcat:Dataset ;
  rdfs:range rdf:PlainLiteral ;
  rdfs:comment "the CKAN name identifier. It is a string which used to create a nice URL on the EU ODP portal."@en ;
  rdfs:isDefinedBy <http://data.europa.eu/euodp/ontologies/ec-odp> ;
  rdfs:label "CKAN name"@en .


