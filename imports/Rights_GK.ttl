# === Algemene definities ===

naa:DeRee rdf:type org:FormalOrganization , premis:Organization ;
          rdfs:label "De Ree archiefsystemen"^^xsd:string ;
		  org:purpose naa:CorporateType .

naa:MaisFlexis rdf:type premis:Environment ;
               rdfs:label "Mais Flexis"^^^xsd:string ;
			   premis:hasVersion "6"^^xsd:integer ;     # Dummywaarde
			   dct:creator naa:DeRee .

# === Rechten op een document (intellectuele eigendom en toegangsrechten) ===

naa:RightsType_CCBY rdf:type premis:License ;    # Is een subklasse van premis:RightsBasis 
                             rfds:label "CC BY"^^xsd:string ;
                             premis:hasNote "Is openbaar op grond van de cc by licentie"@nl , "Declared to be public domain by the intellectual owner, provided it is published under an attribution"@en ;
							 premis:hasJurisdiction <http://nl.dbpedia.org/resource/Nederlands_recht> ;
							 premis:hasDocumentation "https://creativecommons.nl/#"^^xsd:anyURI ;
							 premis:hasTerms "Attribution"@en , "Naamsvermelding"@nl .

# === Typologieen en verzamelingen ===

naa:OrganisationTypes rdf:type skos:ConceptScheme .
naa:CorporateType rdf:type skos:Concept ; skos:inScheme naa:OrganisationTypes ; skos:prefLabel "Particuliere onderneming"@nl ; skos:label "Corporation"@en .
naa:AgencyType rdf:type skos:Concept ; skos:inScheme naa:OrganisationTypes ; skos:prefLabel "Agentschap overheid"@nl ; skos:label "Governmental agency"@en .

naa:TypesOfMaterials rdf:type skos:ConceptScheme .
naa:Photography rdf:type skos:Concept ; skos:inScheme naa:TypesOfMaterials ; skos:prefLabel "Foto"@nl ; skos:label "Photography"@en .
naa:PhotographyBW rdf:type skos:Concept ; skos:inScheme naa:TypesOfMaterials ; skos:prefLabel "Zwart-wit foto"@nl ; skos:label "Photography black and white"@en ;
                           skos:broader naa:Photography .
naa:PhotographyColour rdf:type skos:Concept ; skos:inScheme naa:TypesOfMaterials ; skos:prefLabel "Kleurenfoto"@nl ; skos:label "Photography in colour"@en ;
                           skos:broader naa:Photography .

			   
