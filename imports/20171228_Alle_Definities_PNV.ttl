def:PersonEntity rdf:type owl:Class ;
                 owl:equivalentClass pnv:Person .


###  http://archief.nl/pnv/def/Person
pnv:Person rdf:type owl:Class ;
           owl:equivalentClass bgn:Person ,
                               dbo:Person ,
                               schema:Person ,
                               cidoc:E21_person ,
                               foaf:Person ;
           owl:disjointWith pnv:PersonName ;
           rdfs:comment "A Person is a human being whose individual existence can somehow be documented"^^xsd:string ;
           rdfs:label "Person"^^xsd:string .


###  http://archief.nl/pnv/def/PersonName
pnv:PersonName rdf:type owl:Class ;
               owl:equivalentClass bgn:Name ,
                                   dbo:Name ;
               rdfs:comment "A name is considered to be a resource type in its own right"^^xsd:string ;
               rdfs:label "A Person's name"^^xsd:string .


