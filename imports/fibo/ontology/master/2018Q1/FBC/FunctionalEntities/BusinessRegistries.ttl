<https://spec.edmcouncil.org/fibo/ontology/FBC/FunctionalEntities/BusinessRegistries/>
	a owl:Ontology ;
	rdfs:label "Business Registries Ontology" ;
	dct:abstract "This ontology extends the Registration Authorities ontology to define specific kinds of registries, such as business entity registries, registries for identifiers and codes of various sorts, and registries for financial institutions and intermediaries based on jurisdiction, who regulates them, and the services they provide." ;
	dct:license "http://www.omg.org/techprocess/ab/SpecificationMetadata/MITLicense"^^xsd:anyURI ;
	sm:contentLanguage
		"http://www.omg.org/spec/ODM/"^^xsd:anyURI ,
		"http://www.w3.org/standards/techs/owl#w3c_all"^^xsd:anyURI
		;
	sm:copyright """Copyright (c) 2015-2017 EDM Council, Inc.
Copyright (c) 2015-2017 Object Management Group, Inc.""" ;
	sm:dependsOn
		"http://www.omg.org/spec/LCC/"^^xsd:anyURI ,
		"https://spec.edmcouncil.org/fibo/ontology/BE/"^^xsd:anyURI ,
		"https://spec.edmcouncil.org/fibo/ontology/FBC/FunctionalEntities/RegistrationAuthorities/"^^xsd:anyURI ,
		"https://spec.edmcouncil.org/fibo/ontology/FND/"^^xsd:anyURI
		;
	sm:fileAbbreviation "fibo-fbc-fct-breg" ;
	sm:filename "BusinessRegistries.rdf" ;
	rdfs:seeAlso
		<https://spec.edmcouncil.org/fibo/ontology/AboutTheEDMC-FIBOFamily/> ,
		<https://spec.edmcouncil.org/fibo/ontology/FBC/AboutFBC/> ,
		<https://spec.edmcouncil.org/fibo/ontology/FBC/FunctionalEntities/AboutFBCFunctionalEntities/>
		;
	owl:imports
		<http://www.omg.org/spec/LCC/Countries/CountryRepresentation/> ,
		<https://spec.edmcouncil.org/fibo/ontology/master/2018Q1/BE/Corporations/Corporations/> ,
		<https://spec.edmcouncil.org/fibo/ontology/master/2018Q1/BE/GovernmentEntities/GovernmentEntities/> ,
		<https://spec.edmcouncil.org/fibo/ontology/master/2018Q1/BE/LegalEntities/FormalBusinessOrganizations/> ,
		<https://spec.edmcouncil.org/fibo/ontology/master/2018Q1/BE/LegalEntities/LEIEntities/> ,
		<https://spec.edmcouncil.org/fibo/ontology/master/2018Q1/BE/LegalEntities/LegalPersons/> ,
		<https://spec.edmcouncil.org/fibo/ontology/master/2018Q1/FBC/FunctionalEntities/RegistrationAuthorities/> ,
		<https://spec.edmcouncil.org/fibo/ontology/master/2018Q1/FND/AgentsAndPeople/Agents/> ,
		<https://spec.edmcouncil.org/fibo/ontology/master/2018Q1/FND/Arrangements/IdentifiersAndIndices/> ,
		<https://spec.edmcouncil.org/fibo/ontology/master/2018Q1/FND/DatesAndTimes/FinancialDates/> ,
		<https://spec.edmcouncil.org/fibo/ontology/master/2018Q1/FND/Law/Jurisdiction/> ,
		<https://spec.edmcouncil.org/fibo/ontology/master/2018Q1/FND/Places/Addresses/> ,
		<https://spec.edmcouncil.org/fibo/ontology/master/2018Q1/FND/Places/VirtualPlaces/> ,
		<https://spec.edmcouncil.org/fibo/ontology/master/2018Q1/FND/Relations/Relations/> ,
		<https://spec.edmcouncil.org/fibo/ontology/master/2018Q1/FND/Utilities/AnnotationVocabulary/>
		;
	owl:versionIRI <https://spec.edmcouncil.org/fibo/ontology/master/2018Q1/FBC/FunctionalEntities/BusinessRegistries/> ;
	skos:changeNote "The https://spec.edmcouncil.org/fibo/ontology/FBC/FunctionalEntities/BusinessRegistries.rdf version of this ontology was modified per the issue resolutions identified in the FIBO FBC 1.1 RTF report." ;
	fibo-fnd-utl-av:hasMaturityLevel fibo-fnd-utl-av:Release ;
	.

fibo-be-le-fbo:RegisteredAddress
	a owl:Class ;
	rdfs:subClassOf fibo-fbc-fct-breg:RegistrationAddress ;
	.

fibo-be-le-lp:LegalEntity
	a owl:Class ;
	rdfs:subClassOf [
		a owl:Restriction ;
		owl:onProperty fibo-fbc-fct-breg:hasHeadquartersAddress ;
		owl:onClass fibo-fbc-fct-breg:RegistrationAddress ;
		owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
	] ;
	.

fibo-fbc-fct-breg:ActiveStatus
	a
		owl:NamedIndividual ,
		fibo-fbc-fct-breg:EntityStatus
		;
	rdfs:label "active status" ;
	skos:definition "as of the last report or update, the entity was legally registered and operating" ;
	fibo-fnd-rel-rel:hasTag "ACTIVE" ;
	fibo-fnd-utl-av:adaptedFrom "https://www.gleif.org/en/about-lei/common-data-file-format/lei-cdf-format/lei-cdf-format-version-2-1"^^xsd:anyURI ;
	.

fibo-fbc-fct-breg:AnnulledStatus
	a
		owl:NamedIndividual ,
		fibo-fbc-fct-breg:RegistrationStatus
		;
	rdfs:label "annulled status" ;
	skos:definition "status indicating that the registration was determined to be erroneous or invalid after issuance" ;
	fibo-fnd-rel-rel:hasTag "ANNULLED" ;
	fibo-fnd-utl-av:adaptedFrom "https://www.gleif.org/en/about-lei/common-data-file-format/lei-cdf-format/lei-cdf-format-version-2-1"^^xsd:anyURI ;
	.

fibo-fbc-fct-breg:BusinessRegisterIdentifier
	a owl:Class ;
	rdfs:subClassOf
		fibo-fnd-arr-id:Identifier ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-aap-agt:identifies ;
			owl:onClass fibo-fbc-fct-breg:BusinessRegistry ;
			owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fbc-fct-ra:isRegisteredBy ;
			owl:someValuesFrom fibo-fbc-fct-breg:BusinessRegistrationAuthority ;
		]
		;
	rdfs:label "business register identifier" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/FBC/FunctionalEntities/BusinessRegistries/> ;
	skos:definition "an identifier that uniquely identifies a business register, such as a register identified by the Global Legal Entity Identifier Foundation (GLEIF) registration authorities list" ;
	fibo-fnd-utl-av:adaptedFrom "https://www.gleif.org/en/about-lei/gleif-registration-authorities-list"^^xsd:anyURI ;
	.

fibo-fbc-fct-breg:BusinessRegistrationAuthority
	a owl:Class ;
	rdfs:subClassOf
		fibo-fbc-fct-ra:RegistrationAuthority ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fbc-fct-ra:registers ;
			owl:someValuesFrom fibo-be-corp-corp:RegistrationIdentifier ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-rel-rel:manages ;
			owl:someValuesFrom fibo-fbc-fct-breg:BusinessRegistry ;
		]
		;
	rdfs:label "business registration authority" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/FBC/FunctionalEntities/BusinessRegistries/> ;
	skos:definition "a registration authority that is responsible for maintaining a registry of business entities" ;
	fibo-fnd-utl-av:adaptedFrom "https://www.gleif.org/en/about-lei/gleif-registration-authorities-list"^^xsd:anyURI ;
	fibo-fnd-utl-av:explanatoryNote "A business registry may include any government-managed registry for registering a business, such as a state department of corporations in the US, as well as other registries such as a local operating unit (LOU) for registration of legal entity identifiers (LEIs).  Any sanctioned registration authority as defined by the Registration Authorities List, published by GLEIF, is a business registration authority in this sense." ;
	.

fibo-fbc-fct-breg:BusinessRegistry
	a owl:Class ;
	rdfs:subClassOf
		fibo-fbc-fct-ra:Registry ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fbc-fct-breg:hasRegistryName ;
			owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
			owl:onDataRange xsd:string ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-rel-rel:isManagedBy ;
			owl:onClass fibo-fbc-fct-breg:BusinessRegistrationAuthority ;
			owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fbc-fct-ra:hasRegistryEntry ;
			owl:someValuesFrom fibo-fbc-fct-breg:BusinessRegistryEntry ;
		]
		;
	rdfs:label "business registry" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/FBC/FunctionalEntities/BusinessRegistries/> ;
	skos:definition "a registry for registering and maintaining information about business entities" ;
	fibo-fnd-utl-av:adaptedFrom "https://www.gleif.org/en/about-lei/gleif-registration-authorities-list"^^xsd:anyURI ;
	.

fibo-fbc-fct-breg:BusinessRegistryEntry
	a owl:Class ;
	rdfs:subClassOf
		fibo-fbc-fct-ra:RegistryEntry ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fbc-fct-breg:hasRegistrationStatus ;
			owl:onClass fibo-fbc-fct-breg:RegistrationStatus ;
			owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fbc-fct-ra:hasRegistrationDate ;
			owl:someValuesFrom [
				a rdfs:Datatype ;
				owl:unionOf (
					xsd:string
					xsd:dateTime
					xsd:dateTimeStamp
				) ;
			] ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-rel-rel:comprises ;
			owl:someValuesFrom fibo-be-corp-corp:RegistrationIdentifier ;
		]
		;
	rdfs:label "business registry entry" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/FBC/FunctionalEntities/BusinessRegistries/> ;
	skos:definition "an entry in a business registry" ;
	.

fibo-fbc-fct-breg:CancelledStatus
	a
		owl:NamedIndividual ,
		fibo-fbc-fct-breg:RegistrationStatus
		;
	rdfs:label "cancelled status" ;
	skos:definition "status indicating that the registration was abandoned prior to issuance" ;
	fibo-fnd-rel-rel:hasTag "CANCELLED" ;
	fibo-fnd-utl-av:adaptedFrom "https://www.gleif.org/en/about-lei/common-data-file-format/lei-cdf-format/lei-cdf-format-version-2-1"^^xsd:anyURI ;
	.

fibo-fbc-fct-breg:DuplicateStatus
	a
		owl:NamedIndividual ,
		fibo-fbc-fct-breg:RegistrationStatus
		;
	rdfs:label "duplicate status" ;
	skos:definition "status indicating that the registration was determined to be a duplicate registration of the same entity as another registration; duplicate status is assigned to the non-surviving registration (i.e. the identifier that should no longer be used)" ;
	fibo-fnd-rel-rel:hasTag "DUPLICATE" ;
	fibo-fnd-utl-av:adaptedFrom "https://www.gleif.org/en/about-lei/common-data-file-format/lei-cdf-format/lei-cdf-format-version-2-1"^^xsd:anyURI ;
	.

fibo-fbc-fct-breg:EntityExpirationReasonEnumeration
	a rdfs:Datatype ;
	rdfs:label "entity expiration reason enumeration" ;
	owl:equivalentClass [
		a rdfs:Datatype ;
		owl:oneOf (
			"DISSOLVED"
			"CORPORATE_ACTION"
			"OTHER"
		) ;
	] ;
	skos:definition "an enumerated datatype specifying the reason that a legal entity ceased to exist and/or operate as specified in the LEI registry for that entity" ;
	fibo-fnd-utl-av:adaptedFrom "https://www.gleif.org/en/about-lei/common-data-file-format/lei-cdf-format/lei-cdf-format-version-2-1"^^xsd:anyURI ;
	.

fibo-fbc-fct-breg:EntityStatus
	a owl:Class ;
	rdfs:subClassOf fibo-fnd-arr-lif:LifecycleStage ;
	rdfs:label "entity status" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/FBC/FunctionalEntities/BusinessRegistries/> ;
	skos:definition "a lifecycle stage indicating the operational and/or legal status of an entity as specified in a given business registry for that entity" ;
	fibo-fnd-utl-av:adaptedFrom "https://www.gleif.org/en/about-lei/common-data-file-format/lei-cdf-format/lei-cdf-format-version-2-1"^^xsd:anyURI ;
	fibo-fnd-utl-av:explanatoryNote "In some jurisdictions, there may be other possible values for entity status, such as suspended in the State of California, thus the individuals provided herein are not intended to be exhaustive." ;
	.

fibo-fbc-fct-breg:EntityValidationLevelEnumeration
	a rdfs:Datatype ;
	rdfs:label "entity validation level enumeration" ;
	owl:equivalentClass [
		a rdfs:Datatype ;
		owl:oneOf (
			"ENTITY_SUPPLIED_ONLY"
			"PARTIALLY_CORROBORATED"
			"FULLY_CORROBORATED"
		) ;
	] ;
	skos:definition "an enumerated datatype specifying the level of validation performed by the GLEIF or LOU with respect to the reference data provided by the registrant" ;
	fibo-fnd-utl-av:adaptedFrom "https://www.gleif.org/en/about-lei/common-data-file-format/lei-cdf-format/lei-cdf-format-version-2-1"^^xsd:anyURI ;
	.

fibo-fbc-fct-breg:InactiveStatus
	a
		owl:NamedIndividual ,
		fibo-fbc-fct-breg:EntityStatus
		;
	rdfs:label "inactive status" ;
	skos:definition "the entity that was assigned the identifier (business, LEI, or other) is no longer legally registered and/or operating, whether as a result of business closure, acquisition by or merger with another (or new) entity, or determination of illegitimacy" ;
	fibo-fnd-rel-rel:hasTag "INACTIVE" ;
	fibo-fnd-utl-av:adaptedFrom "https://www.gleif.org/en/about-lei/common-data-file-format/lei-cdf-format/lei-cdf-format-version-2-1"^^xsd:anyURI ;
	.

fibo-fbc-fct-breg:IssuedStatus
	a
		owl:NamedIndividual ,
		fibo-fbc-fct-breg:RegistrationStatus
		;
	rdfs:label "issued status" ;
	skos:definition "status indicating that the registration has been validated and issued, and which identifies an entity that was operating legally as of the last update" ;
	fibo-fnd-rel-rel:hasTag "ISSUED" ;
	fibo-fnd-utl-av:adaptedFrom "https://www.gleif.org/en/about-lei/common-data-file-format/lei-cdf-format/lei-cdf-format-version-2-1"^^xsd:anyURI ;
	.

fibo-fbc-fct-breg:LapsedStatus
	a
		owl:NamedIndividual ,
		fibo-fbc-fct-breg:RegistrationStatus
		;
	rdfs:label "lapsed status" ;
	skos:definition "status indicating that the registration that has not been renewed by the specified renewal date and is not known by public sources to have ceased operation as of the last update" ;
	fibo-fnd-rel-rel:hasTag "LAPSED" ;
	fibo-fnd-utl-av:adaptedFrom "https://www.gleif.org/en/about-lei/common-data-file-format/lei-cdf-format/lei-cdf-format-version-2-1"^^xsd:anyURI ;
	.

fibo-fbc-fct-breg:LegalEntityIdentifierRegistry
	a owl:Class ;
	rdfs:subClassOf
		fibo-fbc-fct-breg:BusinessRegistry ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-rel-rel:isManagedBy ;
			owl:onClass fibo-fbc-fct-breg:LocalOperatingUnit ;
			owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fbc-fct-ra:hasRegistryEntry ;
			owl:someValuesFrom fibo-fbc-fct-breg:LegalEntityIdentifierRegistryEntry ;
		]
		;
	rdfs:label "legal entity identifier registry" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/FBC/FunctionalEntities/BusinessRegistries/> ;
	skos:definition "a registry for registering and maintaining information about business entities for a particular jurisdiction" ;
	fibo-fnd-utl-av:abbreviation "LEI registry" ;
	fibo-fnd-utl-av:adaptedFrom "https://www.gleif.org/en/about-lei/common-data-file-format/lei-cdf-format/lei-cdf-format-version-2-1"^^xsd:anyURI ;
	.

fibo-fbc-fct-breg:LegalEntityIdentifierRegistryEntry
	a owl:Class ;
	rdfs:subClassOf
		fibo-fbc-fct-breg:BusinessRegistryEntry ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fbc-fct-breg:hasValidationAuthority ;
			owl:onClass fibo-fbc-fct-breg:BusinessRegistrationAuthority ;
			owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fbc-fct-breg:hasRegistrationStatus ;
			owl:someValuesFrom fibo-fbc-fct-breg:RegistrationStatus ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fbc-fct-breg:hasValidationLevel ;
			owl:someValuesFrom fibo-fbc-fct-breg:EntityValidationLevelEnumeration ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-rel-rel:comprises ;
			owl:someValuesFrom fibo-be-le-lei:LegalEntityIdentifier ;
		]
		;
	rdfs:label "legal entity identifier registry entry" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/FBC/FunctionalEntities/BusinessRegistries/> ;
	skos:definition "an entry in a legal entity identifier registry that conforms to ISO 17442 and the Global Legal Entity Identifier Foundation (GLEIF) Common Data Format (CDF)" ;
	fibo-fnd-utl-av:abbreviation "LEI registry entry" ;
	fibo-fnd-utl-av:adaptedFrom "https://www.gleif.org/en/about-lei/common-data-file-format/lei-cdf-format/lei-cdf-format-version-2-1"^^xsd:anyURI ;
	.

fibo-fbc-fct-breg:LocalOperatingUnit
	a owl:Class ;
	rdfs:subClassOf
		fibo-fbc-fct-breg:BusinessRegistrationAuthority ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fbc-fct-ra:registers ;
			owl:someValuesFrom fibo-be-le-lei:LegalEntityIdentifier ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-rel-rel:issues ;
			owl:someValuesFrom fibo-be-le-lei:LegalEntityIdentifier ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-rel-rel:manages ;
			owl:someValuesFrom fibo-fbc-fct-breg:LegalEntityIdentifierRegistry ;
		]
		;
	rdfs:label "local operating unit" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/FBC/FunctionalEntities/BusinessRegistries/> ;
	skos:definition "a registration authority that issues legal entity identifiers and is authorized by the Global LEI Foundation to do so" ;
	fibo-fnd-utl-av:abbreviation "LOU" ;
