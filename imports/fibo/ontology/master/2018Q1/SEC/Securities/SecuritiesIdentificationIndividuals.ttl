fibo-sec-sec-idind:CGSCUSIPAccessRepository
	a
		owl:NamedIndividual ,
		fibo-sec-sec-id:NationalSecuritiesIdentifyingNumberRegistry
		;
	rdfs:label "CGS CUSIP Access Repository" ;
	rdfs:seeAlso <https://www.cusip.com/cusip/index.htm> ;
	skos:definition "individual representing the CGS (CUSIP Global Services) CUSIP Access services and repository, a proprietary repository of security identifiers, issued by CUSIP Global Services, that is the National Securities Identifying Number (NSIN) for securities issued in North America, which is also part of the ISIN for the security it identifies" ;
	fibo-fnd-rel-rel:isManagedBy fibo-sec-sec-idind:CUSIPGlobalServices ;
	.

fibo-sec-sec-idind:CUSIPGlobalServices
	a
		owl:NamedIndividual ,
		fibo-fbc-pas-fpas:FinancialServiceProvider ,
		fibo-ind-ind-ind:FinancialInformationPublisher ,
		fibo-sec-sec-id:NationalNumberingAgency
		;
	rdfs:label "CUSIP Global Services" ;
	rdfs:seeAlso <http://www.cusip.com/> ;
	skos:definition "individual representing the CUSIP Global Services financial services provider that is the national numbering agency (NNA) for CUSIPs in North America" ;
	fibo-fbc-fct-ra:registers fibo-sec-sec-idind:CommitteeOnUniformSecuritiesIdentificationProceduresNumber ;
	fibo-fnd-rel-rel:hasIdentity fibo-fbc-fct-usfsind:SPGlobalInc-US-NY ;
	fibo-fnd-rel-rel:isManagedBy fibo-fbc-fct-usfsind:SPGlobalInc-US-NY ;
	fibo-fnd-rel-rel:issues fibo-sec-sec-idind:CommitteeOnUniformSecuritiesIdentificationProceduresNumber ;
	.

fibo-sec-sec-idind:CUSIPInternationalNumberingSystemNumber
	a
		owl:NamedIndividual ,
		fibo-fbc-fct-ra:RegistryIdentifier ,
		fibo-sec-sec-id:ProprietarySecurityIdentifier
		;
	rdfs:label "CUSIP International Numbering System (CINS) number" ;
	rdfs:seeAlso <http://isin.net/cins/> ;
	skos:definition "a 9-character alphanumeric identifier that employs the same 9 characters as CUSIP, but also contains a letter of the alphabet in the first position signifying the issuer's country or geographic region, issued by CUSIP Global Services" ;
	fibo-fbc-fct-ra:isRegisteredBy fibo-sec-sec-idind:CUSIPGlobalServices ;
	fibo-fbc-fct-ra:isRegisteredIn fibo-sec-sec-idind:CGSCUSIPAccessRepository ;
	fibo-fnd-rel-rel:hasDefinition fibo-sec-sec-idind:CUSIPInternationalNumberingSystemScheme ;
	fibo-fnd-rel-rel:isIssuedBy fibo-sec-sec-idind:CUSIPGlobalServices ;
	fibo-fnd-utl-av:abbreviation "CINS" ;
	fibo-fnd-utl-av:adaptedFrom
		"http://www.investopedia.com/terms/c/cins.asp"^^xsd:anyURI ,
		"https://www.cusip.com/cusip/about-cgs-identifiers.htm"^^xsd:anyURI
		;
	fibo-fnd-utl-av:synonym "CINS number" ;
	.

fibo-sec-sec-idind:CUSIPInternationalNumberingSystemScheme
	a
		owl:NamedIndividual ,
		fibo-sec-sec-id:ProprietarySecurityIdentificationScheme
		;
	rdfs:label "CUSIP International Numbering System (CINS) scheme" ;
	skos:definition "a security identification scheme that extends the CUSIP scheme, used to identify securities outside of the United States and Canada for the purposes of facilitating clearing and settlement of trades" ;
	fibo-fnd-rel-rel:defines fibo-sec-sec-idind:CUSIPInternationalNumberingSystemNumber ;
	fibo-fnd-utl-av:abbreviation "CINS scheme" ;
	fibo-fnd-utl-av:adaptedFrom "https://www.cusip.com/cusip/about-cgs-identifiers.htm"^^xsd:anyURI ;
	.

fibo-sec-sec-idind:CommitteeOnUniformSecuritiesIdentificationProceduresNumber
	a
		owl:NamedIndividual ,
		fibo-fbc-fct-ra:RegistryIdentifier ,
		fibo-sec-sec-id:NationalSecuritiesIdentifyingNumber ,
		fibo-sec-sec-id:ProprietarySecurityIdentifier
		;
	rdfs:label "Committee on Uniform Securities Identification Procedures (CUSIP) number" ;
	rdfs:seeAlso <https://www.cusip.com/cusip/about-cgs-identifiers.htm> ;
	skos:definition "a nine-character alphanumeric number that identifies all North American stocks and registered bonds for the purposes of facilitating clearing and settlement of trades, issued by CUSIP Global Services on behalf of the American Bankers' Association, which is a part of Standard and Poor's Capital IQ, that is the National Numbering Agency Identifier for securities issued in North America, which is also part of the ISIN for the security it identifies" ;
	fibo-fbc-fct-ra:isRegisteredBy fibo-sec-sec-idind:CUSIPGlobalServices ;
	fibo-fbc-fct-ra:isRegisteredIn fibo-sec-sec-idind:CGSCUSIPAccessRepository ;
	fibo-fnd-rel-rel:hasDefinition fibo-sec-sec-idind:CommitteeOnUniformSecuritiesIdentificationProceduresScheme ;
	fibo-fnd-rel-rel:isIssuedBy fibo-sec-sec-idind:CUSIPGlobalServices ;
	fibo-fnd-utl-av:abbreviation "CUSIP" ;
	fibo-fnd-utl-av:adaptedFrom "Barron's Dictionary of Finance and Investment Terms, Ninth Edition, 2014" ;
	fibo-fnd-utl-av:synonym "CUSIP number" ;
	.

fibo-sec-sec-idind:CommitteeOnUniformSecuritiesIdentificationProceduresScheme
	a
		owl:NamedIndividual ,
		fibo-sec-sec-id:NationalSecurityIdentificationScheme ,
		fibo-sec-sec-id:ProprietarySecurityIdentificationScheme
		;
	rdfs:label "Committee on Uniform Securities Identification Procedures (CUSIP) scheme" ;
	rdfs:seeAlso <https://www.cusip.com/cusip/about-cgs-identifiers.htm> ;
	skos:definition "a national security identification scheme used to identify all North American stocks and registered bonds for the purposes of facilitating clearing and settlement of trades" ;
	fibo-fnd-rel-rel:defines fibo-sec-sec-idind:CommitteeOnUniformSecuritiesIdentificationProceduresNumber ;
	fibo-fnd-utl-av:abbreviation "CUSIP scheme" ;
	fibo-fnd-utl-av:adaptedFrom "Barron's Dictionary of Finance and Investment Terms, Ninth Edition, 2014" ;
	.

fibo-sec-sec-idind:EuroclearClearstreamCommonCode
	a
		owl:NamedIndividual ,
		fibo-fbc-fct-ra:RegistryIdentifier ,
		fibo-sec-sec-id:ProprietarySecurityIdentifier
		;
	rdfs:label "Euroclear Clearstream common code" ;
	skos:definition "a 9-character alphanumeric securities identifier, issued in Luxembourg, jointly by Euroclear and Clearstream" ;
	fibo-fbc-fct-ra:isRegisteredBy
		fibo-fbc-fct-eufseind:Clearstream ,
		fibo-fbc-fct-eufseind:Euroclear
		;
	fibo-fnd-rel-rel:hasDefinition fibo-sec-sec-idind:EuroclearClearstreamCommonCodeScheme ;
	fibo-fnd-rel-rel:isIssuedBy
		fibo-fbc-fct-eufseind:Clearstream ,
		fibo-fbc-fct-eufseind:Euroclear
		;
	fibo-fnd-utl-av:adaptedFrom "http://www.isin.net/common-code-isin/"^^xsd:anyURI ;
	fibo-fnd-utl-av:synonym "common code" ;
	.

fibo-sec-sec-idind:EuroclearClearstreamCommonCodeScheme
	a
		owl:NamedIndividual ,
		fibo-sec-sec-id:ProprietarySecurityIdentificationScheme
		;
	rdfs:label "Euroclear Clearstream common code  scheme" ;
	skos:definition "a nine-digit security identification scheme, defined originally by Euroclear and CEDEL (now Clearstream) that is used to identify securities in Europe for the purposes of facilitating clearing and settlement of trades" ;
	fibo-fnd-rel-rel:defines fibo-sec-sec-idind:EuroclearClearstreamCommonCode ;
	fibo-fnd-utl-av:synonym "common code scheme" ;
	.

fibo-sec-sec-idind:FinancialInstrumentGlobalIdentifier
	a
		owl:NamedIndividual ,
		fibo-fbc-fi-fi:FinancialInstrumentIdentifier ,
		fibo-fbc-fct-ra:RegistryIdentifier
		;
	rdfs:label "financial instrument global identifier" ;
	skos:definition "a financial instrument identifier that is defined as specified in the Object Management Group (OMG) Financial Instrument Global Identifier (FIGI) Specification" ;
	fibo-fbc-fct-ra:isRegisteredBy fibo-fbc-fct-usfsind:BloombergLP ;
	fibo-fbc-fct-ra:isRegisteredIn fibo-sec-sec-idind:FinancialInstrumentGlobalIdentifierRegistry ;
	fibo-fnd-rel-rel:hasDefinition fibo-sec-sec-idind:FinancialInstrumentGlobalIdentifierScheme ;
	fibo-fnd-utl-av:abbreviation "FIGI" ;
	.

fibo-sec-sec-idind:FinancialInstrumentGlobalIdentifierRegistry
	a
		owl:NamedIndividual ,
		fibo-fbc-fct-ra:Registry
		;
	rdfs:label "Financial Instrument Global Identifier (FIGI) Registry" ;
	rdfs:seeAlso <http://www.openfigi.com/> ;
	skos:definition "an open, OMG standards-based registry used by the FIGI registration authority to manage the financial instrument identifiers and related information that it registers according to the Financial Instrument Global Identifier (FIGI) standard" ;
	fibo-fnd-rel-rel:isCharacterizedBy fibo-sec-sec-idind:FinancialInstrumentGlobalIdentifierScheme ;
	fibo-fnd-rel-rel:isManagedBy fibo-fbc-fct-usfsind:BloombergLP ;
	fibo-fnd-utl-av:abbreviation "FIGI Registry" ;
	.

fibo-sec-sec-idind:FinancialInstrumentGlobalIdentifierRegistryEntry
	a owl:Class ;
	rdfs:subClassOf
		fibo-fbc-fct-ra:RegistryEntry ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-rel-rel:appliesTo ;
			owl:allValuesFrom fibo-fbc-fi-fi:FinancialInstrument ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-rel-rel:comprises ;
			owl:hasValue fibo-sec-sec-idind:FinancialInstrumentGlobalIdentifier ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-arr-arr:isConstituentOf ;
			owl:hasValue fibo-sec-sec-idind:FinancialInstrumentGlobalIdentifierRegistry ;
		]
		;
	rdfs:label "Financial Instrument Global Identifier (FIGI) registry entry" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Securities/SecuritiesIdentificationIndividuals/> ;
	skos:definition "an entry in a Financial Instrument Global Identifier (FIGI) registry" ;
	fibo-fnd-utl-av:abbreviation "FIGI registry entry" ;
	.

fibo-sec-sec-idind:FinancialInstrumentGlobalIdentifierScheme
	a
		owl:NamedIndividual ,
		fibo-fbc-fct-ra:RegistrationScheme ,
		fibo-sec-sec-id:FinancialInstrumentIdentificationScheme
		;
	rdfs:label "financial instrument global identifier scheme" ;
	skos:definition "a standard identification scheme for financial instrument identifiers (not limited to securities) published by the Object Management Group (OMG)" ;
	fibo-fnd-rel-rel:characterizes fibo-sec-sec-idind:FinancialInstrumentGlobalIdentifierRegistry ;
	fibo-fnd-rel-rel:defines fibo-sec-sec-idind:FinancialInstrumentGlobalIdentifier ;
	fibo-fnd-utl-av:abbreviation "FIGI scheme" ;
	.

