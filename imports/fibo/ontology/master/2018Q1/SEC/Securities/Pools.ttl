		<https://spec.edmcouncil.org/fibo/ontology/FND/>
		;
	sm:fileAbbreviation "fibo-sec-sec-pls" ;
	sm:filename "Pools.rdf" ;
	rdfs:seeAlso
		<https://spec.edmcouncil.org/fibo/ontology/AboutTheEDMC-FIBOFamily/> ,
		<https://spec.edmcouncil.org/fibo/ontology/SEC/AboutSEC/> ,
		<https://spec.edmcouncil.org/fibo/ontology/SEC/Securities/AboutSecurities/>
		;
	owl:imports
		<https://spec.edmcouncil.org/fibo/ontology/master/2018Q1/BE/LegalEntities/LegalPersons/> ,
		<https://spec.edmcouncil.org/fibo/ontology/master/2018Q1/FBC/FinancialInstruments/FinancialInstruments/> ,
		<https://spec.edmcouncil.org/fibo/ontology/master/2018Q1/FBC/FunctionalEntities/FinancialServicesEntities/> ,
		<https://spec.edmcouncil.org/fibo/ontology/master/2018Q1/FBC/ProductsAndServices/FinancialProductsAndServices/> ,
		<https://spec.edmcouncil.org/fibo/ontology/master/2018Q1/FND/Accounting/AccountingEquity/> ,
		<https://spec.edmcouncil.org/fibo/ontology/master/2018Q1/FND/Accounting/CurrencyAmount/> ,
		<https://spec.edmcouncil.org/fibo/ontology/master/2018Q1/FND/Arrangements/Arrangements/> ,
		<https://spec.edmcouncil.org/fibo/ontology/master/2018Q1/FND/OwnershipAndControl/Ownership/> ,
		<https://spec.edmcouncil.org/fibo/ontology/master/2018Q1/FND/Parties/Roles/> ,
