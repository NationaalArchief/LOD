@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

<https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/DebtInstruments/>
	a owl:Ontology ;
	rdfs:label "Debt Instruments Ontology" ;
	dct:abstract "This ontology defines concepts that are specific to debt instruments (tradable and non-tradable)." ;
	dct:license sm:MITLicense ;
	sm:contentLanguage
		"http://www.omg.org/spec/ODM/"^^xsd:anyURI ,
		"http://www.w3.org/standards/techs/owl#w3c_all"^^xsd:anyURI
		;
	sm:copyright """Copyright (c) 2016-2018 EDM Council, Inc.
Copyright (c) 2016-2018 Object Management Group, Inc.""" ;
	sm:dependsOn
		"https://spec.edmcouncil.org/fibo/ontology/FBC/"^^xsd:anyURI ,
		"https://spec.edmcouncil.org/fibo/ontology/FND/"^^xsd:anyURI ,
		"https://spec.edmcouncil.org/fibo/ontology/IND/"^^xsd:anyURI ,
		"https://spec.edmcouncil.org/fibo/ontology/SEC/Securities/"^^xsd:anyURI
		;
	sm:fileAbbreviation "fibo-sec-dbt-dbti" ;
	sm:filename "DebtInstruments.rdf" ;
	rdfs:seeAlso
		<https://spec.edmcouncil.org/fibo/ontology/AboutTheEDMC-FIBOFamily/> ,
		<https://spec.edmcouncil.org/fibo/ontology/SEC/AboutSEC/> ,
		<https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/AboutDebtInstruments/> ,
		<https://spec.edmcouncil.org/fibo/ontology/SEC/Securities/AboutSecurities/>
		;
	owl:imports
		<https://spec.edmcouncil.org/fibo/ontology/master/2018Q1/FBC/DebtAndEquities/Debt/> ,
		<https://spec.edmcouncil.org/fibo/ontology/master/2018Q1/FBC/DebtAndEquities/Guaranty/> ,
		<https://spec.edmcouncil.org/fibo/ontology/master/2018Q1/FBC/FinancialInstruments/FinancialInstruments/> ,
		<https://spec.edmcouncil.org/fibo/ontology/master/2018Q1/FBC/ProductsAndServices/FinancialProductsAndServices/> ,
		<https://spec.edmcouncil.org/fibo/ontology/master/2018Q1/FND/Accounting/CurrencyAmount/> ,
		<https://spec.edmcouncil.org/fibo/ontology/master/2018Q1/FND/Agreements/Contracts/> ,
		<https://spec.edmcouncil.org/fibo/ontology/master/2018Q1/FND/DatesAndTimes/BusinessDates/> ,
		<https://spec.edmcouncil.org/fibo/ontology/master/2018Q1/FND/DatesAndTimes/FinancialDates/> ,
		<https://spec.edmcouncil.org/fibo/ontology/master/2018Q1/FND/DatesAndTimes/Occurrences/> ,
		<https://spec.edmcouncil.org/fibo/ontology/master/2018Q1/FND/Quantities/QuantitiesAndUnits/> ,
		<https://spec.edmcouncil.org/fibo/ontology/master/2018Q1/FND/Relations/Relations/> ,
		<https://spec.edmcouncil.org/fibo/ontology/master/2018Q1/FND/Utilities/AnnotationVocabulary/> ,
		<https://spec.edmcouncil.org/fibo/ontology/master/2018Q1/IND/EconomicIndicators/EconomicIndicators/> ,
		<https://spec.edmcouncil.org/fibo/ontology/master/2018Q1/SEC/Securities/SecuritiesIssuance/>
		;
	owl:versionIRI <https://spec.edmcouncil.org/fibo/ontology/master/2018Q1/SEC/Debt/DebtInstruments/> ;
	fibo-fnd-utl-av:hasMaturityLevel fibo-fnd-utl-av:Release ;
	.

fibo-fbc-fi-fi:DebtInstrument
	a owl:Class ;
	rdfs:subClassOf
		[
			a owl:Restriction ;
			owl:onProperty fibo-sec-dbt-dbti:hasCallFeature ;
			owl:allValuesFrom fibo-sec-dbt-dbti:CallFeature ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-sec-dbt-dbti:hasRedemptionProvision ;
			owl:onClass fibo-fbc-dae-dbt:RedemptionProvision ;
			owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fbc-pas-fpas:hasOffering ;
			owl:onClass fibo-sec-dbt-dbti:DebtOffering ;
			owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-sec-dbt-dbti:hasInterestPaymentTerms ;
			owl:someValuesFrom fibo-fbc-dae-dbt:InterestPaymentTerms ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-sec-dbt-dbti:hasRepaymentTerms ;
			owl:someValuesFrom fibo-fbc-dae-dbt:PrincipalRepaymentTerms ;
		]
		;
	.

fibo-sec-dbt-dbti:AtADiscount
	a
		owl:NamedIndividual ,
		fibo-sec-dbt-dbti:RelativePrice
		;
	rdfs:label "at a discount" ;
	skos:definition "a selling price that is less than the face or nominal value" ;
	fibo-fnd-utl-av:adaptedFrom
		"Barron's Dictionary of Business and Economics Terms, Fifth Edition, 2012" ,
		"http://www.investopedia.com/terms/a/at-a-discount.asp"
		;
	fibo-fnd-utl-av:synonym "below par" ;
	.

fibo-sec-dbt-dbti:AtAPremium
	a
		owl:NamedIndividual ,
		fibo-sec-dbt-dbti:RelativePrice
		;
	rdfs:label "at a premium" ;
	skos:definition "a selling price significantly above the stated face or redemption value due to high demand or timing of redemption" ;
	fibo-fnd-utl-av:adaptedFrom
		"Barron's Dictionary of Business and Economics Terms, Fifth Edition, 2012" ,
		"http://www.investopedia.com/terms/a/at-a-premium.asp"
		;
	fibo-fnd-utl-av:synonym
		"above par" ,
		"premium"
		;
	.

fibo-sec-dbt-dbti:CallEvent
	a owl:Class ;
	rdfs:subClassOf fibo-fnd-dt-oc:OccurrenceKind ;
	rdfs:label "call event" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/DebtInstruments/> ;
	skos:definition "an event associated with the call schedule for a debt instrument" ;
	.

fibo-sec-dbt-dbti:CallFeature
	a owl:Class ;
	rdfs:subClassOf
		fibo-sec-dbt-dbti:TradableDebtInstrumentRedemptionProvision ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-rel-rel:comprises ;
			owl:someValuesFrom fibo-sec-dbt-dbti:CallNotificationProvision ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-rel-rel:comprises ;
			owl:someValuesFrom fibo-sec-dbt-dbti:CallSchedule ;
		]
		;
	rdfs:label "call feature" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/DebtInstruments/> ;
	skos:definition "a redemption provision defining the schedule and price of redemption prior to maturity" ;
	fibo-fnd-utl-av:adaptedFrom "Barron's Dictionary of Business and Economics Terms, Fifth Edition, 2012" ;
	fibo-fnd-utl-av:explanatoryNote "Most corporate and municipal bonds have ten-year call features (termed call protection by holders); government securities typically have none." ;
	fibo-fnd-utl-av:synonym "call provision" ;
	.

fibo-sec-dbt-dbti:CallNotificationProvision
	a owl:Class ;
	rdfs:subClassOf
		fibo-sec-dbt-dbti:TradableDebtInstrumentRedemptionProvision ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-dt-fd:hasDatePeriod ;
			owl:allValuesFrom fibo-sec-dbt-dbti:CallWindow ;
		]
		;
	rdfs:label "call notification provision" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/DebtInstruments/> ;
	skos:definition "a provision of a call feature defining notification requirements" ;
	.

fibo-sec-dbt-dbti:CallPremium
	a owl:Class ;
	rdfs:subClassOf fibo-fnd-acc-cur:MonetaryPrice ;
	rdfs:label "call premium" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/DebtInstruments/> ;
	skos:definition "an amount over par that an issuer must pay an investor to redeem the security early" ;
	fibo-fnd-utl-av:adaptedFrom "Barron's Dictionary of Business and Economics Terms, Fifth Edition, 2012" ;
	.

fibo-sec-dbt-dbti:CallPrice
	a owl:Class ;
	rdfs:subClassOf fibo-fnd-acc-cur:MonetaryPrice ;
	rdfs:label "call price" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/DebtInstruments/> ;
	skos:definition "a price at which a debt instrument with a call feature may be redeemed by the issuer" ;
	fibo-fnd-utl-av:adaptedFrom "Barron's Dictionary of Business and Economics Terms, Fifth Edition, 2012" ;
	fibo-fnd-utl-av:synonym "redemption price" ;
	fibo-ind-ei-ei:actualExpression "par value + call premium" ;
	.

fibo-sec-dbt-dbti:CallSchedule
	a owl:Class ;
	rdfs:subClassOf
		fibo-fnd-dt-fd:Schedule ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-rel-rel:comprises ;
			owl:someValuesFrom fibo-sec-dbt-dbti:CallEvent ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-rel-rel:comprises ;
			owl:someValuesFrom [
				a owl:Class ;
				owl:unionOf (
					fibo-sec-dbt-dbti:CallPremium
					fibo-sec-dbt-dbti:CallPrice
				) ;
			] ;
		]
		;
	rdfs:label "call schedule" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/DebtInstruments/> ;
	skos:definition "a schedule that defines the events associated with the call feature of a debt instrument, i.e, the dates on which the debt instrument may be called (recalled) at what price by the issuer" ;
	fibo-fnd-utl-av:adaptedFrom "Barron's Dictionary of Business and Economics Terms, Fifth Edition, 2012" ;
	.

fibo-sec-dbt-dbti:CallWindow
	a owl:Class ;
	rdfs:subClassOf fibo-fnd-dt-fd:DatePeriod ;
	rdfs:label "call window" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/DebtInstruments/> ;
	skos:definition "an explicit period of time prior to a call date during which an issuer or agent must give notice to holders" ;
	.

fibo-sec-dbt-dbti:DebtOffering
	a owl:Class ;
	rdfs:subClassOf
		fibo-sec-sec-iss:SecuritiesOffering ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-rel-rel:appliesTo ;
			owl:someValuesFrom fibo-fbc-fi-fi:DebtInstrument ;
		]
		;
	rdfs:label "debt offering" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/DebtInstruments/> ;
	skos:definition "an offering of one or more debt instruments for sale" ;
	.

fibo-sec-dbt-dbti:IssuedDebt
	a owl:Class ;
	rdfs:subClassOf
		fibo-fbc-dae-dbt:Debt ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-rel-rel:isConferredBy ;
			owl:allValuesFrom fibo-fbc-fi-fi:DebtInstrument ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fbc-dae-dbt:hasDebtAmount ;
			owl:allValuesFrom fibo-fnd-acc-cur:MonetaryAmount ;
		]
		;
	rdfs:label "issued debt" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/DebtInstruments/> ;
	skos:definition "debt that is set out by the borrower in some form of financial security in which the lender is the holder or counterparty of that security" ;
	.

fibo-sec-dbt-dbti:MakeWholeFeature
	a owl:Class ;
	rdfs:subClassOf fibo-sec-dbt-dbti:CallFeature ;
	rdfs:label "make whole feature" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/DebtInstruments/> ;
	skos:definition "a call provision allowing the issuer to pay off remaining debt early" ;
	fibo-fnd-utl-av:adaptedFrom "http://www.investopedia.com/terms/m/make-wholecall.asp"^^xsd:anyURI ;
	fibo-fnd-utl-av:explanatoryNote "The issuer typically has to make a lump sum payment to the investor derived from a formula based on the net present value (NPV) of future interest or coupon payments that will not be paid incrementally because of the call combined with the principal payment the investor would have received at maturity." ;
	fibo-fnd-utl-av:synonym "make whole provision" ;
	.

fibo-sec-dbt-dbti:NonTradableDebtInstrument
	a owl:Class ;
	rdfs:subClassOf
		fibo-fbc-fi-fi:DebtInstrument ,
		fibo-fnd-agr-ctr:MutualContractualAgreement ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-sec-dbt-dbti:hasRedemptionProvision ;
			owl:onClass fibo-sec-dbt-dbti:NonTradableDebtRedemptionProvision ;
			owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-sec-dbt-dbti:hasRelativePriceAtIssue ;
			owl:someValuesFrom fibo-sec-dbt-dbti:RelativePrice ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-sec-dbt-dbti:hasRelativePriceAtRedemption ;
			owl:someValuesFrom fibo-sec-dbt-dbti:RelativePrice ;
		]
		;
	rdfs:label "non-tradable debt instrument" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/DebtInstruments/> ;
	owl:disjointWith fibo-sec-dbt-dbti:TradableDebtInstrument ;
	skos:definition "a debt instrument that may not be bought or sold" ;
	skos:example "Low-risk instruments such as savings bonds are examples of nonnegotiable debt instruments." ;
	fibo-fnd-utl-av:adaptedFrom "Non-Tradeable. (n.d.) Farlex Financial Dictionary. (2009). Retrieved December 16 2016 from http://financial-dictionary.thefreedictionary.com/Non-Tradeable" ;
	fibo-fnd-utl-av:explanatoryNote "Generally, a nonnegotiable instrument may be redeemed by the issuer, but this is often subject to some limitations." ;
	.

fibo-sec-dbt-dbti:NonTradableDebtRedemptionProvision
	a owl:Class ;
	rdfs:subClassOf fibo-fbc-dae-dbt:RedemptionProvision ;
	rdfs:label "non-tradable debt redemption terms" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/DebtInstruments/> ;
	skos:definition "terms for repayment of principal on debt instruments that are not traded securities, such as over the counter, bilateral paper" ;
	.

fibo-sec-dbt-dbti:ParValue
	a
		owl:NamedIndividual ,
		fibo-sec-dbt-dbti:RelativePrice
		;
	rdfs:label "par value" ;
	skos:definition "the stated value of a negotiable instrument, stock, or bond, as compared with the value that instrument might receive when sold" ;
	fibo-fnd-utl-av:adaptedFrom
		"Barron's Dictionary of Business and Economics Terms, Fifth Edition, 2012" ,
		"http://www.investopedia.com/terms/p/par.asp"
		;
	fibo-fnd-utl-av:synonym
		"face value" ,
		"nominal value" ,
		"par"
		;
	.

fibo-sec-dbt-dbti:PubliclyIssuedDebt
	a owl:Class ;
	rdfs:subClassOf
		fibo-sec-dbt-dbti:IssuedDebt ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-rel-rel:isConferredBy ;
			owl:allValuesFrom fibo-sec-dbt-dbti:TradableDebtInstrument ;
		]
		;
	rdfs:label "publicly issued debt" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/DebtInstruments/> ;
	skos:definition "an issued debt in the form of a tradable debt instrument (security)" ;
	.

fibo-sec-dbt-dbti:PutEvent
	a owl:Class ;
	rdfs:subClassOf fibo-fnd-dt-oc:OccurrenceKind ;
	rdfs:label "put event" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/DebtInstruments/> ;
	skos:definition "an event associated with the put schedule for a debt instrument, i.e., an event involving the 'put', or surrender of the instrument by the holder" ;
	.

fibo-sec-dbt-dbti:PutFeature
	a owl:Class ;
	rdfs:subClassOf
		fibo-sec-dbt-dbti:TradableDebtInstrumentRedemptionProvision ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-rel-rel:comprises ;
			owl:someValuesFrom fibo-sec-dbt-dbti:PutNotificationProvision ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-rel-rel:comprises ;
			owl:someValuesFrom fibo-sec-dbt-dbti:PutSchedule ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-rel-rel:comprises ;
			owl:someValuesFrom fibo-sec-dbt-dbti:PutWindow ;
		]
		;
	rdfs:label "put feature" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/DebtInstruments/> ;
	skos:definition "a redemption provision giving the holder the right, but not the obligation, to sell a specified amount of the debt instrument (i.e., redeem it), prior to maturity" ;
	skos:editorialNote 'FIBIM has term "Putable Date" which (by implication, and comparing with definition for "Next Call Date") is presumably a single calendar date in the future, at a given point in time. That does not cover the definition of formal terms defining when and how the issue may be put, which is what is modeled here.' ;
	fibo-fnd-utl-av:adaptedFrom "Barron's Dictionary of Business and Economics Terms, Fifth Edition, 2012" ;
	fibo-fnd-utl-av:synonym "put provision" ;
	.

fibo-sec-dbt-dbti:PutNotificationProvision
	a owl:Class ;
	rdfs:subClassOf
		fibo-sec-dbt-dbti:TradableDebtInstrumentRedemptionProvision ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-dt-fd:hasDatePeriod ;
			owl:allValuesFrom fibo-sec-dbt-dbti:PutWindow ;
		]
		;
	rdfs:label "put notification provision" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/DebtInstruments/> ;
	skos:definition "a provision of a put feature defining notification requirements" ;
	.

fibo-sec-dbt-dbti:PutPremium
	a owl:Class ;
	rdfs:subClassOf fibo-fnd-acc-cur:MonetaryPrice ;
	rdfs:label "put premium" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/DebtInstruments/> ;
	skos:definition "an amount over par that a debt instrument holder must pay to sell the security early" ;
	fibo-fnd-utl-av:adaptedFrom
		"Barron's Dictionary of Business and Economics Terms, Fifth Edition, 2012" ,
		"http://www.investopedia.com/terms/p/put.asp"
		;
	.

fibo-sec-dbt-dbti:PutPrice
	a owl:Class ;
	rdfs:subClassOf fibo-fnd-acc-cur:MonetaryPrice ;
	rdfs:label "put price" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/DebtInstruments/> ;
	skos:definition "a price at which a debt instrument with a put feature may be sold by the holder" ;
	skos:editorialNote "needs review - should the expression be exercise value vs. par value?" ;
	fibo-ind-ei-ei:actualExpression "par value + put premium" ;
	.

fibo-sec-dbt-dbti:PutSchedule
	a owl:Class ;
	rdfs:subClassOf
		fibo-fnd-dt-fd:Schedule ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-rel-rel:comprises ;
			owl:someValuesFrom fibo-sec-dbt-dbti:PutEvent ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-rel-rel:comprises ;
			owl:someValuesFrom [
				a owl:Class ;
				owl:unionOf (
					fibo-sec-dbt-dbti:PutPremium
					fibo-sec-dbt-dbti:PutPrice
				) ;
			] ;
		]
		;
	rdfs:label "put schedule" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/DebtInstruments/> ;
	skos:definition "a schedule that defines the events associated with the put feature of a debt instrument, i.e, the dates on which the debt instrument may be sold at what price by the holder" ;
	fibo-fnd-utl-av:adaptedFrom
		"Barron's Dictionary of Business and Economics Terms, Fifth Edition, 2012" ,
		"http://www.investopedia.com/terms/p/put.asp"
		;
	.

fibo-sec-dbt-dbti:PutWindow
	a owl:Class ;
	rdfs:subClassOf fibo-fnd-dt-fd:DatePeriod ;
	rdfs:label "put window" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/DebtInstruments/> ;
	skos:definition "an explicit period of time prior to a put date during which holder or agent must give notice to an issuer" ;
	.

fibo-sec-dbt-dbti:RelativePrice
	a owl:Class ;
	rdfs:subClassOf fibo-fnd-acc-cur:MonetaryPrice ;
	rdfs:label "relative price" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/DebtInstruments/> ;
	skos:definition "a relative price with respect to either a stated or market value for a debt instrument at some point in time, defined as par, premium, or discount" ;
	fibo-fnd-utl-av:adaptedFrom
		"http://www.investopedia.com/terms/m/market-price.asp" ,
		"http://www.investopedia.com/terms/p/par.asp"
		;
	.

fibo-sec-dbt-dbti:TradableDebtInstrument
	a owl:Class ;
	rdfs:subClassOf
		fibo-fbc-fi-fi:DebtInstrument ,
		fibo-fbc-fi-fi:Security ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-sec-dbt-dbti:mayBeSubordinatedTo ;
			owl:onClass fibo-sec-dbt-dbti:TradableDebtInstrument ;
			owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fbc-dae-gty:hasGuarantor ;
			owl:someValuesFrom fibo-fbc-dae-gty:Guarantor ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fbc-pas-fpas:hasOffering ;
			owl:someValuesFrom fibo-sec-dbt-dbti:DebtOffering ;
		]
		;
	rdfs:label "tradable debt instrument"@en ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/DebtInstruments/> ;
	skos:definition "a debt instrument that is also a security, i.e., that can be bought and sold by the holder" ;
	fibo-fnd-utl-av:explanatoryNote "Details from Ecofin: A [debt] instrument can be traded, if its features depend only on one borrower. If the instrument has no bilateral or multilateral obligations, the investor can easily transfer it to another investor without asking the borrower (except the terms prohibit this explicitly). This is simplified with securitised instruments, where the debt is already split into handy denominations which trade easily (e.g. in round thousands or millions as with bonds, commercial paper, etc.). But in principle it works also with interbank loans and similar instruments. FIBIM Definition: Financial instruments evidencing moneys owed by the issuer to the holder on terms as specified."@en ;
	.

fibo-sec-dbt-dbti:TradableDebtInstrumentRedemptionProvision
	a owl:Class ;
	rdfs:subClassOf fibo-fbc-dae-dbt:RedemptionProvision ;
	rdfs:label "tradable debt instrument redemption provision" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/SEC/Debt/DebtInstruments/> ;
	skos:definition "terms for the redemption of the debt represented by a debt security" ;
	fibo-fnd-utl-av:explanatoryNote "Debt represented by a tradable debt security may be paid as a one off payment at maturity, by paying down principal amounts periodically, or by a call of the issue by the issuer. Debt that is defined by a convertible bond are redeemed by conversion into an equity security issued by the same company. More notes from the EDMC SME Review: Distinction between call and amortizing is whether you pay off everybody at the same time or via a lottery. SF can be either. either amortize the payment of a SF or pay it off by a lottery. The terms of the instrument determine that. Mandatory - all in the schedule. Mandatory to the bond issuer. Issuer has to pay off a certain amount at a certain time - this is in the Schedule. Callable - nothing mandatory." ;
	.

