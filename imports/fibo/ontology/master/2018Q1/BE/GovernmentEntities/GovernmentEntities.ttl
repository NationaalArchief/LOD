fibo-be-ge-ge:BranchOfGovernment
	a owl:Class ;
	rdfs:subClassOf fibo-be-ge-ge:GovernmentBody ;
	rdfs:label "branch of government" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/BE/GovernmentEntities/GovernmentEntities/> ;
	skos:definition "a division of the government of a state, with separate and independent powers and areas of responsibility so that the powers of one branch are not in conflict with the powers associated with the other branches" ;
	fibo-fnd-utl-av:adaptedFrom
		"https://en.wikipedia.org/wiki/Separation_of_powers"^^xsd:anyURI ,
		"https://www.usa.gov/branches-of-government"^^xsd:anyURI
		;
	.

fibo-be-ge-ge:ExecutiveBranch
	a owl:Class ;
	rdfs:subClassOf fibo-be-ge-ge:BranchOfGovernment ;
	rdfs:label "executive branch" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/BE/GovernmentEntities/GovernmentEntities/> ;
	skos:definition "the branch of government that has its authority and responsibility for the daily administration of the state" ;
	fibo-fnd-utl-av:adaptedFrom
		"https://en.wikipedia.org/wiki/Executive_(government)"^^xsd:anyURI ,
		"https://www.usa.gov/branches-of-government"^^xsd:anyURI
		;
	fibo-fnd-utl-av:explanatoryNote "The executive branch executes, or enforces the law." ;
	.

fibo-be-ge-ge:FederalGovernment
	a owl:Class ;
	rdfs:subClassOf
		fibo-be-ge-ge:NationalGovernment ,
		[
			a owl:Class ;
			owl:intersectionOf (
				[
					a owl:Restriction ;
					owl:onProperty fibo-fnd-rel-rel:governs ;
					owl:onClass fibo-fnd-plc-cty:FederalCapitalArea ;
					owl:minQualifiedCardinality "0"^^xsd:nonNegativeInteger ;
				]
				[
					a owl:Restriction ;
					owl:onProperty fibo-fnd-rel-rel:governs ;
					owl:onClass fibo-fnd-plc-cty:FederalState ;
					owl:minQualifiedCardinality "2"^^xsd:nonNegativeInteger ;
				]
			) ;
		]
		;
	rdfs:label "federal government" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/BE/GovernmentEntities/GovernmentEntities/> ;
	skos:definition "a union of states under a central government distinct from the individual governments of the separate states" ;
	fibo-fnd-utl-av:adaptedFrom "https://en.wikipedia.org/wiki/Federation"^^xsd:anyURI ;
	fibo-fnd-utl-av:explanatoryNote "A federation is a political entity characterized by a union of partially self-governing states or regions under a central (federal) government. In a federation, the self-governing status of the component states, as well as the division of power between them and the central government, are typically constitutionally entrenched and may not be altered by a unilateral decision of either party, the states or the federal political body." ;
	.

fibo-be-ge-ge:FederatedSovereignty
	a owl:Class ;
	rdfs:subClassOf
		fibo-be-ge-ge:SovereignState ,
		[
			a owl:Class ;
			owl:intersectionOf (
				[
					a owl:Restriction ;
					owl:onProperty fibo-be-ge-ge:isRepresentedBy ;
					owl:onClass fibo-be-ge-ge:RegionalGovernment ;
					owl:minQualifiedCardinality "2"^^xsd:nonNegativeInteger ;
				]
				[
					a owl:Restriction ;
					owl:onProperty fibo-be-ge-ge:isRepresentedBy ;
					owl:onClass fibo-be-ge-ge:FederalGovernment ;
					owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
				]
			) ;
		] ,
		[
			a owl:Class ;
			owl:intersectionOf (
				[
					a owl:Restriction ;
					owl:onProperty fibo-be-ge-ge:hasFullSovereigntyOver ;
					owl:someValuesFrom fibo-fnd-plc-cty:Country ;
				]
				[
					a owl:Restriction ;
					owl:onProperty fibo-be-ge-ge:hasSharedSovereigntyOver ;
					owl:onClass fibo-fnd-plc-cty:FederalState ;
					owl:minQualifiedCardinality "2"^^xsd:nonNegativeInteger ;
				]
			) ;
		]
		;
	rdfs:label "federated sovereignty" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/BE/GovernmentEntities/GovernmentEntities/> ;
	skos:definition "a political entity characterized by a union of partially self-governing states or regions under a central (federal) government" ;
	fibo-fnd-utl-av:adaptedFrom "https://en.wikipedia.org/wiki/Federation"^^xsd:anyURI ;
	fibo-fnd-utl-av:explanatoryNote "Federal entity in the sense of a legal entity, that is, what it is that incurs debt for a federal government, i.e. the Federal entity in the sense of the legal entity, as distinct from the Federal government. It is identified as a legal entity because it is able to incur debt in its own right." ;
	.

fibo-be-ge-ge:Government
	a owl:Class ;
	rdfs:subClassOf
		fibo-be-fct-fct:FunctionalEntity ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-rel-rel:governs ;
			owl:someValuesFrom fibo-fnd-plc-cty:GeopoliticalEntity ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-rel-rel:hasPart ;
			owl:someValuesFrom fibo-be-ge-ge:BranchOfGovernment ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-rel-rel:hasPart ;
			owl:someValuesFrom fibo-be-ge-ge:GovernmentAgency ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-rel-rel:hasPart ;
			owl:someValuesFrom fibo-be-ge-ge:GovernmentDepartment ;
		]
		;
	rdfs:label "government" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/BE/GovernmentEntities/GovernmentEntities/> ;
	owl:disjointWith fibo-be-ge-ge:Instrumentality ;
	skos:definition "the system by which a state or community is controlled" ;
	fibo-fnd-utl-av:explanatoryNote "In the Commonwealth of Nations, the word government is also used more narrowly to refer to the collective group of people that exercises executive authority in a state. This usage is analogous to what is called an 'administration' in American English. Furthermore, especially in American English, the concepts of the state and the government may be used synonymously to refer to the person or group of people exercising authority over a politically organized territory." ;
	.

fibo-be-ge-ge:GovernmentAgency
	a owl:Class ;
	rdfs:subClassOf
		fibo-be-ge-ge:GovernmentBody ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-rel-rel:isManagedBy ;
			owl:someValuesFrom fibo-be-ge-ge:GovernmentAppointee ;
		]
		;
	rdfs:label "government agency" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/BE/GovernmentEntities/GovernmentEntities/> ;
	owl:disjointWith fibo-be-ge-ge:GovernmentDepartment ;
	skos:definition "a permanent or semi-permanent organization, often an appointed commission, in the machinery of government that is responsible for the oversight and administration of specific functions" ;
	fibo-fnd-utl-av:adaptedFrom "https://en.wikipedia.org/wiki/Government_agency"^^xsd:anyURI ;
	fibo-fnd-utl-av:explanatoryNote "There is a notable variety of agency types. Although usage differs, a government agency is normally distinct both from a department or ministry, and other types of public body established by government. The functions of an agency are normally executive in character, since different types of organizations (such as commissions) are most often constituted in an advisory role; this distinction is often blurred in practice however." ;
	.

fibo-be-ge-ge:GovernmentAppointee
	a owl:Class ;
	rdfs:subClassOf
		fibo-be-ge-ge:GovernmentOfficial ,
		fibo-be-oac-exec:Executive ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-pty-rl:isPlayedBy ;
			owl:someValuesFrom [
				a owl:Restriction ;
				owl:onProperty fibo-fnd-rel-rel:isAppointedBy ;
				owl:someValuesFrom fibo-be-ge-ge:GovernmentBody ;
			] ;
		]
		;
	rdfs:label "government appointee" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/BE/GovernmentEntities/GovernmentEntities/> ;
	skos:definition "an individual appointed by government decree to lead, or participate in some capacity in a government body" ;
	.

fibo-be-ge-ge:GovernmentBody
	a owl:Class ;
	rdfs:subClassOf
		fibo-fnd-org-fm:FormalOrganization ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-be-le-lp:hasObjective ;
			owl:someValuesFrom fibo-be-le-lp:PublicPurpose ;
		]
		;
	rdfs:label "government body" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/BE/GovernmentEntities/GovernmentEntities/> ;
	skos:definition "a formal organization that is an agency, instrumentality, or other body of supranational, national, federal, state, or local government, including certain multijurisdictional agencies and departments that carry out the business of government" ;
	fibo-fnd-utl-av:adaptedFrom "http://definitions.uslegal.com/g/government-entity/"^^xsd:anyURI ;
	fibo-fnd-utl-av:explanatoryNote "Identifying government bodies is a pre-requisite for identifying government officials.  This information is needed to help ensure compliance with applicable laws relating to bribery or corruption, including the U.S. Foreign Corrupt Practices Act (FCPA), the UK Bribery Act 2010 (UKBA), the U.S. Bank Bribery Act (Bribery Act), and other anti-bribery and corruption laws in the jurisdictions where financial institutions conduct business." ;
	.

fibo-be-ge-ge:GovernmentDepartment
	a owl:Class ;
	rdfs:subClassOf
		fibo-be-ge-ge:GovernmentBody ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-rel-rel:isManagedBy ;
			owl:someValuesFrom fibo-be-ge-ge:GovernmentMinister ;
		]
		;
	rdfs:label "government department" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/BE/GovernmentEntities/GovernmentEntities/> ;
	skos:definition "a specialized organization responsible for a sector of government public administration" ;
	fibo-fnd-utl-av:adaptedFrom "https://en.wikipedia.org/wiki/Ministry"^^xsd:anyURI ;
	.

fibo-be-ge-ge:GovernmentMinister
	a owl:Class ;
	rdfs:subClassOf
		fibo-be-ge-ge:GovernmentOfficial ,
		fibo-be-oac-exec:Executive ,
		[
			a owl:Class ;
			owl:unionOf (
				[
					a owl:Restriction ;
					owl:onProperty fibo-fnd-pty-rl:isPlayedBy ;
					owl:someValuesFrom [
						a owl:Restriction ;
						owl:onProperty fibo-be-ge-ge:isElectedBy ;
						owl:allValuesFrom fibo-be-ge-ge:Polity ;
					] ;
				]
				[
					a owl:Restriction ;
					owl:onProperty fibo-fnd-pty-rl:isPlayedBy ;
					owl:someValuesFrom [
						a owl:Restriction ;
						owl:onProperty fibo-fnd-rel-rel:isAppointedBy ;
						owl:allValuesFrom fibo-be-ge-ge:ExecutiveBranch ;
					] ;
				]
			) ;
		]
		;
	rdfs:label "government minister" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/BE/GovernmentEntities/GovernmentEntities/> ;
	skos:definition "a person appointed or elected to a high office in the government" ;
	skos:example "Minister of Finance, Secretary of State, Attorney General of California" ;
	fibo-fnd-utl-av:adaptedFrom "http://www.thefreedictionary.com/government+minister"^^xsd:anyURI ;
	.

fibo-be-ge-ge:GovernmentOfficial
	a owl:Class ;
	rdfs:subClassOf
		fibo-be-oac-exec:ResponsibleParty ,
		fibo-fnd-pty-pty:OrganizationMember ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-rel-rel:hasIdentity ;
			owl:onClass fibo-fnd-aap-ppl:LegallyCapableAdult ;
			owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-pty-rl:isPlayedBy ;
			owl:someValuesFrom [
				a owl:Restriction ;
				owl:onProperty fibo-fnd-rel-rel:isMemberOf ;
				owl:allValuesFrom fibo-be-ge-ge:GovernmentBody ;
			] ;
		]
		;
	rdfs:label "government official" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/BE/GovernmentEntities/GovernmentEntities/> ;
	skos:definition "a person elected or appointed to administer some aspect of a government" ;
	fibo-fnd-utl-av:adaptedFrom "http://www.thefreedictionary.com/government+officials"^^xsd:anyURI ;
	.

fibo-be-ge-ge:Instrumentality
	a owl:Class ;
	rdfs:subClassOf
		fibo-be-ge-ge:GovernmentBody ,
		fibo-be-le-lp:JuridicalPerson ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-be-ge-ge:isInstrumentOf ;
			owl:someValuesFrom fibo-be-ge-ge:Government ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-rel-rel:isMandatedBy ;
			owl:someValuesFrom fibo-fnd-law-jur:StatuteLaw ;
		]
		;
	rdfs:label "instrumentality" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/BE/GovernmentEntities/GovernmentEntities/> ;
	skos:definition "an organization that serves a public purpose and is closely tied to a government, but is not a government agency" ;
	fibo-fnd-utl-av:adaptedFrom "http://www.investopedia.com/terms/i/instrumentality.asp"^^xsd:anyURI ;
	fibo-fnd-utl-av:explanatoryNote "Many instrumentalities are private companies, and some are chartered directly by state or federal government. Instrumentalities are subject to a unique set of laws that shape their activities." ;
	.

fibo-be-ge-ge:Judiciary
	a owl:Class ;
	rdfs:subClassOf
		fibo-be-ge-ge:BranchOfGovernment ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-rel-rel:hasPart ;
			owl:someValuesFrom fibo-fnd-law-cor:CourtOfLaw ;
		]
		;
	rdfs:label "judiciary" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/BE/GovernmentEntities/GovernmentEntities/> ;
	skos:definition "a branch of government that comprises the system of courts that interprets and applies the law in the name of the supranational, national, federal, or regional government, depending on its jurisdiction" ;
	fibo-fnd-utl-av:adaptedFrom "https://en.wikipedia.org/wiki/Judiciary"^^xsd:anyURI ;
	fibo-fnd-utl-av:explanatoryNote "The judiciary also provides a mechanism for the resolution of disputes. Under the doctrine of the separation of powers, the judiciary generally does not make law (that is, in a plenary fashion, which is the responsibility of the legislature) or enforce law (which is the responsibility of the executive), but rather interprets law and applies it to the facts of each case." ;
	.

fibo-be-ge-ge:Legislature
	a owl:Class ;
	rdfs:subClassOf fibo-be-ge-ge:BranchOfGovernment ;
	rdfs:label "legislature" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/BE/GovernmentEntities/GovernmentEntities/> ;
	skos:definition "the law-making body of a political unit, usually a national government, that has power to enact, amend, and repeal public policy" ;
	fibo-fnd-utl-av:adaptedFrom "https://en.wikipedia.org/wiki/Legislature"^^xsd:anyURI ;
	fibo-fnd-utl-av:explanatoryNote "Laws enacted by legislatures are known as legislation. Legislatures observe and steer governing actions and usually have exclusive authority to amend the budget or budgets involved in the process." ;
	.

fibo-be-ge-ge:MunicipalEntity
	a owl:Class ;
	rdfs:subClassOf
		fibo-be-ge-ge:Polity ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-be-ge-ge:hasSovereigntyOver ;
			owl:someValuesFrom fibo-fnd-plc-cty:Municipality ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-be-ge-ge:isRepresentedBy ;
			owl:someValuesFrom fibo-be-ge-ge:MunicipalGovernment ;
		]
		;
	rdfs:label "municipal entity" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/BE/GovernmentEntities/GovernmentEntities/> ;
	skos:definition "a polity that typically represents a city, township, or other administrative subdivision having corporate status and powers of self-government or jurisdiction" ;
	fibo-fnd-utl-av:adaptedFrom "https://en.wikipedia.org/wiki/Municipality"^^xsd:anyURI ;
	fibo-fnd-utl-av:explanatoryNote "Municipal entity in the sense of a legal entity, that is, what it is that incurs debt for a municipality, as distinct from the Municipal government. A municipal entity has a Government which sets laws applicable within the geographical area corresponding to its jurisdiction." ;
	fibo-fnd-utl-av:synonym "municipality" ;
	.

fibo-be-ge-ge:MunicipalGovernment
	a owl:Class ;
	rdfs:subClassOf
		fibo-be-ge-ge:Government ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-rel-rel:governs ;
			owl:someValuesFrom fibo-fnd-plc-cty:Municipality ;
		]
		;
	rdfs:label "municipal government" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/BE/GovernmentEntities/GovernmentEntities/> ;
	skos:definition "the government of a municipality" ;
	fibo-fnd-utl-av:adaptedFrom "http://www.thefreedictionary.com/municipal+government"^^xsd:anyURI ;
	.

fibo-be-ge-ge:NationalGovernment
	a owl:Class ;
	rdfs:subClassOf
		fibo-be-ge-ge:Government ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-rel-rel:governs ;
			owl:someValuesFrom fibo-fnd-plc-cty:Country ;
		]
		;
	rdfs:label "national government" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/BE/GovernmentEntities/GovernmentEntities/> ;
	skos:definition "a government and the politicians that that run a country as a whole (as opposed to local government)" ;
	fibo-fnd-utl-av:adaptedFrom
		"http://dictionary.reference.com/browse/national+government"^^xsd:anyURI ,
		"http://www.collinsdictionary.com/dictionary/english/national-government"^^xsd:anyURI
		;
	.

fibo-be-ge-ge:Polity
	a owl:Class ;
	rdfs:subClassOf
		fibo-be-le-lp:LegalPerson ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-be-ge-ge:hasSovereigntyOver ;
			owl:someValuesFrom fibo-fnd-plc-cty:GeopoliticalEntity ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-be-ge-ge:isRepresentedBy ;
			owl:someValuesFrom fibo-be-ge-ge:Government ;
		]
		;
	rdfs:label "polity" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/BE/GovernmentEntities/GovernmentEntities/> ;
	skos:definition "a legal person that is a supranational entity, crown, state, or subordinate civil authority, such as a province, prefecture, county, municipality, city, or district representing the people of that entity" ;
	fibo-fnd-utl-av:adaptedFrom "https://en.wikipedia.org/wiki/Polity"^^xsd:anyURI ;
	.

fibo-be-ge-ge:RegionalGovernment
	a owl:Class ;
	rdfs:subClassOf
		fibo-be-ge-ge:Government ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-rel-rel:governs ;
			owl:someValuesFrom fibo-fnd-plc-cty:GeopoliticalEntity ;
		]
		;
	rdfs:label "regional government" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/BE/GovernmentEntities/GovernmentEntities/> ;
	skos:definition "an administrative body for a small geographic area, such as a county, smaller town, or other similar community" ;
	fibo-fnd-utl-av:adaptedFrom "http://www.businessdictionary.com/definition/local-government.html"^^xsd:anyURI ;
	fibo-fnd-utl-av:explanatoryNote "A local government will typically only have control over their specific geographical region, and cannot pass or enforce laws that will affect a wider area. Local governments can elect officials, enact taxes, and do many other things that a national government would do, just on a smaller scale." ;
	fibo-fnd-utl-av:synonym "local government" ;
	.

fibo-be-ge-ge:RegionalSovereignty
	a owl:Class ;
	rdfs:subClassOf
		fibo-be-ge-ge:Polity ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-be-ge-ge:isRepresentedBy ;
			owl:onClass fibo-be-ge-ge:RegionalGovernment ;
			owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-be-ge-ge:hasSharedSovereigntyOver ;
			owl:someValuesFrom fibo-fnd-plc-cty:GeopoliticalEntity ;
		]
		;
	rdfs:label "regional sovereignty" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/BE/GovernmentEntities/GovernmentEntities/> ;
	skos:definition "the legal person that corresponds to an administrative division, administrative unit, administrative entity or country subdivision (or, sometimes, geopolitical division or subnational entity), that has the capacity to incur debt, issue contracts, and enter into relations with other similar entities" ;
	skos:example "A country may be divided into provinces, which, in turn, are divided into counties, which, in turn, may be divided in whole or in part into municipalities; and so on." ;
	fibo-fnd-utl-av:adaptedFrom "http://en.wikipedia.org/wiki/Administrative_division"^^xsd:anyURI ;
	.

fibo-be-ge-ge:SovereignState
	a owl:Class ;
	rdfs:subClassOf
		fibo-be-ge-ge:Polity ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-be-ge-ge:hasFullSovereigntyOver ;
			owl:someValuesFrom fibo-fnd-plc-cty:Country ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-be-ge-ge:isRepresentedBy ;
			owl:someValuesFrom fibo-be-ge-ge:NationalGovernment ;
		]
		;
	rdfs:label "sovereign state" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/BE/GovernmentEntities/GovernmentEntities/> ;
	skos:definition "a nonphysical juridical entity that is represented by one centralized government that has sovereignty over a geographic area" ;
	fibo-fnd-utl-av:adaptedFrom "http://www.businessdictionary.com/definition/sovereign-authority.html"^^xsd:anyURI ;
	fibo-fnd-utl-av:explanatoryNote "A legal entity that is represented by one centralized government, has a permanent population, defined territory, and the capacity to enter into relations with other sovereign states." ;
	.

fibo-be-ge-ge:SupranationalEntity
	a owl:Class ;
	rdfs:subClassOf
		fibo-be-ge-ge:Polity ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-be-ge-ge:hasSharedSovereigntyOver ;
			owl:onClass fibo-fnd-plc-cty:Country ;
			owl:minQualifiedCardinality "2"^^xsd:nonNegativeInteger ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-be-ge-ge:isRepresentedBy ;
			owl:someValuesFrom fibo-be-ge-ge:GovernmentBody ;
		]
		;
	rdfs:label "supranational entity" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/BE/GovernmentEntities/GovernmentEntities/> ;
	skos:definition "a type of multinational political union where negotiated power is delegated to an authority by governments of member states" ;
	fibo-fnd-utl-av:adaptedFrom "Kiljunen, Kimmo (2004). The European Constitution in the Making. Centre for European Policy Studies. pp. 21-26. ISBN 978-92-9079-493-6" ;
	fibo-fnd-utl-av:explanatoryNote "A supranational union is a supranational polity which lies somewhere between a confederation that is an association of States and a federation that is a state.  Unlike states in a federal super-state, member states retain ultimate sovereignty, although some sovereignty is shared with, or ceded to, the supranational body." ;
	.

fibo-be-ge-ge:TribalArea
	a owl:Class ;
	rdfs:subClassOf fibo-fnd-plc-cty:GeopoliticalEntity ;
	rdfs:label "tribal area" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/BE/GovernmentEntities/GovernmentEntities/> ;
	skos:definition "a designation for an area of land managed by a group of indigenous people (tribe) rather than by the sovereign state or regional governmental entity in which the tribal area is geographically located" ;
	fibo-fnd-utl-av:adaptedFrom "https://en.wikipedia.org/wiki/Indian_reservation"^^xsd:anyURI ;
	.

fibo-be-ge-ge:TribalEntity
	a owl:Class ;
	rdfs:subClassOf
		fibo-be-ge-ge:Polity ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-be-ge-ge:hasPartialSovereigntyOver ;
			owl:someValuesFrom fibo-be-ge-ge:TribalArea ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-be-ge-ge:isRepresentedBy ;
			owl:someValuesFrom fibo-be-ge-ge:TribalGovernment ;
		]
		;
	rdfs:label "tribal entity" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/BE/GovernmentEntities/GovernmentEntities/> ;
	skos:definition "a legal entity that represents fundamental unit of sovereign tribal (indigenous) government" ;
	fibo-fnd-utl-av:adaptedFrom "https://en.wikipedia.org/wiki/List_of_federally_recognized_tribes_by_state"^^xsd:anyURI ;
	fibo-fnd-utl-av:explanatoryNote "Any indigenous group or community which is recognized as having rights and obligations independent of the central government." ;
	.

fibo-be-ge-ge:TribalGovernment
	a owl:Class ;
	rdfs:subClassOf
		fibo-be-ge-ge:Government ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-rel-rel:governs ;
			owl:someValuesFrom fibo-be-ge-ge:TribalArea ;
		]
		;
	rdfs:label "tribal government" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/BE/GovernmentEntities/GovernmentEntities/> ;
	skos:definition "a government representing a group of indigenous people that has legal authority to govern those people, including authority to legislate the existance of tribal entities" ;
	fibo-fnd-utl-av:adaptedFrom "http://www.businessdictionary.com/definition/local-government.html"^^xsd:anyURI ;
	.

fibo-be-ge-ge:hasFullSovereigntyOver
	a owl:ObjectProperty ;
	rdfs:subPropertyOf fibo-be-ge-ge:hasSovereigntyOver ;
	rdfs:label "has full sovereignty over" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/BE/GovernmentEntities/GovernmentEntities/> ;
	skos:definition "relates a polity to a geopolitical entity where the polity has absolute, supreme and ultimate dominion and authority of a political state subject to no higher power, expressed within its territory in full self-government and in complete freedom from any outside influence" ;
	.

fibo-be-ge-ge:hasJurisdiction
	a owl:ObjectProperty ;
	rdfs:subPropertyOf fibo-fnd-rel-rel:governs ;
	rdfs:label "has jurisdiction" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/BE/GovernmentEntities/GovernmentEntities/> ;
	rdfs:range fibo-fnd-law-jur:Jurisdiction ;
	skos:definition "relates a polity or government entity to one or more jurisdictions, over which it has some level of legal authority" ;
	.

fibo-be-ge-ge:hasPartialSovereigntyOver
	a owl:ObjectProperty ;
	rdfs:subPropertyOf fibo-be-ge-ge:hasSovereigntyOver ;
	rdfs:label "has partial sovereignty over" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/BE/GovernmentEntities/GovernmentEntities/> ;
	skos:definition "relates a polity to a geopolitical entity where the polity exercises partial dominion and authority of a political state" ;
	.

fibo-be-ge-ge:hasSharedSovereigntyOver
