		;
	owl:versionIRI <https://spec.edmcouncil.org/fibo/ontology/master/2018Q1/DER/RateDerivatives/IRSwaps/> ;
	fibo-fnd-utl-av:hasMaturityLevel fibo-fnd-utl-av:Release ;
	.

fibo-der-rtd-irswp:CrossCurrencyInterestRateSwap
	a owl:Class ;
	rdfs:subClassOf
		fibo-der-rtd-irswp:InterestRateSwap ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fbc-fct-ra:specifies ;
			owl:onClass fibo-fnd-acc-cur:Currency ;
			owl:qualifiedCardinality "2"^^xsd:nonNegativeInteger ;
		]
		;
	rdfs:label "cross-currency interest rate swap" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/DER/RateDerivatives/IRSwaps/> ;
	skos:definition "an interest rate swap in which the two streams of interest payments are in different currencies" ;
	.

fibo-der-rtd-irswp:ExplicitInterestAmountCalculationEvent
	a owl:Class ;
	rdfs:subClassOf
		fibo-der-rtd-irswp:InterestRateStreamEvent ,
		fibo-sec-sec-sch:ScheduledCalculationPeriodEndEvent
		;
	rdfs:label "explicit interest amount calculation event" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/DER/RateDerivatives/IRSwaps/> ;
	skos:definition "the explicit representation of the calculation event in a given period, in which an interest payment is calculated based on the rate (fixed or floating) and the notional amount (in the payment currency, and factored for Fx if necessary), on a given date" ;
	.

fibo-der-rtd-irswp:FixedFloatCrossCurrencyInterestRateSwap
	a owl:Class ;
	rdfs:subClassOf
		fibo-der-rtd-irswp:CrossCurrencyInterestRateSwap ,
		fibo-der-rtd-irswp:FixedFloatInterestRateSwap
		;
	rdfs:label "fixed float cross-currency interest rate swap" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/DER/RateDerivatives/IRSwaps/> ;
	owl:equivalentClass [
		a owl:Class ;
		owl:intersectionOf (
			fibo-der-rtd-irswp:CrossCurrencyInterestRateSwap
			fibo-der-rtd-irswp:FixedFloatInterestRateSwap
		) ;
	] ;
	skos:definition "an interest rate swap in which fixed interest payments on the notional are exchanged for floating interest payments and the two streams of interest payments are in different currencies" ;
	fibo-fnd-utl-av:synonym
		"fixed float cross currency interest rate swap" ,
		"fixed-float cross-currency interest rate swap"
		;
	.

fibo-der-rtd-irswp:FixedFloatInterestRateSwap
	a owl:Class ;
	rdfs:subClassOf
		fibo-der-rtd-irswp:InterestRateSwap ,
		[
			a owl:Class ;
			owl:intersectionOf (
				[
					a owl:Restriction ;
					owl:onProperty fibo-der-drc-swp:exchanges ;
					owl:someValuesFrom fibo-der-rtd-irswp:FixedInterestRateLeg ;
				]
				[
					a owl:Restriction ;
					owl:onProperty fibo-der-drc-swp:exchanges ;
					owl:someValuesFrom fibo-der-rtd-irswp:FloatingInterestRateLeg ;
				]
			) ;
		]
		;
	rdfs:label "fixed float interest rate swap" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/DER/RateDerivatives/IRSwaps/> ;
	owl:equivalentClass [
		a owl:Class ;
		owl:intersectionOf (
			fibo-der-rtd-irswp:InterestRateSwap
			[
				a owl:Restriction ;
				owl:onProperty fibo-der-drc-swp:hasLeg ;
				owl:someValuesFrom fibo-der-rtd-irswp:FixedInterestRateLeg ;
			]
			[
				a owl:Restriction ;
				owl:onProperty fibo-der-drc-swp:hasLeg ;
				owl:someValuesFrom fibo-der-rtd-irswp:FloatingInterestRateLeg ;
			]
		) ;
	] ;
	skos:definition "an interest rate swap in which fixed interest payments on the notional are exchanged for floating interest payments" ;
	fibo-fnd-utl-av:synonym "vanilla interest rate swap" ;
	.

fibo-der-rtd-irswp:FixedFloatSingleCurrencyInterestRateSwap
	a owl:Class ;
	rdfs:subClassOf fibo-der-rtd-irswp:FixedFloatInterestRateSwap ;
	rdfs:label "fixed float single currency interest rate swap" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/DER/RateDerivatives/IRSwaps/> ;
	owl:equivalentClass [
		a owl:Class ;
		owl:intersectionOf (
			fibo-der-rtd-irswp:FixedFloatInterestRateSwap
			fibo-der-rtd-irswp:SingleCurrencyInterestRateSwap
		) ;
	] ;
	skos:definition "an interest rate swap in which fixed interest payments on the notional are exchanged for floating interest payments and where both payment streams are expressed in terms of the same currency" ;
	.

fibo-der-rtd-irswp:FixedInterestRateLeg
	a owl:Class ;
	rdfs:subClassOf fibo-der-rtd-irswp:InterestRateSwapLeg ;
	rdfs:label "fixed interest rate leg" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/DER/RateDerivatives/IRSwaps/> ;
	owl:disjointWith fibo-der-rtd-irswp:FloatingInterestRateLeg ;
	owl:equivalentClass [
		a owl:Class ;
		owl:intersectionOf (
			fibo-der-rtd-irswp:InterestRateSwapLeg
			[
				a owl:Restriction ;
				owl:onProperty fibo-fbc-dae-dbt:hasInterestRate ;
				owl:someValuesFrom fibo-fbc-dae-dbt:FixedInterestRate ;
			]
		) ;
	] ;
	skos:definition "a swap stream that specifies fixed interest amounts and terms for the payment of that interest" ;
	skos:editorialNote "This may be the funding leg of some swaps (i.e. one party agrees to pay fixed interest amounts in exchange for whatever is the other leg) or it may be one or both sides of an Interest Rate Swap, where the two parties exchange different interest streams. Note that the FpML term on which this Swapstream term was based, is the one for asset return swaps, where this is the payment leg. This is the leg which has interest amounts, interest calculations and calculation dates, and optional stub calculation terms. It should be possible to relate this to the specific interest payments of an underlier if there is one." ;
	fibo-fnd-utl-av:synonym "fixed interest rate swap stream terms" ;
	.

fibo-der-rtd-irswp:FixedRateInterestCalculation
	a owl:Class ;
	rdfs:subClassOf
		fibo-fnd-utl-alx:Expression ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-utl-alx:hasArgument ;
			owl:someValuesFrom fibo-fbc-dae-dbt:FixedInterestRate ;
		]
		;
	rdfs:label "fixed rate interest calculation" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/DER/RateDerivatives/IRSwaps/> ;
	skos:definition "calculation of the amount of interest due in a given period on a fixed rate IR swap leg" ;
	.

fibo-der-rtd-irswp:FloatFloatCrossCurrencyInterestRateSwap
	a owl:Class ;
	rdfs:subClassOf fibo-der-rtd-irswp:FloatFloatInterestRateSwap ;
	rdfs:label "float float cross-currency interest rate swap" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/DER/RateDerivatives/IRSwaps/> ;
	owl:equivalentClass [
		a owl:Class ;
		owl:intersectionOf (
			fibo-der-rtd-irswp:CrossCurrencyInterestRateSwap
			fibo-der-rtd-irswp:FloatFloatInterestRateSwap
		) ;
	] ;
	skos:definition "an interest rate swap that exchanges cashflows based on two different interest rates in different currencies" ;
	.

fibo-der-rtd-irswp:FloatFloatInterestRateSwap
	a owl:Class ;
	rdfs:subClassOf
		fibo-der-drc-swp:BasisSwap ,
		fibo-der-rtd-irswp:InterestRateSwap ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-der-drc-swp:exchanges ;
			owl:onClass fibo-der-rtd-irswp:FloatingInterestRateLeg ;
			owl:qualifiedCardinality "2"^^xsd:nonNegativeInteger ;
		]
		;
	rdfs:label "float float interest rate swap" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/DER/RateDerivatives/IRSwaps/> ;
	owl:equivalentClass [
		a owl:Class ;
		owl:intersectionOf (
			fibo-der-rtd-irswp:InterestRateSwap
			[
				a owl:Restriction ;
				owl:onProperty fibo-der-drc-swp:hasLeg ;
				owl:onClass fibo-der-rtd-irswp:FloatingInterestRateLeg ;
				owl:qualifiedCardinality "2"^^xsd:nonNegativeInteger ;
			]
		) ;
	] ;
	skos:definition "an interest rate swap that exchanges cashflows based on two different floating interest rates" ;
	fibo-fnd-utl-av:adaptedFrom "http://www.investopedia.com/terms/b/basisrateswap.asp"^^xsd:anyURI ;
	fibo-fnd-utl-av:explanatoryNote "This is a swap in which two parties swap variable interest rates based on different money markets, and this is usually done to limit interest-rate risk that a company faces as a result of having differing lending and borrowing rates." ;
	fibo-fnd-utl-av:synonym "basis rate swap" ;
	.

fibo-der-rtd-irswp:FloatFloatSingleCurrencyInterestRateSwap
	a owl:Class ;
	rdfs:subClassOf fibo-der-rtd-irswp:FloatFloatInterestRateSwap ;
	rdfs:label "float float single currency interest rate swap" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/DER/RateDerivatives/IRSwaps/> ;
	owl:equivalentClass [
		a owl:Class ;
		owl:intersectionOf (
			fibo-der-rtd-irswp:FloatFloatInterestRateSwap
			fibo-der-rtd-irswp:SingleCurrencyInterestRateSwap
		) ;
	] ;
	skos:definition "an interest rate swap that exchanges cashflows based on two different floating interest rates in the same currency" ;
	.

fibo-der-rtd-irswp:FloatingInterestRateLeg
	a owl:Class ;
	rdfs:subClassOf
		fibo-der-rtd-irswp:InterestRateSwapLeg ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-der-rtd-irswp:hasInterestCalculationSchedule ;
			owl:someValuesFrom fibo-der-rtd-irswp:SwapStreamInterestCalculationSchedule ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-der-rtd-irswp:hasInterestRateResetSchedule ;
			owl:someValuesFrom fibo-der-rtd-irswp:SwapStreamInterestRateResetSchedule ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fbc-fct-ra:specifies ;
			owl:someValuesFrom fibo-der-rtd-irswp:SwapStreamInterestRateFixing ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fbc-fct-ra:specifies ;
			owl:someValuesFrom fibo-der-rtd-irswp:SwapStreamInterestRateReset ;
		]
		;
	rdfs:label "floating interest rate leg" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/DER/RateDerivatives/IRSwaps/> ;
	owl:equivalentClass [
		a owl:Class ;
		owl:intersectionOf (
			fibo-der-rtd-irswp:InterestRateSwapLeg
			[
				a owl:Restriction ;
				owl:onProperty fibo-fbc-dae-dbt:hasInterestRate ;
				owl:someValuesFrom fibo-fbc-dae-dbt:FloatingInterestRate ;
			]
		) ;
	] ;
	skos:definition "a swapstream in which variable interest is paid on some notional amount, linked to some underlying interest reference rate" ;
	skos:editorialNote "Instead of an absolute rate you have an underlier and an offset called a spread (same as margin in floating rate notes)." ;
	fibo-fnd-utl-av:synonym "floating interest rate swap stream terms" ;
	.

fibo-der-rtd-irswp:FloatingRateFixingValue
	a owl:Class ;
	rdfs:subClassOf fibo-fnd-utl-alx:Variable ;
	rdfs:label "floating rate fixing value" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/DER/RateDerivatives/IRSwaps/> ;
	skos:definition "value of floating interest rate amount during a given calculation period, along with the relevant parameters in its calculation" ;
	skos:editorialNote "Forms an explicit set of terms at a point in time, and is part of the explicit schedule information." ;
	.

fibo-der-rtd-irswp:FloatingRateInterestCalculation
	a owl:Class ;
	rdfs:subClassOf
		fibo-fnd-utl-alx:Expression ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-utl-alx:hasArgument ;
			owl:someValuesFrom fibo-der-rtd-irswp:FloatingRateFixingValue ;
		]
		;
	rdfs:label "floating rate interest calculation" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/DER/RateDerivatives/IRSwaps/> ;
	.

fibo-der-rtd-irswp:InterestRateStreamEvent
	a owl:Class ;
	rdfs:subClassOf fibo-der-drc-swp:SwapStreamEvent ;
	rdfs:label "interest rate stream event" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/DER/RateDerivatives/IRSwaps/> ;
	.

fibo-der-rtd-irswp:InterestRateSwap
	a owl:Class ;
	rdfs:subClassOf
		fibo-der-drc-swp:Swap ,
		fibo-der-rtd-rtd:InterestRateDerivativeInstrument ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-der-drc-swp:exchanges ;
			owl:onClass fibo-der-rtd-irswp:InterestRateSwapLeg ;
			owl:qualifiedCardinality "2"^^xsd:nonNegativeInteger ;
		]
		;
	rdfs:label "interest rate swap" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/DER/RateDerivatives/IRSwaps/> ;
	owl:equivalentClass [
		a owl:Class ;
		owl:intersectionOf (
			fibo-der-drc-swp:Swap
			[
				a owl:Restriction ;
				owl:onProperty fibo-der-drc-swp:hasLeg ;
				owl:onClass fibo-der-rtd-irswp:InterestRateSwapLeg ;
				owl:qualifiedCardinality "2"^^xsd:nonNegativeInteger ;
			]
		) ;
	] ;
	skos:definition "a swap in which the underlier for one or both legs is an interest rate" ;
	.

fibo-der-rtd-irswp:InterestRateSwapLeg
	a owl:Class ;
	rdfs:subClassOf
		fibo-der-drc-swp:SwapLeg ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fbc-fct-ra:specifies ;
			owl:onClass fibo-fnd-acc-cur:Currency ;
			owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-der-drc-swp:hasPaymentSchedule ;
			owl:someValuesFrom fibo-der-rtd-irswp:SwapStreamInterestPaymentSchedule ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-der-rtd-irswp:hasNotionalStepSchedule ;
			owl:someValuesFrom fibo-der-rtd-irswp:NotionalStepSchedule ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fbc-fct-ra:specifies ;
			owl:someValuesFrom fibo-der-rtd-irswp:InterestRateStreamEvent ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fbc-fct-ra:specifies ;
			owl:someValuesFrom fibo-der-rtd-irswp:NotionalStepChangeEvent ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fbc-fct-ra:specifies ;
			owl:someValuesFrom fibo-der-rtd-irswp:SwapStreamInterestCalculation ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fbc-fct-ra:specifies ;
			owl:someValuesFrom fibo-der-rtd-irswp:SwapStreamInterestPayment ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-dt-bd:hasBusinessRecurrenceIntervalConvention ;
			owl:someValuesFrom fibo-fbc-dae-dbt:DayCountConvention ;
		]
		;
	rdfs:label "interest rate swap leg" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/DER/RateDerivatives/IRSwaps/> ;
	owl:equivalentClass [
		a owl:Class ;
		owl:intersectionOf (
			fibo-der-drc-swp:SwapLeg
			[
				a owl:Restriction ;
				owl:onProperty fibo-fbc-dae-dbt:hasInterestRate ;
				owl:someValuesFrom fibo-fnd-acc-cur:InterestRate ;
			]
		) ;
	] ;
	skos:definition "the components specifiying an interest rate stream, including both a parametric and cashflow representation for the stream of payments" ;
	skos:editorialNote "In IR markets there is no real distinction between an IR leg and a return leg, rather there are 2 funding legs." ;
	.

fibo-der-rtd-irswp:NotionalStepAmount
	a owl:Class ;
	rdfs:subClassOf
		fibo-fnd-acc-cur:MonetaryAmount ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-acc-cur:hasNotionalAmount ;
			owl:someValuesFrom fibo-fnd-acc-cur:MonetaryAmount ;
		]
		;
	rdfs:label "notional step amount" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/DER/RateDerivatives/IRSwaps/> ;
	skos:definition "the amount of money which is removed from the notional on each step date" ;
	skos:editorialNote "Note that this is an actual concrete sum of money, which may be specified either as a monetary amount (e.g. dollars and cents) or as a percentage of either the original notional amount or the previous notional amount." ;
	.

fibo-der-rtd-irswp:NotionalStepChangeEvent
	a owl:Class ;
	rdfs:subClassOf
		fibo-der-rtd-irswp:InterestRateStreamEvent ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-dt-fd:hasDate ;
			owl:someValuesFrom fibo-fnd-dt-fd:CalculatedDate ;
		]
		;
	rdfs:label "notional step change event" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/DER/RateDerivatives/IRSwaps/> ;
	skos:definition "a step change in the notional amount of the swap stream" ;
	skos:editorialNote "This alwys occurs on a Calculation date (that is, one of the Calculation Period End dates). Therefore the frequency / period length of the steps in the step schedule is a multiple of the Calculation Period or frequency. For example, if the notional is recalculated on every Calculation Date, applying a new interest rate to the new notional amount, then the two frequencies are the same. If notional is updated every second calculation period, then the step schedule specifies periods that are twice as long, and so on." ;
	.

fibo-der-rtd-irswp:NotionalStepPercentageAmount
	a owl:Class ;
	rdfs:subClassOf
		fibo-fnd-acc-cur:MonetaryMeasure ,
		fibo-fnd-qt-qtu:Rate ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-acc-cur:hasRateValue ;
			owl:allValuesFrom xsd:decimal ;
		]
		;
	rdfs:label "notional step percentage amount" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/DER/RateDerivatives/IRSwaps/> ;
	skos:definition "the percentage amount by which the notional changes on each step date" ;
	skos:editorialNote "The percentage is either a percentage applied to the initial notional amount or the previous outstanding notional, and may be positive or negative." ;
	.

fibo-der-rtd-irswp:NotionalStepPeriodLength
	a owl:Class ;
	rdfs:subClassOf fibo-fnd-dt-fd:RecurrenceInterval ;
	rdfs:label "notional step period length" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/DER/RateDerivatives/IRSwaps/> ;
	skos:definition "a recurrence interval indicating the frequency with which step changes occur, which is a multiple of the calculation period in the calculation schedule" ;
	.

fibo-der-rtd-irswp:NotionalStepRelativeToSelection
	a rdfs:Datatype ;
	owl:equivalentClass [
		a rdfs:Datatype ;
		owl:oneOf (
			"Initial"
			"Previous"
		) ;
	] ;
	skos:definition "a specification of whether a percentage rate change, used to calculate a change in notional outstanding, is expressed as a percentage of the initial notional amount or of the previously outstanding notional amount" ;
	.

fibo-der-rtd-irswp:NotionalStepSchedule
	a owl:Class ;
	rdfs:subClassOf
		fibo-sec-sec-sch:ParametricSchedule ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fbc-fct-ra:specifies ;
			owl:someValuesFrom fibo-der-rtd-irswp:NotionalStepAmount ;
		] ,
		[
			a owl:Restriction ;
			owl:onProperty fibo-fnd-rel-rel:refersTo ;
			owl:someValuesFrom fibo-der-rtd-irswp:SwapStreamInterestCalculationSchedule ;
		]
		;
	rdfs:label "notional step schedule" ;
	rdfs:isDefinedBy <https://spec.edmcouncil.org/fibo/ontology/DER/RateDerivatives/IRSwaps/> ;
	skos:definition "schedule of changes in the notional amount on which interest is paid, comprising the regular sequence of step events" ;
	skos:editorialNote "For the Notional Step Schedule there are no stubs." ;
	.

fibo-der-rtd-irswp:SingleCurrencyInterestRateSwap
	a owl:Class ;
	rdfs:subClassOf
		fibo-der-rtd-irswp:InterestRateSwap ,
