 rdfs:domain nadef:HistorischeNaam ;
 rdfs:range xsd:string ;
 rdfs:comment "initials as part of the first name"@en ;
 rdfs:label "initialen"@nl ,
"initials"@en .


###  http://archief.overheid.nl/def/naamstoevoeging
nadef:naamstoevoeging rdf:type owl:DatatypeProperty ;
rdfs:domain nadef:HistorischeNaam ;
rdfs:comment "distinctive addition to last name"@en ,
