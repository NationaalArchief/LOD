###  http://archief.overheid.nl/def/ArchiefEntiteit
nadef:ArchiefEntiteit rdf:type owl:Class ;
                      owl:equivalentClass edm:NonInformationResource ;
                      rdfs:subClassOf edm:ProvidedCHO ;
                      owl:disjointWith nadef:ArchiefHulpbron .


###  http://archief.overheid.nl/def/ArchiefHulpbron
nadef:ArchiefHulpbron rdf:type owl:Class ;
                      owl:equivalentClass dbo:Reference ,
                                          dcterms:BibliographicResource ,
                                          edm:InformationResource ;
                      rdfs:label "ArchiefHulpbron"@nl .


###  http://archief.overheid.nl/def/GrafischRecord
nadef:GrafischRecord rdf:type owl:Class ;
                     rdfs:subClassOf nadef:ArchiefHulpbron ,
                                     ore:Aggregation .


###  http://archief.overheid.nl/def/PersoonEntiteit
nadef:PersoonEntiteit rdf:type owl:Class ;
                      owl:equivalentClass bgn:Person ,
                                          foaf:Person ;
                      rdfs:subClassOf nadef:ArchiefEntiteit ;
                      rdfs:label "Fysiek persoon in het domein van archieven."@nl .


