###  http://archief.overheid.nl/def/heeftNaam
naa:heeftNaam rdf:type owl:ObjectProperty ;
              rdfs:domain naa:ArchiefEntiteit ;
              rdfs:range naa:ArchiefNaam ,
                         naa:PersoonsNaam ;
              rdfs:label "has name"@en ,
                         "heeft naam"@nl ;
              skos:prefLabel "heeft naam"@nl .


###  http://archief.overheid.nl/def/heeftUitgevoerd
naa:heeftUitgevoerd rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf cidoc-crm:P11i_participated_in ;
                    rdfs:domain bgn:Person ;
                    rdfs:range cidoc-crm:E65_Creation .


###  http://archief.overheid.nl/def/isBeschrevenIn
naa:isBeschrevenIn rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf dcterms:isReferencedBy ;
                   rdfs:domain naa:ArchiefEntiteit ;
                   rdfs:range naa:ArchiefVerwijzing ;
                   rdfs:comment "Gebruik isBeschrevenIn om een entiteit te koppelen aan het archiefstuk waar het (of zij, of hij) beschreven staat in het archief."@nl .


###  http://archief.overheid.nl/def/isOpvolgerVan
naa:isOpvolgerVan rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf owl:topObjectProperty ;
                  owl:inverseOf naa:isVoorgangerVan ;
                  rdfs:domain foaf:Agent ;
                  rdfs:range foaf:Agent .


###  http://archief.overheid.nl/def/isRecordType
naa:isRecordType rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf dc:subject ;
                 rdfs:domain naa:DetailRecord ;
                 rdfs:range naa:DetailRecordType ;
                 rdfs:comment "Optioneel: specificeert dat een DetailRecord van een bepaald type is. Een record uit een NT kan een benoeming zijn, gebruik dan DetailRecord en maak een nieuw type DetailRecordType: TypeBenoeming. Dit voorkomt dat je voor elk \"bijzonder\" NT record een subclass moet maken."@nl .


###  http://archief.overheid.nl/def/isVoorgangerVan
naa:isVoorgangerVan rdf:type owl:ObjectProperty ;
                    rdfs:domain foaf:Agent ;
                    rdfs:range foaf:Agent ;
                    rdfs:comment "Geeft aan dat iemand (beschreven of echt persoon) voor gegaan is aan een ander iemand zonder context van de activiteit."@nl .


###  http://archief.overheid.nl/def/resultaatVanActiviteit
naa:resultaatVanActiviteit rdf:type owl:ObjectProperty ;
                           rdfs:subPropertyOf cidoc-crm:P94_has_created ;
                           rdfs:range naa:ArchiefEntiteit ;
                           rdfs:comment "Het resultaat van een activiteit, onderneming of gebeurtenis waarbij een persoon, organisatie of Agent een rol gespeeld heeft. Het resultaat kan fysiek zijn, maar dat hoeft niet. Een idee, lied of andere niet fysieke zaken kunnen ook het resultaat zijn."@nl ;
                           rdfs:label "resultaat van activiteit" .


###  http://id.loc.gov/ontologies/bibframe/identifiedBy
bf:identifiedBy rdf:type owl:ObjectProperty ;
                owl:inverseOf bf:identifies ;
                rdfs:range bf:Identifier .


###  http://id.loc.gov/ontologies/bibframe/identifies
bf:identifies rdf:type owl:ObjectProperty ;
              rdfs:domain bf:Identifier .


###  http://purl.org/dc/elements/1.1/subject
dc:subject rdf:type owl:ObjectProperty .


###  http://purl.org/dc/terms/isPartOf
dcterms:isPartOf rdf:type owl:ObjectProperty .


###  http://purl.org/dc/terms/isReferencedBy
dcterms:isReferencedBy rdf:type owl:ObjectProperty .


###  http://rdfs.org/ns/void#class
void:class rdf:type owl:ObjectProperty ,
                    owl:FunctionalProperty ;
           rdfs:domain void:Dataset ;
           rdfs:range rdfs:Class ;
           rdfs:comment "The rdfs:Class that is the rdf:type of all entities in a class-based partition." ;
           rdfs:label "class" .


###  http://rdfs.org/ns/void#classPartition
void:classPartition rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf void:subset ;
                    rdfs:domain void:Dataset ;
                    rdfs:range void:Dataset ;
                    rdfs:comment "A subset of a void:Dataset that contains only the entities of a certain rdfs:Class." ;
                    rdfs:label "class partition" .


###  http://rdfs.org/ns/void#dataDump
void:dataDump rdf:type owl:ObjectProperty ;
              rdfs:domain void:Dataset ;
              rdfs:range rdfs:Resource ;
              rdfs:comment "An RDF dump, partial or complete, of a void:Dataset." ;
              rdfs:label "Data Dump" .


###  http://rdfs.org/ns/void#exampleResource
void:exampleResource rdf:type owl:ObjectProperty ;
                     rdfs:domain void:Dataset ;
                     rdfs:range rdfs:Resource ;
                     rdfs:label "example resource of dataset" .


###  http://rdfs.org/ns/void#feature
void:feature rdf:type owl:ObjectProperty ;
             rdfs:domain void:Dataset ;
             rdfs:range void:TechnicalFeature ;
             rdfs:label "feature" .


###  http://rdfs.org/ns/void#inDataset
void:inDataset rdf:type owl:ObjectProperty ;
               rdfs:domain foaf:Document ;
               rdfs:range void:Dataset ;
               rdfs:comment "Points to the void:Dataset that a document is a part of." ;
               rdfs:label "in dataset" .


###  http://rdfs.org/ns/void#linkPredicate
void:linkPredicate rdf:type owl:ObjectProperty ;
                   rdfs:domain void:Linkset ;
                   rdfs:range rdf:Property ;
                   rdfs:label "a link predicate" .


###  http://rdfs.org/ns/void#objectsTarget
void:objectsTarget rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf void:target ;
                   rdf:type owl:FunctionalProperty ;
                   rdfs:domain void:Linkset ;
                   rdfs:range void:Dataset ;
                   rdfs:comment "The dataset describing the objects of the triples contained in the Linkset." ;
                   rdfs:label "Objects Target" .


###  http://rdfs.org/ns/void#openSearchDescription
void:openSearchDescription rdf:type owl:ObjectProperty ;
                           rdfs:domain void:Dataset ;
                           rdfs:range foaf:Document ;
                           rdfs:comment "An OpenSearch description document for a free-text search service over a void:Dataset." ;
                           rdfs:label "open search description" .


###  http://rdfs.org/ns/void#property
void:property rdf:type owl:ObjectProperty ,
                       owl:FunctionalProperty ;
              rdfs:domain void:Dataset ;
              rdfs:range rdf:Property ;
              rdfs:comment "The rdf:Property that is the predicate of all triples in a property-based partition." ;
              rdfs:label "property" .


###  http://rdfs.org/ns/void#propertyPartition
void:propertyPartition rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf void:subset ;
                       rdfs:domain void:Dataset ;
                       rdfs:range void:Dataset ;
                       rdfs:comment "A subset of a void:Dataset that contains only the triples of a certain rdf:Property." ;
                       rdfs:label "property partition" .


###  http://rdfs.org/ns/void#subjectsTarget
void:subjectsTarget rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf void:target ;
                    rdf:type owl:FunctionalProperty ;
                    rdfs:domain void:Linkset ;
                    rdfs:range void:Dataset ;
                    rdfs:comment "The dataset describing the subjects of triples contained in the Linkset." ;
                    rdfs:label "Subjects Target" .


###  http://rdfs.org/ns/void#subset
void:subset rdf:type owl:ObjectProperty ;
            rdfs:domain void:Dataset ;
            rdfs:range void:Dataset ;
            rdfs:label "has subset" .


###  http://rdfs.org/ns/void#target
void:target rdf:type owl:ObjectProperty ;
            rdfs:domain void:Linkset ;
            rdfs:range void:Dataset ;
            rdfs:comment "One of the two datasets linked by the Linkset." ;
            rdfs:label "Target" .


###  http://www.cidoc-crm.org/cidoc-crm/P11i_participated_in
cidoc-crm:P11i_participated_in rdf:type owl:ObjectProperty ;
                               rdfs:subPropertyOf cidoc-crm:P12i_was_present_at ;
                               rdfs:domain cidoc-crm:E39_Actor ;
                               rdfs:range cidoc-crm:E5_Event ;
                               rdfs:label "a participé à"@fr ,
                                          "nahm Teil an"@de ,
                                          "participa em"@pt ,
                                          "participated in"@en ,
                                          "συμμετείχε σε"@el ,
                                          "участвовал в"@ru ,
                                          "参与了"@zh .


###  http://www.cidoc-crm.org/cidoc-crm/P129_is_about
cidoc-crm:P129_is_about rdf:type owl:ObjectProperty .


###  http://www.cidoc-crm.org/cidoc-crm/P12_occurred_in_the_presence_of
cidoc-crm:P12_occurred_in_the_presence_of rdf:type owl:ObjectProperty .


###  http://www.cidoc-crm.org/cidoc-crm/P12i_was_present_at
cidoc-crm:P12i_was_present_at rdf:type owl:ObjectProperty ;
                              rdfs:domain cidoc-crm:E77_Persistent_Item ;
                              rdfs:label "estava presente no"@pt ,
                                         "war anwesend bei"@de ,
                                         "was present at"@en ,
                                         "était présent à"@fr ,
                                         "ήταν παρών/παρούσα/παρόν σε"@el ,
                                         "присутствовал при"@ru ,
                                         "当时在场於"@zh .


###  http://www.cidoc-crm.org/cidoc-crm/P92_brought_into_existence
cidoc-crm:P92_brought_into_existence rdf:type owl:ObjectProperty ;
                                     rdfs:subPropertyOf cidoc-crm:P12_occurred_in_the_presence_of ;
                                     rdfs:domain cidoc-crm:E63_Beginning_of_Existence ;
                                     rdfs:range cidoc-crm:E77_Persistent_Item ;
                                     rdfs:comment """This property allows an E63 Beginning of Existence event to be linked to the E77 Persistent Item brought into existence by it.
It allows a “start” to be attached to any Persistent Item being documented i.e. E70 Thing, E72 Legal Object, E39 Actor, E41 Appellation, E51 Contact Point and E55 Type"""@en ;
                                     rdfs:label "a fait exister"@fr ,
                                                "brachte in Existenz"@de ,
                                                "brought into existence"@en ,
                                                "trouxe à existência"@pt ,
                                                "γέννησε"@el ,
                                                "создал"@ru ,
                                                "开始了"@zh .


###  http://www.cidoc-crm.org/cidoc-crm/P92i_was_brought_into_existence_by
cidoc-crm:P92i_was_brought_into_existence_by rdf:type owl:ObjectProperty ;
                                             rdfs:subPropertyOf cidoc-crm:P12i_was_present_at ;
                                             rdfs:domain cidoc-crm:E77_Persistent_Item ;
                                             rdfs:range cidoc-crm:E63_Beginning_of_Existence ;
                                             rdfs:label "a commencé à exister du fait de"@fr ,
                                                        "passou a existir por"@pt ,
                                                        "was brought into existence by"@en ,
                                                        "wurde in Existenz gebracht durch"@de ,
                                                        "γεννήθηκε από"@el ,
                                                        "был создан посредством"@ru ,
                                                        "被开始於"@zh .


###  http://www.cidoc-crm.org/cidoc-crm/P94_has_created
cidoc-crm:P94_has_created rdf:type owl:ObjectProperty ;
                          rdfs:subPropertyOf cidoc-crm:P92_brought_into_existence ;
                          rdfs:domain cidoc-crm:E65_Creation ;
                          rdfs:range cidoc-crm:E28_Conceptual_Object ;
                          rdfs:comment """This property allows a conceptual E65 Creation to be linked to the E28 Conceptual Object created by it.
It represents the act of conceiving the intellectual content of the E28 Conceptual Object. It does not represent the act of creating the first physical carrier of the E28 Conceptual Object. As an example, this is the composition of a poem, not its commitment to paper.
"""@en ;
                          rdfs:label "a créé"@fr ,
                                     "criou"@pt ,
                                     "has created"@en ,
                                     "hat erschaffen"@de ,
                                     "δημιούργησε"@el ,
                                     "создал"@ru ,
                                     "创造了"@zh .


###  http://www.cidoc-crm.org/cidoc-crm/P94i_was_created_by
cidoc-crm:P94i_was_created_by rdf:type owl:ObjectProperty ;
                              rdfs:subPropertyOf cidoc-crm:P92i_was_brought_into_existence_by ;
                              rdfs:domain cidoc-crm:E28_Conceptual_Object ;
                              rdfs:range cidoc-crm:E65_Creation ;
                              rdfs:label "a été créé par"@fr ,
                                         "foi criado por"@pt ,
                                         "was created by"@en ,
                                         "wurde erschaffen durch"@de ,
                                         "δημιουργήθηκε από"@el ,
                                         "был создан посредством"@ru ,
                                         "被创造於"@zh .


###  http://www.europeana.eu/schemas/edm/aggregatedCHO
edm:aggregatedCHO rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf dc:subject ,
                                     cidoc-crm:P129_is_about ,
                                     ore:aggregates ;
                  rdfs:domain ore:Aggregation ;
                  rdfs:range edm:ProvidedCHO ;
                  rdfs:label "Aggregated Cultural Heritage Object"@en ;
                  skos:definition "This property associates an ORE aggregation with the cultural heritage object(s) (CHO for short) it is about."@en .


###  http://www.openarchives.org/ore/terms/aggregates
ore:aggregates rdf:type owl:ObjectProperty ;
               owl:inverseOf ore:isAggregatedBy ;
               rdfs:domain ore:Aggregation ;
               rdfs:comment "Aggregations, by definition, aggregate resources. The ore:aggregates relationship expresses that the object resource is a member of the set of Aggregated Resources of the subject (the Aggregation). This relationship between the Aggregation and its Aggregated Resources is thus more specific than a simple part/whole relationship, as expressed by dcterms:hasPart for example." ;
               rdfs:isDefinedBy ore: ;
               rdfs:label "Aggregates" .


###  http://www.openarchives.org/ore/terms/describes
ore:describes rdf:type owl:ObjectProperty ;
              owl:inverseOf ore:isDescribedBy ;
              rdfs:domain ore:ResourceMap ;
              rdfs:range ore:Aggregation ;
              rdfs:comment "This relationship asserts that the subject (a Resource Map) describes the object (an Aggregation)." ;
              rdfs:isDefinedBy ore: ;
              rdfs:label "Describes" .


###  http://www.openarchives.org/ore/terms/isAggregatedBy
ore:isAggregatedBy rdf:type owl:ObjectProperty ;
                   rdfs:subPropertyOf dcterms:isPartOf ;
                   rdfs:domain ore:AggregatedResource ;
                   rdfs:range ore:Aggregation ;
                   rdfs:comment "The inverse relationship of ore:aggregates, ore:isAggregatedBy asserts that an Aggregated Resource is aggregated by an Aggregation." ;
                   rdfs:isDefinedBy ore: ;
                   rdfs:label "Is Aggregated By" .


###  http://www.openarchives.org/ore/terms/isDescribedBy
ore:isDescribedBy rdf:type owl:ObjectProperty ;
                  rdfs:domain ore:Aggregation ;
                  rdfs:range ore:ResourceMap ;
                  rdfs:comment "The inverse relationship of ore:describes, in this case the object of the relationship is the Resource Map and the subject is the Aggregation which it describes." ;
                  rdfs:isDefinedBy ore: ;
                  rdfs:label "Is Described By" .


