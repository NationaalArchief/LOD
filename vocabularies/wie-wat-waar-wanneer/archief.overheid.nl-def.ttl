#    Classes
#################################################################

###  http://archief.overheid.nl/def/ArchiefEntiteit
nadef:ArchiefEntiteit rdf:type owl:Class ;
owl:equivalentClass edm:NonInformationResource ;
rdfs:subClassOf edm:ProvidedCHO ;
owl:disjointWith nadef:ArchiefHulpbron ;
rdfs:comment "Een archief entiteit staat voor entiteiten waar naar verwezen wordt in de verzamelingen  archief records."@nl .


###  http://archief.overheid.nl/def/ArchiefHulpbron
nadef:ArchiefHulpbron rdf:type owl:Class ;
owl:equivalentClass dbo:Reference ,
dcterms:BibliographicResource ,
cidoc-crm:E73_Information_Object ,
edm:InformationResource ;
rdfs:label "ArchiefHulpbron"@nl .


###  http://archief.overheid.nl/def/Foto
nadef:Foto rdf:type owl:Class .


###  http://archief.overheid.nl/def/FotoCreatie
nadef:FotoCreatie rdf:type owl:Class .


###  http://archief.overheid.nl/def/GrafischRecord
nadef:GrafischRecord rdf:type owl:Class ;
      rdfs:subClassOf nadef:ArchiefHulpbron ,
          ore:Aggregation ;
      rdfs:comment "Een record van een grafisch object. Bevat beschrijving en de verwijzing naar het fysieke object en de eventuele zichtexemplaren."@nl ;
      rdfs:label "Grafisch record"@nl ;
      skos:prefLabel "Grafisch record"@nl .


###  http://archief.overheid.nl/def/HistorischeNaam
nadef:HistorischeNaam rdf:type owl:Class ;
owl:disjointWith bgn:Person ;
rdfs:comment "Een naam van een persoon uit de geschiedenis, niet de persoon zelf."@nl ;
rdfs:label "Naam van een persoon uit de geschiedenis"@nl ;
skos:prefLabel "Historische naam"@nl .


###  http://archief.overheid.nl/def/PersoonEntiteit
nadef:PersoonEntiteit rdf:type owl:Class ;
owl:equivalentClass bgn:Person ,
cidoc-crm:E39_Actor ,
foaf:Person ;
rdfs:subClassOf nadef:ArchiefEntiteit ;
owl:disjointWith nadef:PersoonsBeschrijving ;
rdfs:comment "Fysiek persoon in het domein van archieven."@nl .


###  http://archief.overheid.nl/def/PersoonsBeschrijving
nadef:PersoonsBeschrijving rdf:type owl:Class ;
rdfs:subClassOf nadef:ArchiefHulpbron ,
 nadef:PersoonsNaam ;
rdfs:comment "Beschrijving van een persoon in de context van archieven. Gebruik dit om originele data over personen te borgen uit de records van archieven."@nl ;
rdfs:label "Persoonsbeschrijving"@nl .


###  http://archief.overheid.nl/def/PersoonsNaam
nadef:PersoonsNaam rdf:type owl:Class ;
    rdfs:subClassOf nadef:HistorischeNaam ;
    owl:disjointWith bgn:Person ;
    rdfs:comment "Een naam van een persoon, niet de persoon zelf."@nl ;
    rdfs:label "Persoonsnaam"@nl ;
    skos:prefLabel "Persoonsnaam"@nl .


###  http://biographynet.nl/schema/Person
bgn:Person rdf:type owl:Class ;
           owl:equivalentClass foaf:Person ;
           rdfs:comment "In all National Archives' Linked Data sets referring to Persons, nadef:PersoonEntiteit is used because of the distinction between a Reference and a real-world Person"@en .


