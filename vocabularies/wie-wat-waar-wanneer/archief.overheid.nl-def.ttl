   rdfs:label "patronym after last name"@en ;
   skos:label "patroniem na achternaam"@nl ;
   skos:prefLabel "patroniem na achternaam"@nl .


###  http://archief.overheid.nl/def/patroniem_voor_naam
nadef:patroniem_voor_naam rdf:type owl:DatatypeProperty ;
