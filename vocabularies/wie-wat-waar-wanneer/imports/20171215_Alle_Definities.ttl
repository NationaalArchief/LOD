# === Entities en References ====

# === An ArchivalEntity represents a Resource in the domain of non-information objects, like its EDM counterpart   ===
# === An ArchivalReference represents an ArchivalEntity as mentioned in an archival document ===

def:ArchivalEntity a owl:Class ;
    rdfs:label "Entity in an Archives domain"^^xsd:string ;
    owl:equivalentClass edm:NonInformationResource ;
    rdfs:comment "An Archival entity represents entities referred to across the boundaries of archival records"^^xsd:string ;
    owl:disjointWith def:ArchivalReference ;
.
def:ArchivalReference a owl:Class ;
    rdfs:label "Reference in an Archives domain"^^xsd:string ;
    owl:equivalentClass edm:InformationResource , dbo:Reference , dct:BibliographicResource ;
    rdfs:comment "An Archival Reference is the document-bound stand-in for archival entities referred to in archival records. May also be a dataset"^^xsd:string ;
    owl:disjointWith def:ArchivalEntity ;
.
def:PersonEntity a owl:Class  ;
    rdfs:label "Person in an Archives domain"^^xsd:string ;
    rdfs:subClassOf def:ArchivalEntity ;
    owl:equivalentClass foaf:Person , bgn:Person ;
    rdfs:comment "A Person Entity is a Person referred to across a set of archival records"^^xsd:string ;
.
def:CorporateEntity a owl:Class ;
    rdfs:label "Corporation in an Archives domain"^^xsd:string ;
    rdfs:subClassOf def:ArchivalEntity ;
    owl:equivalentClass foaf:Agent ;
    rdfs:comment "A Corporate Entity is a corporation referred to across a set of archival records"^^xsd:string ;
.
def:FilmEntity a owl:Class ;
    rdfs:label "Entity representing a Film in an Archives domain"^^xsd:string ;
    rdfs:subClassOf def:ArchivalEntity ;
    owl:equivalentClass <http://www.movieontology.org/2009/11/09/Movie> , dbo:Film ;
    rdfs:comment "A Film Entity is within the archival domain an information resource describing a movie independently of its references"^^xsd:string ;
.
def:ArchivalEvent a owl:Class ;
    rdfs:label "Event in an Archives domain"^^xsd:string ;
    owl:equivalentClass edm:Event , sem:Event ;
    rdfs:subClassOf def:ArchivalEntity ;
    rdfs:comment "An Archival Event is an Event referred to across a set of archival records, e.g., a war going on somewhere"^^xsd:string ;
.
def:PersonReference a owl:Class ;
    rdfs:label "Reference to a Person in an Archives domain"^^xsd:string ;
    rdfs:subClassOf def:ArchivalReference ;
    owl:disjointWith def:PersonEntity ;
    rdfs:comment "A Person Reference is the reference to a Person in a single archival record"^^xsd:string ;
.
def:CommissionReference a owl:Class ;
    rdfs:label "Reference to a Commission in an Archives domain"^^xsd:string ;
    rdfs:subClassOf def:ArchivalReference ;
    rdfs:comment "A Commission Reference is the reference to a Commission in a single archival record"^^xsd:string ;
.
def:FilmReference a owl:Class ;
    rdfs:label "Reference to a Film in an Archives domain"^^xsd:string ;
    rdfs:subClassOf def:ArchivalReference ;
    rdfs:comment "A Film Reference is the reference to a FilmEntity in a single archival record"^^xsd:string ;
.
def:PoliticianReference a owl:Class ;
    rdfs:label "Reference to a Politician in an Archives domain"^^xsd:string ;
    rdfs:subClassOf def:ArchivalReference ;
    rdfs:comment "A Person Reference is the reference to a Politician in a single archival record"^^xsd:string ;
.
def:ParticipatingPartyReference a owl:Class ;
    rdfs:label "Reference to a Party (Person or Organisation)"^^xsd:string ;
    rdfs:subClassOf def:ArchivalReference ;
    rdfs:comment "An Participating Party is the stand-in in a single archival record for Parties involved in bringing about movies"^^xsd:string ;
.
def:PrisonRecordsEntry a owl:Class ;
    rdfs:label "Document referring to a Person in an Entry of a Prison Record, still in the Archives domain"^^xsd:string ;
    rdfs:subClassOf dct:BibliographicResource ;
    rdfs:comment "An Prison Records Entry is the document referring to an inmate of Scheveningen prison during WW II, as being referenced in the Books of the Dead of Schevingen prison, the so-called 'Oranjehotel'."^^xsd:string ;
.
def:WarSourcesEntry a owl:Class ;
    rdfs:label "A single reference to a person, place or event involved in the Second World War"^^xsd:string ;
    rdfs:subClassOf dct:BibliographicResource ;
    rdfs:comment "A War Sources Entry is the reference to a person, place or event within the War Sources aggregator"^^xsd:string ;
.
def:WebMemorialEntry a owl:Class ;
    rdfs:label "Web document referring to a Person who is to be remembered"^^xsd:string ;
    rdfs:subClassOf dct:BibliographicResource ;
    rdfs:comment "A Web Memoral Entry is the reference to a person or an event to be remembered in the context the Web Memorial is dedicated to"^^xsd:string ;
.


# == Classes for Persons according to the BiographyNet model (partly still the same as BioDes) ==

bgnExt:Gender rdf:type owl:Class ;
  rdfs:label "Gender"^^xsd:string ;
  owl:equivalentClass bgn:Sex ;
  skos:prefLabel "Geslacht"@nl ;
.
bgnExt:Relation rdf:type owl:Class ;
  rdfs:label "Relation"^^xsd:string ;
  skos:prefLabel "Relatie"@nl ;
.
bgnExt:State rdf:type owl:Class ;
  rdfs:label "State"^^xsd:string ;
  skos:prefLabel "Toestand"@nl ;
.
bgnExt:ArchivalName rdf:type owl:Class ;
  rdfs:label "Archival name"^^xsd:string ;
  owl:disjointWith bgnExt:ArchivalEntity ;
  owl:equivalentClass dbo:Name ;
  rdfs:comment "A name in the archival domain"^^xsd:string ;
  skos:prefLabel "Naam"@nl ;
.
bgnExt:CorporateName rdf:type owl:Class ;
  rdfs:subClassOf bgnExt:ArchivalName ;
  rdfs:label "Corporate name"^^xsd:string ;
  owl:disjointWith bgnExt:CorporateEntity ;
  owl:equivalentClass dbo:Name ;
  skos:prefLabel "Bedrijfsnaam"@nl ;
.
bgnExt:PersonName rdf:type owl:Class ;
  rdfs:subClassOf bgnExt:ArchivalName ;
  rdfs:label "Person name"^^xsd:string ;
  owl:disjointWith bgnExt:PersonEntity , def:PersonReference ;
  owl:equivalentClass dbo:Name , bgn:Name ;
  skos:prefLabel "Persoonsnaam"@nl ;
.

