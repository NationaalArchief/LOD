      rdfs:subClassOf def:AdditionalFindingAid ;
      owl:disjointWith def:DetailedContentDescription ;
      def:modelsConceptOf def:GuideToArchivesTypeAccess ;
.
def:ResearchersAid rdf:type owl:Class ;
      rdfs:label "Researchers' aid"^^xsd:string ;
      skos:prefLabel "Onderzoeksgids"@nl ;
      skos:definition "Nader in te vullen"@nl ;
