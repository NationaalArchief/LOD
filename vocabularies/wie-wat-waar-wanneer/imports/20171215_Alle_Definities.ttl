      rdfs:label "Nadere Toegang"^^xsd:string ;
      skos:prefLabel "Nadere Toegang"@nl ;
      skos:definition "Een Nadere Toegang bevat verdiepende beschrijving (ofwel, een omschrijving van de stukken op een niveau dieper dan de beschrijving van het inventarisnummer). Nadere Toegangen zijn meer op de inhoud van de stukken gericht."@nl ;
      rdfs:subClassOf def:AdditionalFindingAid ;
      owl:disjointWith def:ArchivalInventory ;
      def:modelsConceptOf def:DetailedInventoryTypeAccess ;
