#################################################################

###  http://archief.overheid.nl/def/achternaam
nadef:achternaam rdf:type owl:DatatypeProperty ;
rdfs:range xsd:string ;
rdfs:domain nadef:HistorischeNaam ;
  rdfs:comment "In order to deal with names and titles being part of prepostion, intraposition or postposition, for each of these properties specialisations have been defined for either name or title"^^xsd:string ;
  rdfs:label "last name"@en ;
  rdfs:label "achternaam"@nl ;
  skos:altLabel "geslachtsnaam"@nl ;
  skos:prefLabel "achternaam"@nl .


###  http://archief.overheid.nl/def/achtervoegsel
nadef:achtervoegsel rdf:type owl:DatatypeProperty ;
                    rdfs:domain nadef:HistorischeNaam ;
                    rdfs:range xsd:string ;
                    rdfs:label "postposition"@en .

